[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setListeners = function _setListeners() {[m
[32m+[m[32m        var _this3 = this;[m
[32m+[m
[32m+[m[32m        var triggers = this.config.trigger.split(' ');[m
[32m+[m[32m        triggers.forEach(function (trigger) {[m
[32m+[m[32m          if (trigger === 'click') {[m
[32m+[m[32m            $$$1(_this3.element).on(_this3.constructor.Event.CLICK, _this3.config.selector, function (event) {[m
[32m+[m[32m              return _this3.toggle(event);[m
[32m+[m[32m            });[m
[32m+[m[32m          } else if (trigger !== Trigger.MANUAL) {[m
[32m+[m[32m            var eventIn = trigger === Trigger.HOVER ? _this3.constructor.Event.MOUSEENTER : _this3.constructor.Event.FOCUSIN;[m
[32m+[m[32m            var eventOut = trigger === Trigger.HOVER ? _this3.constructor.Event.MOUSELEAVE : _this3.constructor.Event.FOCUSOUT;[m
[32m+[m[32m            $$$1(_this3.element).on(eventIn, _this3.config.selector, function (event) {[m
[32m+[m[32m              return _this3._enter(event);[m
[32m+[m[32m            }).on(eventOut, _this3.config.selector, function (event) {[m
[32m+[m[32m              return _this3._leave(event);[m
[32m+[m[32m            });[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(_this3.element).closest('.modal').on('hide.bs.modal', function () {[m
[32m+[m[32m            return _this3.hide();[m
[32m+[m[32m          });[m
[32m+[m[32m        });[m
