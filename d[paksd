[32m+[m[32m          } else {[m
[32m+[m[32m            complete();[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.hide = function hide(callback) {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        var tip = this.getTipElement();[m
[32m+[m[32m        var hideEvent = $$$1.Event(this.constructor.Event.HIDE);[m
[32m+[m
[32m+[m[32m        var complete = function complete() {[m
