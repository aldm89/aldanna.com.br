[32m+[m[32m  /**[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   * Bootstrap (v4.1.3): index.js[m
[32m+[m[32m   * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  (function ($$$1) {[m
[32m+[m[32m    if (typeof $$$1 === 'undefined') {[m
[32m+[m[32m      throw new TypeError('Bootstrap\'s JavaScript requires jQuery. jQuery must be included before Bootstrap\'s JavaScript.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var version = $$$1.fn.jquery.split(' ')[0].split('.');[m
[32m+[m[32m    var minMajor = 1;[m
[32m+[m[32m    var ltMajor = 2;[m
[32m+[m[32m    var minMinor = 9;[m
[32m+[m[32m    var minPatch = 1;[m
[32m+[m[32m    var maxMajor = 4;[m
[32m+[m
[32m+[m[32m    if (version[0] < ltMajor && version[1] < minMinor || version[0] === minMajor && version[1] === minMinor && version[2] < minPatch || version[0] >= maxMajor) {[m
[32m+[m[32m      throw new Error('Bootstrap\'s JavaScript requires at least jQuery v1.9.1 but less than v4.0.0');[m
[32m+[m[32m    }[m
[32m+[m[32m  })($);[m
[32m+[m
[32m+[m[32m  exports.Util = Util;[m
[32m+[m[32m  exports.Alert = Alert;[m
[32m+[m[32m  exports.Button = Button;[m
[32m+[m[32m  exports.Carousel = Carousel;[m
[32m+[m[32m  exports.Collapse = Collapse;[m
[32m+[m[32m  exports.Dropdown = Dropdown;[m
[32m+[m[32m  exports.Modal = Modal;[m
[32m+[m[32m  exports.Popover = Popover;[m
[32m+[m[32m  exports.Scrollspy = ScrollSpy;[m
[32m+[m[32m  exports.Tab = Tab;[m
[32m+[m[32m  exports.Tooltip = Tooltip;[m
[32m+[m
[32m+[m[32m  Object.defineProperty(exports, '__esModule', { value: true });[m
[32m+[m
[32m+[m[32m})));[m
