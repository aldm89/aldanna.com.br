[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return ScrollSpy;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
