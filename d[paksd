[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this.element).trigger(this.constructor.Event.INSERTED);[m
[32m+[m[32m          this._popper = new Popper(this.element, tip, {[m
[32m+[m[32m            placement: attachment,[m
[32m+[m[32m            modifiers: {[m
[32m+[m[32m              offset: {[m
[32m+[m[32m                offset: this.config.offset[m
[32m+[m[32m              },[m
[32m+[m[32m              flip: {[m
[32m+[m[32m                behavior: this.config.fallbackPlacement[m
[32m+[m[32m              },[m
[32m+[m[32m              arrow: {[m
[32m+[m[32m                element: Selector.ARROW[m
[32m+[m[32m              },[m
[32m+[m[32m              preventOverflow: {[m
[32m+[m[32m                boundariesElement: this.config.boundary[m
[32m+[m[32m              }[m
[32m+[m[32m            },[m
[32m+[m[32m            onCreate: function onCreate(data) {[m
[32m+[m[32m              if (data.originalPlacement !== data.placement) {[m
[32m+[m[32m                _this._handlePopperPlacementChange(data);[m
[32m+[m[32m              }[m
[32m+[m[32m            },[m
[32m+[m[32m            onUpdate: function onUpdate(data) {[m
[32m+[m[32m              _this._handlePopperPlacementChange(data);[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          $$$1(tip).addClass(ClassName.SHOW); // If this is a touch-enabled device we add extra[m
[32m+[m[32m          // empty mouseover listeners to the body's immediate children;[m
[32m+[m[32m          // only needed because of broken event delegation on iOS[m
[32m+[m[32m          // https://www.quirksmode.org/blog/archives/2014/02/mouse_event_bub.html[m
[32m+[m
[32m+[m[32m          if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m            $$$1(document.body).children().on('mouseover', null, $$$1.noop);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var complete = function complete() {[m
[32m+[m[32m            if (_this.config.animation) {[m
[32m+[m[32m              _this._fixTransition();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            var prevHoverState = _this._hoverState;[m
[32m+[m[32m            _this._hoverState = null;[m
[32m+[m[32m            $$$1(_this.element).trigger(_this.constructor.Event.SHOWN);[m
[32m+[m
[32m+[m[32m            if (prevHoverState === HoverState.OUT) {[m
