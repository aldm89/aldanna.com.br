[32m+[m[32m        if (this.config.selector) {[m
[32m+[m[32m          this.config = _objectSpread({}, this.config, {[m
[32m+[m[32m            trigger: 'manual',[m
[32m+[m[32m            selector: ''[m
[32m+[m[32m          });[m
[32m+[m[32m        } else {[m
[32m+[m[32m          this._fixTitle();[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._fixTitle = function _fixTitle() {[m
[32m+[m[32m        var titleType = typeof this.element.getAttribute('data-original-title');[m
[32m+[m
[32m+[m[32m        if (this.element.getAttribute('title') || titleType !== 'string') {[m
[32m+[m[32m          this.element.setAttribute('data-original-title', this.element.getAttribute('title') || '');[m
[32m+[m[32m          this.element.setAttribute('title', '');[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._enter = function _enter(event, context) {[m
[32m+[m[32m        var dataKey = this.constructor.DATA_KEY;[m
[32m+[m[32m        context = context || $$$1(event.currentTarget).data(dataKey);[m
[32m+[m
[32m+[m[32m        if (!context) {[m
[32m+[m[32m          context = new this.constructor(event.currentTarget, this._getDelegateConfig());[m
[32m+[m[32m          $$$1(event.currentTarget).data(dataKey, context);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event) {[m
[32m+[m[32m          context._activeTrigger[event.type === 'focusin' ? Trigger.FOCUS : Trigger.HOVER] = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if ($$$1(context.getTipElement()).hasClass(ClassName.SHOW) || context._hoverState === HoverState.SHOW) {[m
[32m+[m[32m          context._hoverState = HoverState.SHOW;[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        clearTimeout(context._timeout);[m
[32m+[m[32m        context._hoverState = HoverState.SHOW;[m
[32m+[m
[32m+[m[32m        if (!context.config.delay || !context.config.delay.show) {[m
[32m+[m[32m          context.show();[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        context._timeout = setTimeout(function () {[m
[32m+[m[32m          if (context._hoverState === HoverState.SHOW) {[m
[32m+[m[32m            context.show();[m
[32m+[m[32m          }[m
[32m+[m[32m        }, context.config.delay.show);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._leave = function _leave(event, context) {[m
[32m+[m[32m        var dataKey = this.constructor.DATA_KEY;[m
[32m+[m[32m        context = context || $$$1(event.currentTarget).data(dataKey);[m
[32m+[m
[32m+[m[32m        if (!context) {[m
[32m+[m[32m          context = new this.constructor(event.currentTarget, this._getDelegateConfig());[m
[32m+[m[32m          $$$1(event.currentTarget).data(dataKey, context);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event) {[m
[32m+[m[32m          context._activeTrigger[event.type === 'focusout' ? Trigger.FOCUS : Trigger.HOVER] = false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (context._isWithActiveTrigger()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        clearTimeout(context._timeout);[m
[32m+[m[32m        context._hoverState = HoverState.OUT;[m
[32m+[m
[32m+[m[32m        if (!context.config.delay || !context.config.delay.hide) {[m
[32m+[m[32m          context.hide();[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        context._timeout = setTimeout(function () {[m
[32m+[m[32m          if (context._hoverState === HoverState.OUT) {[m
[32m+[m[32m            context.hide();[m
[32m+[m[32m          }[m
[32m+[m[32m        }, context.config.delay.hide);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._isWithActiveTrigger = function _isWithActiveTrigger() {[m
[32m+[m[32m        for (var trigger in this._activeTrigger) {[m
[32m+[m[32m          if (this._activeTrigger[trigger]) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return false;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, this.constructor.Default, $$$1(this.element).data(), typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m        if (typeof config.delay === 'number') {[m
[32m+[m[32m          config.delay = {[m
[32m+[m[32m            show: config.delay,[m
[32m+[m[32m            hide: config.delay[m
[32m+[m[32m          };[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (typeof config.title === 'number') {[m
[32m+[m[32m          config.title = config.title.toString();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (typeof config.content === 'number') {[m
[32m+[m[32m          config.content = config.content.toString();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, this.constructor.DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getDelegateConfig = function _getDelegateConfig() {[m
[32m+[m[32m        var config = {};[m
[32m+[m
[32m+[m[32m        if (this.config) {[m
[32m+[m[32m          for (var key in this.config) {[m
[32m+[m[32m            if (this.constructor.Default[key] !== this.config[key]) {[m
[32m+[m[32m              config[key] = this.config[key];[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._cleanTipClass = function _cleanTipClass() {[m
[32m+[m[32m        var $tip = $$$1(this.getTipElement());[m
[32m+[m[32m        var tabClass = $tip.attr('class').match(BSCLS_PREFIX_REGEX);[m
[32m+[m
[32m+[m[32m        if (tabClass !== null && tabClass.length) {[m
[32m+[m[32m          $tip.removeClass(tabClass.join(''));[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._handlePopperPlacementChange = function _handlePopperPlacementChange(popperData) {[m
[32m+[m[32m        var popperInstance = popperData.instance;[m
[32m+[m[32m        this.tip = popperInstance.popper;[m
[32m+[m
[32m+[m[32m        this._cleanTipClass();[m
[32m+[m
[32m+[m[32m        this.addAttachmentClass(this._getAttachment(popperData.placement));[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._fixTransition = function _fixTransition() {[m
[32m+[m[32m        var tip = this.getTipElement();[m
[32m+[m[32m        var initConfigAnimation = this.config.animation;[m
[32m+[m
[32m+[m[32m        if (tip.getAttribute('x-placement') !== null) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $$$1(tip).removeClass(ClassName.FADE);[m
[32m+[m[32m        this.config.animation = false;[m
[32m+[m[32m        this.hide();[m
[32m+[m[32m        this.show();[m
[32m+[m[32m        this.config.animation = initConfigAnimation;[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Tooltip._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = typeof config === 'object' && config;[m
[32m+[m
[32m+[m[32m          if (!data && /dispose|hide/.test(config)) {[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Tooltip(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config]();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Tooltip, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
