[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.show = function show() {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        if (this._isTransitioning || $$$1(this._element).hasClass(ClassName.SHOW)) {[m
