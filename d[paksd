[32m+[m[32m  /**[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   * Bootstrap (v4.1.3): tooltip.js[m
[32m+[m[32m   * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var Tooltip = function ($$$1) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Constants[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m[32m    var NAME = 'tooltip';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.tooltip';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var CLASS_PREFIX = 'bs-tooltip';[m
[32m+[m[32m    var BSCLS_PREFIX_REGEX = new RegExp("(^|\\s)" + CLASS_PREFIX + "\\S+", 'g');[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      animation: 'boolean',[m
[32m+[m[32m      template: 'string',[m
[32m+[m[32m      title: '(string|element|function)',[m
[32m+[m[32m      trigger: 'string',[m
[32m+[m[32m      delay: '(number|object)',[m
[32m+[m[32m      html: 'boolean',[m
[32m+[m[32m      selector: '(string|boolean)',[m
[32m+[m[32m      placement: '(string|function)',[m
[32m+[m[32m      offset: '(number|string)',[m
[32m+[m[32m      container: '(string|element|boolean)',[m
[32m+[m[32m      fallbackPlacement: '(string|array)',[m
[32m+[m[32m      boundary: '(string|element)'[m
[32m+[m[32m    };[m
[32m+[m[32m    var AttachmentMap = {[m
[32m+[m[32m      AUTO: 'auto',[m
[32m+[m[32m      TOP: 'top',[m
[32m+[m[32m      RIGHT: 'right',[m
[32m+[m[32m      BOTTOM: 'bottom',[m
[32m+[m[32m      LEFT: 'left'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      animation: true,[m
[32m+[m[32m      template: '<div class="tooltip" role="tooltip">' + '<div class="arrow"></div>' + '<div class="tooltip-inner"></div></div>',[m
[32m+[m[32m      trigger: 'hover focus',[m
[32m+[m[32m      title: '',[m
[32m+[m[32m      delay: 0,[m
[32m+[m[32m      html: false,[m
[32m+[m[32m      selector: false,[m
[32m+[m[32m      placement: 'top',[m
[32m+[m[32m      offset: 0,[m
[32m+[m[32m      container: false,[m
[32m+[m[32m      fallbackPlacement: 'flip',[m
[32m+[m[32m      boundary: 'scrollParent'[m
[32m+[m[32m    };[m
[32m+[m[32m    var HoverState = {[m
[32m+[m[32m      SHOW: 'show',[m
[32m+[m[32m      OUT: 'out'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      HIDE: "hide" + EVENT_KEY,[m
[32m+[m[32m      HIDDEN: "hidden" + EVENT_KEY,[m
