[32m+[m
[32m+[m[32m            if (callback) {[m
[32m+[m[32m              callback();[m
[32m+[m[32m            }[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if ($$$1(this._element).hasClass(ClassName.FADE)) {[m
[32m+[m[32m            var _backdropTransitionDuration = Util.getTransitionDurationFromElement(this._backdrop);[m
[32m+[m
[32m+[m[32m            $$$1(this._backdrop).one(Util.TRANSITION_END, callbackRemove).emulateTransitionEnd(_backdropTransitionDuration);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            callbackRemove();[m
[32m+[m[32m          }[m
[32m+[m[32m        } else if (callback) {[m
[32m+[m[32m          callback();[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // ----------------------------------------------------------------------[m
[32m+[m[32m      // the following methods are used to handle overflowing modals[m
[32m+[m[32m      // todo (fat): these should probably be refactored out of modal.js[m
[32m+[m[32m      // ----------------------------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._adjustDialog = function _adjustDialog() {[m
[32m+[m[32m        var isModalOverflowing = this._element.scrollHeight > document.documentElement.clientHeight;[m
[32m+[m
[32m+[m[32m        if (!this._isBodyOverflowing && isModalOverflowing) {[m
[32m+[m[32m          this._element.style.paddingLeft = this._scrollbarWidth + "px";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._isBodyOverflowing && !isModalOverflowing) {[m
[32m+[m[32m          this._element.style.paddingRight = this._scrollbarWidth + "px";[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._resetAdjustments = function _resetAdjustments() {[m
[32m+[m[32m        this._element.style.paddingLeft = '';[m
[32m+[m[32m        this._element.style.paddingRight = '';[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._checkScrollbar = function _checkScrollbar() {[m
[32m+[m[32m        var rect = document.body.getBoundingClientRect();[m
[32m+[m[32m        this._isBodyOverflowing = rect.left + rect.right < window.innerWidth;[m
[32m+[m[32m        this._scrollbarWidth = this._getScrollbarWidth();[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setScrollbar = function _setScrollbar() {[m
[32m+[m[32m        var _this9 = this;[m
[32m+[m
[32m+[m[32m        if (this._isBodyOverflowing) {[m
[32m+[m[32m          // Note: DOMNode.style.paddingRight returns the actual value or '' if not set[m
[32m+[m[32m          //   while $(DOMNode).css('padding-right') returns the calculated value or 0 if not set[m
[32m+[m[32m          var fixedContent = [].slice.call(document.querySelectorAll(Selector.FIXED_CONTENT));[m
[32m+[m[32m          var stickyContent = [].slice.call(document.querySelectorAll(Selector.STICKY_CONTENT)); // Adjust fixed content padding[m
[32m+[m
[32m+[m[32m          $$$1(fixedContent).each(function (index, element) {[m
[32m+[m[32m            var actualPadding = element.style.paddingRight;[m
[32m+[m[32m            var calculatedPadding = $$$1(element).css('padding-right');[m
[32m+[m[32m            $$$1(element).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + _this9._scrollbarWidth + "px");[m
[32m+[m[32m          }); // Adjust sticky content margin[m
[32m+[m
[32m+[m[32m          $$$1(stickyContent).each(function (index, element) {[m
[32m+[m[32m            var actualMargin = element.style.marginRight;[m
[32m+[m[32m            var calculatedMargin = $$$1(element).css('margin-right');[m
[32m+[m[32m            $$$1(element).data('margin-right', actualMargin).css('margin-right', parseFloat(calculatedMargin) - _this9._scrollbarWidth + "px");[m
[32m+[m[32m          }); // Adjust body padding[m
[32m+[m
[32m+[m[32m          var actualPadding = document.body.style.paddingRight;[m
[32m+[m[32m          var calculatedPadding = $$$1(document.body).css('padding-right');[m
[32m+[m[32m          $$$1(document.body).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + this._scrollbarWidth + "px");[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._resetScrollbar = function _resetScrollbar() {[m
[32m+[m[32m        // Restore fixed content padding[m
[32m+[m[32m        var fixedContent = [].slice.call(document.querySelectorAll(Selector.FIXED_CONTENT));[m
[32m+[m[32m        $$$1(fixedContent).each(function (index, element) {[m
[32m+[m[32m          var padding = $$$1(element).data('padding-right');[m
[32m+[m[32m          $$$1(element).removeData('padding-right');[m
[32m+[m[32m          element.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m        }); // Restore sticky content[m
[32m+[m
[32m+[m[32m        var elements = [].slice.call(document.querySelectorAll("" + Selector.STICKY_CONTENT));[m
[32m+[m[32m        $$$1(elements).each(function (index, element) {[m
[32m+[m[32m          var margin = $$$1(element).data('margin-right');[m
[32m+[m
[32m+[m[32m          if (typeof margin !== 'undefined') {[m
[32m+[m[32m            $$$1(element).css('margin-right', margin).removeData('margin-right');[m
[32m+[m[32m          }[m
[32m+[m[32m        }); // Restore body padding[m
[32m+[m
[32m+[m[32m        var padding = $$$1(document.body).data('padding-right');[m
[32m+[m[32m        $$$1(document.body).removeData('padding-right');[m
[32m+[m[32m        document.body.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getScrollbarWidth = function _getScrollbarWidth() {[m
[32m+[m[32m        // thx d.walsh[m
[32m+[m[32m        var scrollDiv = document.createElement('div');[m
[32m+[m[32m        scrollDiv.className = ClassName.SCROLLBAR_MEASURER;[m
[32m+[m[32m        document.body.appendChild(scrollDiv);[m
[32m+[m[32m        var scrollbarWidth = scrollDiv.getBoundingClientRect().width - scrollDiv.clientWidth;[m
[32m+[m[32m        document.body.removeChild(scrollDiv);[m
[32m+[m[32m        return scrollbarWidth;[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Modal._jQueryInterface = function _jQueryInterface(config, relatedTarget) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = _objectSpread({}, Default, $$$1(this).data(), typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Modal(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config](relatedTarget);[m
[32m+[m[32m          } else if (_config.show) {[m
[32m+[m[32m            data.show(relatedTarget);[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Modal, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return Modal;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Data Api implementation[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m
[32m+[m[32m    $$$1(document).on(Event.CLICK_DATA_API, Selector.DATA_TOGGLE, function (event) {[m
[32m+[m[32m      var _this10 = this;[m
[32m+[m
[32m+[m[32m      var target;[m
[32m+[m[32m      var selector = Util.getSelectorFromElement(this);[m
[32m+[m
[32m+[m[32m      if (selector) {[m
[32m+[m[32m        target = document.querySelector(selector);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var config = $$$1(target).data(DATA_KEY) ? 'toggle' : _objectSpread({}, $$$1(target).data(), $$$1(this).data());[m
[32m+[m
[32m+[m[32m      if (this.tagName === 'A' || this.tagName === 'AREA') {[m
[32m+[m[32m        event.preventDefault();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $target = $$$1(target).one(Event.SHOW, function (showEvent) {[m
[32m+[m[32m        if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m          // Only register focus restorer if modal will actually get shown[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $target.one(Event.HIDDEN, function () {[m
[32m+[m[32m          if ($$$1(_this10).is(':visible')) {[m
[32m+[m[32m            _this10.focus();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      Modal._jQueryInterface.call($$$1(target), config, this);[m
[32m+[m[32m    });[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * jQuery[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME] = Modal._jQueryInterface;[m
[32m+[m[32m    $$$1.fn[NAME].Constructor = Modal;[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME].noConflict = function () {[m
[32m+[m[32m      $$$1.fn[NAME] = JQUERY_NO_CONFLICT;[m
[32m+[m[32m      return Modal._jQueryInterface;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Modal;[m
[32m+[m[32m  }($);[m
