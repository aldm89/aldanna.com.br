[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.setElementContent = function setElementContent($element, content) {[m
[32m+[m[32m        var html = this.config.html;[m
[32m+[m
[32m+[m[32m        if (typeof content === 'object' && (content.nodeType || content.jquery)) {[m
[32m+[m[32m          // Content is a DOM node or a jQuery[m
[32m+[m[32m          if (html) {[m
[32m+[m[32m            if (!$$$1(content).parent().is($element)) {[m
[32m+[m[32m              $element.empty().append(content);[m
[32m+[m[32m            }[m
[32m+[m[32m          } else {[m
[32m+[m[32m            $element.text($$$1(content).text());[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $element[html ? 'html' : 'text'](content);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.getTitle = function getTitle() {[m
[32m+[m[32m        var title = this.element.getAttribute('data-original-title');[m
[32m+[m
[32m+[m[32m        if (!title) {[m
[32m+[m[32m          title = typeof this.config.title === 'function' ? this.config.title.call(this.element) : this.config.title;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return title;[m
[32m+[m[32m      }; // Private[m
