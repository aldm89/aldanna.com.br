[32m+[m[32m            }[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if ($$$1(this.tip).hasClass(ClassName.FADE)) {[m
[32m+[m[32m            var transitionDuration = Util.getTransitionDurationFromElement(this.tip);[m
[32m+[m[32m            $$$1(this.tip).one(Util.TRANSITION_END, complete).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            complete();[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.hide = function hide(callback) {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        var tip = this.getTipElement();[m
