[32m+[m
[32m+[m[32m  var ScrollSpy = function ($$$1) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Constants[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m[32m    var NAME = 'scrollspy';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.scrollspy';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var DATA_API_KEY = '.data-api';[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      offset: 10,[m
[32m+[m[32m      method: 'auto',[m
[32m+[m[32m      target: ''[m
[32m+[m[32m    };[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      offset: 'number',[m
[32m+[m[32m      method: 'string',[m
[32m+[m[32m      target: '(string|element)'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      ACTIVATE: "activate" + EVENT_KEY,[m
[32m+[m[32m      SCROLL: "scroll" + EVENT_KEY,[m
[32m+[m[32m      LOAD_DATA_API: "load" + EVENT_KEY + DATA_API_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      DROPDOWN_ITEM: 'dropdown-item',[m
[32m+[m[32m      DROPDOWN_MENU: 'dropdown-menu',[m
[32m+[m[32m      ACTIVE: 'active'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      DATA_SPY: '[data-spy="scroll"]',[m
[32m+[m[32m      ACTIVE: '.active',[m
[32m+[m[32m      NAV_LIST_GROUP: '.nav, .list-group',[m
[32m+[m[32m      NAV_LINKS: '.nav-link',[m
[32m+[m[32m      NAV_ITEMS: '.nav-item',[m
[32m+[m[32m      LIST_ITEMS: '.list-group-item',[m
[32m+[m[32m      DROPDOWN: '.dropdown',[m
[32m+[m[32m      DROPDOWN_ITEMS: '.dropdown-item',[m
[32m+[m[32m      DROPDOWN_TOGGLE: '.dropdown-toggle'[m
[32m+[m[32m    };[m
[32m+[m[32m    var OffsetMethod = {[m
[32m+[m[32m      OFFSET: 'offset',[m
[32m+[m[32m      POSITION: 'position'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var ScrollSpy =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function ScrollSpy(element, config) {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        this._element = element;[m
[32m+[m[32m        this._scrollElement = element.tagName === 'BODY' ? window : element;[m
[32m+[m[32m        this._config = this._getConfig(config);[m
[32m+[m[32m        this._selector = this._config.target + " " + Selector.NAV_LINKS + "," + (this._config.target + " " + Selector.LIST_ITEMS + ",") + (this._config.target + " " + Selector.DROPDOWN_ITEMS);[m
[32m+[m[32m        this._offsets = [];[m
[32m+[m[32m        this._targets = [];[m
[32m+[m[32m        this._activeTarget = null;[m
[32m+[m[32m        this._scrollHeight = 0;[m
[32m+[m[32m        $$$1(this._scrollElement).on(Event.SCROLL, function (event) {[m
[32m+[m[32m          return _this._process(event);[m
[32m+[m[32m        });[m
[32m+[m[32m        this.refresh();[m
[32m+[m
[32m+[m[32m        this._process();[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = ScrollSpy.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.refresh = function refresh() {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        var autoMethod = this._scrollElement === this._scrollElement.window ? OffsetMethod.OFFSET : OffsetMethod.POSITION;[m
[32m+[m[32m        var offsetMethod = this._config.method === 'auto' ? autoMethod : this._config.method;[m
[32m+[m[32m        var offsetBase = offsetMethod === OffsetMethod.POSITION ? this._getScrollTop() : 0;[m
[32m+[m[32m        this._offsets = [];[m
[32m+[m[32m        this._targets = [];[m
[32m+[m[32m        this._scrollHeight = this._getScrollHeight();[m
[32m+[m[32m        var targets = [].slice.call(document.querySelectorAll(this._selector));[m
[32m+[m[32m        targets.map(function (element) {[m
[32m+[m[32m          var target;[m
[32m+[m[32m          var targetSelector = Util.getSelectorFromElement(element);[m
[32m+[m
[32m+[m[32m          if (targetSelector) {[m
[32m+[m[32m            target = document.querySelector(targetSelector);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (target) {[m
[32m+[m[32m            var targetBCR = target.getBoundingClientRect();[m
[32m+[m
[32m+[m[32m            if (targetBCR.width || targetBCR.height) {[m
[32m+[m[32m              // TODO (fat): remove sketch reliance on jQuery position/offset[m
[32m+[m[32m              return [$$$1(target)[offsetMethod]().top + offsetBase, targetSelector];[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          return null;[m
[32m+[m[32m        }).filter(function (item) {[m
[32m+[m[32m          return item;[m
[32m+[m[32m        }).sort(function (a, b) {[m
[32m+[m[32m          return a[0] - b[0];[m
[32m+[m[32m        }).forEach(function (item) {[m
[32m+[m[32m          _this2._offsets.push(item[0]);[m
[32m+[m
[32m+[m[32m          _this2._targets.push(item[1]);[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        $$$1.removeData(this._element, DATA_KEY);[m
[32m+[m[32m        $$$1(this._scrollElement).off(EVENT_KEY);[m
[32m+[m[32m        this._element = null;[m
[32m+[m[32m        this._scrollElement = null;[m
[32m+[m[32m        this._config = null;[m
[32m+[m[32m        this._selector = null;[m
[32m+[m[32m        this._offsets = null;[m
[32m+[m[32m        this._targets = null;[m
[32m+[m[32m        this._activeTarget = null;[m
[32m+[m[32m        this._scrollHeight = null;[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, Default, typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m        if (typeof config.target !== 'string') {[m
[32m+[m[32m          var id = $$$1(config.target).attr('id');[m
[32m+[m
[32m+[m[32m          if (!id) {[m
[32m+[m[32m            id = Util.getUID(NAME);[m
[32m+[m[32m            $$$1(config.target).attr('id', id);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          config.target = "#" + id;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getScrollTop = function _getScrollTop() {[m
[32m+[m[32m        return this._scrollElement === window ? this._scrollElement.pageYOffset : this._scrollElement.scrollTop;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getScrollHeight = function _getScrollHeight() {[m
[32m+[m[32m        return this._scrollElement.scrollHeight || Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getOffsetHeight = function _getOffsetHeight() {[m
[32m+[m[32m        return this._scrollElement === window ? window.innerHeight : this._scrollElement.getBoundingClientRect().height;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._process = function _process() {[m
[32m+[m[32m        var scrollTop = this._getScrollTop() + this._config.offset;[m
[32m+[m
[32m+[m[32m        var scrollHeight = this._getScrollHeight();[m
[32m+[m
[32m+[m[32m        var maxScroll = this._config.offset + scrollHeight - this._getOffsetHeight();[m
[32m+[m
[32m+[m[32m        if (this._scrollHeight !== scrollHeight) {[m
[32m+[m[32m          this.refresh();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (scrollTop >= maxScroll) {[m
[32m+[m[32m          var target = this._targets[this._targets.length - 1];[m
[32m+[m
[32m+[m[32m          if (this._activeTarget !== target) {[m
[32m+[m[32m            this._activate(target);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._activeTarget && scrollTop < this._offsets[0] && this._offsets[0] > 0) {[m
[32m+[m[32m          this._activeTarget = null;[m
[32m+[m
[32m+[m[32m          this._clear();[m
[32m+[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var offsetLength = this._offsets.length;[m
[32m+[m
[32m+[m[32m        for (var i = offsetLength; i--;) {[m
[32m+[m[32m          var isActiveTarget = this._activeTarget !== this._targets[i] && scrollTop >= this._offsets[i] && (typeof this._offsets[i + 1] === 'undefined' || scrollTop < this._offsets[i + 1]);[m
[32m+[m
[32m+[m[32m          if (isActiveTarget) {[m
[32m+[m[32m            this._activate(this._targets[i]);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._activate = function _activate(target) {[m
[32m+[m[32m        this._activeTarget = target;[m
[32m+[m
[32m+[m[32m        this._clear();[m
[32m+[m
[32m+[m[32m        var queries = this._selector.split(','); // eslint-disable-next-line arrow-body-style[m
[32m+[m
[32m+[m
[32m+[m[32m        queries = queries.map(function (selector) {[m
[32m+[m[32m          return selector + "[data-target=\"" + target + "\"]," + (selector + "[href=\"" + target + "\"]");[m
[32m+[m[32m        });[m
[32m+[m[32m        var $link = $$$1([].slice.call(document.querySelectorAll(queries.join(','))));[m
[32m+[m
[32m+[m[32m        if ($link.hasClass(ClassName.DROPDOWN_ITEM)) {[m
[32m+[m[32m          $link.closest(Selector.DROPDOWN).find(Selector.DROPDOWN_TOGGLE).addClass(ClassName.ACTIVE);[m
[32m+[m[32m          $link.addClass(ClassName.ACTIVE);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          // Set triggered link as active[m
[32m+[m[32m          $link.addClass(ClassName.ACTIVE); // Set triggered links parents as active[m
[32m+[m[32m          // With both <ul> and <nav> markup a parent is the previous sibling of any nav ancestor[m
[32m+[m
[32m+[m[32m          $link.parents(Selector.NAV_LIST_GROUP).prev(Selector.NAV_LINKS + ", " + Selector.LIST_ITEMS).addClass(ClassName.ACTIVE); // Handle special case when .nav-link is inside .nav-item[m
[32m+[m
[32m+[m[32m          $link.parents(Selector.NAV_LIST_GROUP).prev(Selector.NAV_ITEMS).children(Selector.NAV_LINKS).addClass(ClassName.ACTIVE);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $$$1(this._scrollElement).trigger(Event.ACTIVATE, {[m
[32m+[m[32m          relatedTarget: target[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._clear = function _clear() {[m
[32m+[m[32m        var nodes = [].slice.call(document.querySelectorAll(this._selector));[m
[32m+[m[32m        $$$1(nodes).filter(Selector.ACTIVE).removeClass(ClassName.ACTIVE);[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      ScrollSpy._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = typeof config === 'object' && config;[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new ScrollSpy(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config]();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(ScrollSpy, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return ScrollSpy;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Data Api implementation[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m
[32m+[m[32m    $$$1(window).on(Event.LOAD_DATA_API, function () {[m
[32m+[m[32m      var scrollSpys = [].slice.call(document.querySelectorAll(Selector.DATA_SPY));[m
[32m+[m[32m      var scrollSpysLength = scrollSpys.length;[m
[32m+[m
[32m+[m[32m      for (var i = scrollSpysLength; i--;) {[m
[32m+[m[32m        var $spy = $$$1(scrollSpys[i]);[m
[32m+[m
[32m+[m[32m        ScrollSpy._jQueryInterface.call($spy, $spy.data());[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * jQuery[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME] = ScrollSpy._jQueryInterface;[m
[32m+[m[32m    $$$1.fn[NAME].Constructor = ScrollSpy;[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME].noConflict = function () {[m
[32m+[m[32m      $$$1.fn[NAME] = JQUERY_NO_CONFLICT;[m
[32m+[m[32m      return ScrollSpy._jQueryInterface;[m
[32m+[m[32m    };[m
[32m+[m
