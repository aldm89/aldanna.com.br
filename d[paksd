[32m+[m
[32m+[m[32m      _proto._getParent = function _getParent() {[m
[32m+[m[32m        var _this3 = this;[m
[32m+[m
[32m+[m[32m        var parent = null;[m
[32m+[m
[32m+[m[32m        if (Util.isElement(this._config.parent)) {[m
[32m+[m[32m          parent = this._config.parent; // It's a jQuery object[m
