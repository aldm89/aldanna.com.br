[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        $$$1(this.element).trigger(hideEvent);[m
[32m+[m
[32m+[m[32m        if (hideEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $$$1(tip).removeClass(ClassName.SHOW); // If this is a touch-enabled device we remove the extra[m
[32m+[m[32m        // empty mouseover listeners we added for iOS support[m
[32m+[m
[32m+[m[32m        if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m          $$$1(document.body).children().off('mouseover', null, $$$1.noop);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._activeTrigger[Trigger.CLICK] = false;[m
[32m+[m[32m        this._activeTrigger[Trigger.FOCUS] = false;[m
[32m+[m[32m        this._activeTrigger[Trigger.HOVER] = false;[m
[32m+[m
[32m+[m[32m        if ($$$1(this.tip).hasClass(ClassName.FADE)) {[m
[32m+[m[32m          var transitionDuration = Util.getTransitionDurationFromElement(tip);[m
[32m+[m[32m          $$$1(tip).one(Util.TRANSITION_END, complete).emulateTransitionEnd(transitionDuration);[m
