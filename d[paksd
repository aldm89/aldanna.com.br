[32m+[m[32m    var NAME = 'modal';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.modal';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var DATA_API_KEY = '.data-api';[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var ESCAPE_KEYCODE = 27; // KeyboardEvent.which value for Escape (Esc) key[m
[32m+[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      backdrop: true,[m
[32m+[m[32m      keyboard: true,[m
[32m+[m[32m      focus: true,[m
[32m+[m[32m      show: true[m
[32m+[m[32m    };[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      backdrop: '(boolean|string)',[m
[32m+[m[32m      keyboard: 'boolean',[m
[32m+[m[32m      focus: 'boolean',[m
[32m+[m[32m      show: 'boolean'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      HIDE: "hide" + EVENT_KEY,[m
[32m+[m[32m      HIDDEN: "hidden" + EVENT_KEY,[m
[32m+[m[32m      SHOW: "show" + EVENT_KEY,[m
[32m+[m[32m      SHOWN: "shown" + EVENT_KEY,[m
[32m+[m[32m      FOCUSIN: "focusin" + EVENT_KEY,[m
[32m+[m[32m      RESIZE: "resize" + EVENT_KEY,[m
[32m+[m[32m      CLICK_DISMISS: "click.dismiss" + EVENT_KEY,[m
[32m+[m[32m      KEYDOWN_DISMISS: "keydown.dismiss" + EVENT_KEY,[m
[32m+[m[32m      MOUSEUP_DISMISS: "mouseup.dismiss" + EVENT_KEY,[m
[32m+[m[32m      MOUSEDOWN_DISMISS: "mousedown.dismiss" + EVENT_KEY,[m
[32m+[m[32m      CLICK_DATA_API: "click" + EVENT_KEY + DATA_API_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      SCROLLBAR_MEASURER: 'modal-scrollbar-measure',[m
[32m+[m[32m      BACKDROP: 'modal-backdrop',[m
[32m+[m[32m      OPEN: 'modal-open',[m
[32m+[m[32m      FADE: 'fade',[m
[32m+[m[32m      SHOW: 'show'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      DIALOG: '.modal-dialog',[m
[32m+[m[32m      DATA_TOGGLE: '[data-toggle="modal"]',[m
[32m+[m[32m      DATA_DISMISS: '[data-dismiss="modal"]',[m
[32m+[m[32m      FIXED_CONTENT: '.fixed-top, .fixed-bottom, .is-fixed, .sticky-top',[m
[32m+[m[32m      STICKY_CONTENT: '.sticky-top'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var Modal =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function Modal(element, config) {[m
[32m+[m[32m        this._config = this._getConfig(config);[m
[32m+[m[32m        this._element = element;[m
[32m+[m[32m        this._dialog = element.querySelector(Selector.DIALOG);[m
[32m+[m[32m        this._backdrop = null;[m
[32m+[m[32m        this._isShown = false;[m
[32m+[m[32m        this._isBodyOverflowing = false;[m
[32m+[m[32m        this._ignoreBackdropClick = false;[m
[32m+[m[32m        this._scrollbarWidth = 0;[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = Modal.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.toggle = function toggle(relatedTarget) {[m
[32m+[m[32m        return this._isShown ? this.hide() : this.show(relatedTarget);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.show = function show(relatedTarget) {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        if (this._isTransitioning || this._isShown) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if ($$$1(this._element).hasClass(ClassName.FADE)) {[m
[32m+[m[32m          this._isTransitioning = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var showEvent = $$$1.Event(Event.SHOW, {[m
[32m+[m[32m          relatedTarget: relatedTarget[m
[32m+[m[32m        });[m
[32m+[m[32m        $$$1(this._element).trigger(showEvent);[m
[32m+[m
[32m+[m[32m        if (this._isShown || showEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._isShown = true;[m
[32m+[m
[32m+[m[32m        this._checkScrollbar();[m
[32m+[m
[32m+[m[32m        this._setScrollbar();[m
[32m+[m
[32m+[m[32m        this._adjustDialog();[m
[32m+[m
[32m+[m[32m        $$$1(document.body).addClass(ClassName.OPEN);[m
[32m+[m
[32m+[m[32m        this._setEscapeEvent();[m
[32m+[m
[32m+[m[32m        this._setResizeEvent();[m
[32m+[m
[32m+[m[32m        $$$1(this._element).on(Event.CLICK_DISMISS, Selector.DATA_DISMISS, function (event) {[m
[32m+[m[32m          return _this.hide(event);[m
[32m+[m[32m        });[m
[32m+[m[32m        $$$1(this._dialog).on(Event.MOUSEDOWN_DISMISS, function () {[m
[32m+[m[32m          $$$1(_this._element).one(Event.MOUSEUP_DISMISS, function (event) {[m
[32m+[m[32m            if ($$$1(event.target).is(_this._element)) {[m
[32m+[m[32m              _this._ignoreBackdropClick = true;[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        this._showBackdrop(function () {[m
[32m+[m[32m          return _this._showElement(relatedTarget);[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.hide = function hide(event) {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        if (event) {[m
[32m+[m[32m          event.preventDefault();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._isTransitioning || !this._isShown) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var hideEvent = $$$1.Event(Event.HIDE);[m
[32m+[m[32m        $$$1(this._element).trigger(hideEvent);[m
[32m+[m
[32m+[m[32m        if (!this._isShown || hideEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._isShown = false;[m
[32m+[m[32m        var transition = $$$1(this._element).hasClass(ClassName.FADE);[m
[32m+[m
[32m+[m[32m        if (transition) {[m
[32m+[m[32m          this._isTransitioning = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._setEscapeEvent();[m
[32m+[m
[32m+[m[32m        this._setResizeEvent();[m
[32m+[m
[32m+[m[32m        $$$1(document).off(Event.FOCUSIN);[m
[32m+[m[32m        $$$1(this._element).removeClass(ClassName.SHOW);[m
[32m+[m[32m        $$$1(this._element).off(Event.CLICK_DISMISS);[m
[32m+[m[32m        $$$1(this._dialog).off(Event.MOUSEDOWN_DISMISS);[m
[32m+[m
[32m+[m[32m        if (transition) {[m
[32m+[m[32m          var transitionDuration = Util.getTransitionDurationFromElement(this._element);[m
[32m+[m[32m          $$$1(this._element).one(Util.TRANSITION_END, function (event) {[m
[32m+[m[32m            return _this2._hideModal(event);[m
[32m+[m[32m          }).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          this._hideModal();[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        $$$1.removeData(this._element, DATA_KEY);[m
[32m+[m[32m        $$$1(window, document, this._element, this._backdrop).off(EVENT_KEY);[m
[32m+[m[32m        this._config = null;[m
[32m+[m[32m        this._element = null;[m
[32m+[m[32m        this._dialog = null;[m
[32m+[m[32m        this._backdrop = null;[m
[32m+[m[32m        this._isShown = null;[m
[32m+[m[32m        this._isBodyOverflowing = null;[m
[32m+[m[32m        this._ignoreBackdropClick = null;[m
[32m+[m[32m        this._scrollbarWidth = null;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.handleUpdate = function handleUpdate() {[m
[32m+[m[32m        this._adjustDialog();[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, Default, config);[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._showElement = function _showElement(relatedTarget) {[m
[32m+[m[32m        var _this3 = this;[m
[32m+[m
[32m+[m[32m        var transition = $$$1(this._element).hasClass(ClassName.FADE);[m
[32m+[m
[32m+[m[32m        if (!this._element.parentNode || this._element.parentNode.nodeType !== Node.ELEMENT_NODE) {[m
[32m+[m[32m          // Don't move modal's DOM position[m
[32m+[m[32m          document.body.appendChild(this._element);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._element.style.display = 'block';[m
[32m+[m
[32m+[m[32m        this._element.removeAttribute('aria-hidden');[m
[32m+[m
[32m+[m[32m        this._element.scrollTop = 0;[m
[32m+[m
[32m+[m[32m        if (transition) {[m
[32m+[m[32m          Util.reflow(this._element);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $$$1(this._element).addClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m        if (this._config.focus) {[m
[32m+[m[32m          this._enforceFocus();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var shownEvent = $$$1.Event(Event.SHOWN, {[m
[32m+[m[32m          relatedTarget: relatedTarget[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        var transitionComplete = function transitionComplete() {[m
[32m+[m[32m          if (_this3._config.focus) {[m
[32m+[m[32m            _this3._element.focus();[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          _this3._isTransitioning = false;[m
[32m+[m[32m          $$$1(_this3._element).trigger(shownEvent);[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (transition) {[m
[32m+[m[32m          var transitionDuration = Util.getTransitionDurationFromElement(this._element);[m
[32m+[m[32m          $$$1(this._dialog).one(Util.TRANSITION_END, transitionComplete).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          transitionComplete();[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._enforceFocus = function _enforceFocus() {[m
[32m+[m[32m        var _this4 = this;[m
[32m+[m
[32m+[m[32m        $$$1(document).off(Event.FOCUSIN) // Guard against infinite focus loop[m
[32m+[m[32m        .on(Event.FOCUSIN, function (event) {[m
[32m+[m[32m          if (document !== event.target && _this4._element !== event.target && $$$1(_this4._element).has(event.target).length === 0) {[m
[32m+[m[32m            _this4._element.focus();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setEscapeEvent = function _setEscapeEvent() {[m
[32m+[m[32m        var _this5 = this;[m
[32m+[m
[32m+[m[32m        if (this._isShown && this._config.keyboard) {[m
[32m+[m[32m          $$$1(this._element).on(Event.KEYDOWN_DISMISS, function (event) {[m
[32m+[m[32m            if (event.which === ESCAPE_KEYCODE) {[m
[32m+[m[32m              event.preventDefault();[m
[32m+[m
[32m+[m[32m              _this5.hide();[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m        } else if (!this._isShown) {[m
[32m+[m[32m          $$$1(this._element).off(Event.KEYDOWN_DISMISS);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setResizeEvent = function _setResizeEvent() {[m
[32m+[m[32m        var _this6 = this;[m
[32m+[m
[32m+[m[32m        if (this._isShown) {[m
[32m+[m[32m          $$$1(window).on(Event.RESIZE, function (event) {[m
[32m+[m[32m            return _this6.handleUpdate(event);[m
[32m+[m[32m          });[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $$$1(window).off(Event.RESIZE);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._hideModal = function _hideModal() {[m
[32m+[m[32m        var _this7 = this;[m
[32m+[m
[32m+[m[32m        this._element.style.display = 'none';[m
[32m+[m
[32m+[m[32m        this._element.setAttribute('aria-hidden', true);[m
[32m+[m
[32m+[m[32m        this._isTransitioning = false;[m
[32m+[m
[32m+[m[32m        this._showBackdrop(function () {[m
[32m+[m[32m          $$$1(document.body).removeClass(ClassName.OPEN);[m
[32m+[m
[32m+[m[32m          _this7._resetAdjustments();[m
[32m+[m
[32m+[m[32m          _this7._resetScrollbar();[m
[32m+[m
[32m+[m[32m          $$$1(_this7._element).trigger(Event.HIDDEN);[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._removeBackdrop = function _removeBackdrop() {[m
[32m+[m[32m        if (this._backdrop) {[m
[32m+[m[32m          $$$1(this._backdrop).remove();[m
[32m+[m[32m          this._backdrop = null;[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._showBackdrop = function _showBackdrop(callback) {[m
[32m+[m[32m        var _this8 = this;[m
[32m+[m
[32m+[m[32m        var animate = $$$1(this._element).hasClass(ClassName.FADE) ? ClassName.FADE : '';[m
[32m+[m
[32m+[m[32m        if (this._isShown && this._config.backdrop) {[m
[32m+[m[32m          this._backdrop = document.createElement('div');[m
[32m+[m[32m          this._backdrop.className = ClassName.BACKDROP;[m
[32m+[m
[32m+[m[32m          if (animate) {[m
[32m+[m[32m            this._backdrop.classList.add(animate);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this._backdrop).appendTo(document.body);[m
[32m+[m[32m          $$$1(this._element).on(Event.CLICK_DISMISS, function (event) {[m
[32m+[m[32m            if (_this8._ignoreBackdropClick) {[m
[32m+[m[32m              _this8._ignoreBackdropClick = false;[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (event.target !== event.currentTarget) {[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (_this8._config.backdrop === 'static') {[m
[32m+[m[32m              _this8._element.focus();[m
[32m+[m[32m            } else {[m
[32m+[m[32m              _this8.hide();[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m
[32m+[m[32m          if (animate) {[m
[32m+[m[32m            Util.reflow(this._backdrop);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this._backdrop).addClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m          if (!callback) {[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (!animate) {[m
[32m+[m[32m            callback();[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var backdropTransitionDuration = Util.getTransitionDurationFromElement(this._backdrop);[m
[32m+[m[32m          $$$1(this._backdrop).one(Util.TRANSITION_END, callback).emulateTransitionEnd(backdropTransitionDuration);[m
[32m+[m[32m        } else if (!this._isShown && this._backdrop) {[m
[32m+[m[32m          $$$1(this._backdrop).removeClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m          var callbackRemove = function callbackRemove() {[m
[32m+[m[32m            _this8._removeBackdrop();[m
[32m+[m
[32m+[m[32m            if (callback) {[m
[32m+[m[32m              callback();[m
[32m+[m[32m            }[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if ($$$1(this._element).hasClass(ClassName.FADE)) {[m
[32m+[m[32m            var _backdropTransitionDuration = Util.getTransitionDurationFromElement(this._backdrop);[m
[32m+[m
[32m+[m[32m            $$$1(this._backdrop).one(Util.TRANSITION_END, callbackRemove).emulateTransitionEnd(_backdropTransitionDuration);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            callbackRemove();[m
[32m+[m[32m          }[m
[32m+[m[32m        } else if (callback) {[m
[32m+[m[32m          callback();[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // ----------------------------------------------------------------------[m
[32m+[m[32m      // the following methods are used to handle overflowing modals[m
[32m+[m[32m      // todo (fat): these should probably be refactored out of modal.js[m
[32m+[m[32m      // ----------------------------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._adjustDialog = function _adjustDialog() {[m
[32m+[m[32m        var isModalOverflowing = this._element.scrollHeight > document.documentElement.clientHeight;[m
[32m+[m
[32m+[m[32m        if (!this._isBodyOverflowing && isModalOverflowing) {[m
[32m+[m[32m          this._element.style.paddingLeft = this._scrollbarWidth + "px";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._isBodyOverflowing && !isModalOverflowing) {[m
[32m+[m[32m          this._element.style.paddingRight = this._scrollbarWidth + "px";[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._resetAdjustments = function _resetAdjustments() {[m
[32m+[m[32m        this._element.style.paddingLeft = '';[m
[32m+[m[32m        this._element.style.paddingRight = '';[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._checkScrollbar = function _checkScrollbar() {[m
[32m+[m[32m        var rect = document.body.getBoundingClientRect();[m
[32m+[m[32m        this._isBodyOverflowing = rect.left + rect.right < window.innerWidth;[m
[32m+[m[32m        this._scrollbarWidth = this._getScrollbarWidth();[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setScrollbar = function _setScrollbar() {[m
[32m+[m[32m        var _this9 = this;[m
[32m+[m
[32m+[m[32m        if (this._isBodyOverflowing) {[m
[32m+[m[32m          // Note: DOMNode.style.paddingRight returns the actual value or '' if not set[m
[32m+[m[32m          //   while $(DOMNode).css('padding-right') returns the calculated value or 0 if not set[m
[32m+[m[32m          var fixedContent = [].slice.call(document.querySelectorAll(Selector.FIXED_CONTENT));[m
[32m+[m[32m          var stickyContent = [].slice.call(document.querySelectorAll(Selector.STICKY_CONTENT)); // Adjust fixed content padding[m
[32m+[m
[32m+[m[32m          $$$1(fixedContent).each(function (index, element) {[m
[32m+[m[32m            var actualPadding = element.style.paddingRight;[m
[32m+[m[32m            var calculatedPadding = $$$1(element).css('padding-right');[m
[32m+[m[32m            $$$1(element).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + _this9._scrollbarWidth + "px");[m
[32m+[m[32m          }); // Adjust sticky content margin[m
[32m+[m
[32m+[m[32m          $$$1(stickyContent).each(function (index, element) {[m
[32m+[m[32m            var actualMargin = element.style.marginRight;[m
[32m+[m[32m            var calculatedMargin = $$$1(element).css('margin-right');[m
[32m+[m[32m            $$$1(element).data('margin-right', actualMargin).css('margin-right', parseFloat(calculatedMargin) - _this9._scrollbarWidth + "px");[m
[32m+[m[32m          }); // Adjust body padding[m
[32m+[m
[32m+[m[32m          var actualPadding = document.body.style.paddingRight;[m
[32m+[m[32m          var calculatedPadding = $$$1(document.body).css('padding-right');[m
[32m+[m[32m          $$$1(document.body).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + this._scrollbarWidth + "px");[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._resetScrollbar = function _resetScrollbar() {[m
[32m+[m[32m        // Restore fixed content padding[m
[32m+[m[32m        var fixedContent = [].slice.call(document.querySelectorAll(Selector.FIXED_CONTENT));[m
[32m+[m[32m        $$$1(fixedContent).each(function (index, element) {[m
[32m+[m[32m          var padding = $$$1(element).data('padding-right');[m
[32m+[m[32m          $$$1(element).removeData('padding-right');[m
[32m+[m[32m          element.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m        }); // Restore sticky content[m
[32m+[m
[32m+[m[32m        var elements = [].slice.call(document.querySelectorAll("" + Selector.STICKY_CONTENT));[m
[32m+[m[32m        $$$1(elements).each(function (index, element) {[m
[32m+[m[32m          var margin = $$$1(element).data('margin-right');[m
[32m+[m
[32m+[m[32m          if (typeof margin !== 'undefined') {[m
[32m+[m[32m            $$$1(element).css('margin-right', margin).removeData('margin-right');[m
[32m+[m[32m          }[m
[32m+[m[32m        }); // Restore body padding[m
[32m+[m
[32m+[m[32m        var padding = $$$1(document.body).data('padding-right');[m
[32m+[m[32m        $$$1(document.body).removeData('padding-right');[m
[32m+[m[32m        document.body.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getScrollbarWidth = function _getScrollbarWidth() {[m
[32m+[m[32m        // thx d.walsh[m
[32m+[m[32m        var scrollDiv = document.createElement('div');[m
[32m+[m[32m        scrollDiv.className = ClassName.SCROLLBAR_MEASURER;[m
[32m+[m[32m        document.body.appendChild(scrollDiv);[m
[32m+[m[32m        var scrollbarWidth = scrollDiv.getBoundingClientRect().width - scrollDiv.clientWidth;[m
[32m+[m[32m        document.body.removeChild(scrollDiv);[m
[32m+[m[32m        return scrollbarWidth;[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Modal._jQueryInterface = function _jQueryInterface(config, relatedTarget) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = _objectSpread({}, Default, $$$1(this).data(), typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Modal(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config](relatedTarget);[m
[32m+[m[32m          } else if (_config.show) {[m
[32m+[m[32m            data.show(relatedTarget);[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Modal, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return Modal;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Data Api implementation[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m
[32m+[m[32m    $$$1(document).on(Event.CLICK_DATA_API, Selector.DATA_TOGGLE, function (event) {[m
[32m+[m[32m      var _this10 = this;[m
[32m+[m
[32m+[m[32m      var target;[m
[32m+[m[32m      var selector = Util.getSelectorFromElement(this);[m
[32m+[m
[32m+[m[32m      if (selector) {[m
[32m+[m[32m        target = document.querySelector(selector);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var config = $$$1(target).data(DATA_KEY) ? 'toggle' : _objectSpread({}, $$$1(target).data(), $$$1(this).data());[m
[32m+[m
[32m+[m[32m      if (this.tagName === 'A' || this.tagName === 'AREA') {[m
[32m+[m[32m        event.preventDefault();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $target = $$$1(target).one(Event.SHOW, function (showEvent) {[m
[32m+[m[32m        if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m          // Only register focus restorer if modal will actually get shown[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $target.one(Event.HIDDEN, function () {[m
[32m+[m[32m          if ($$$1(_this10).is(':visible')) {[m
[32m+[m[32m            _this10.focus();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      Modal._jQueryInterface.call($$$1(target), config, this);[m
[32m+[m[32m    });[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * jQuery[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME] = Modal._jQueryInterface;[m
[32m+[m[32m    $$$1.fn[NAME].Constructor = Modal;[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME].noConflict = function () {[m
[32m+[m[32m      $$$1.fn[NAME] = JQUERY_NO_CONFLICT;[m
[32m+[m[32m      return Modal._jQueryInterface;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Modal;[m
[32m+[m[32m  }($);[m
