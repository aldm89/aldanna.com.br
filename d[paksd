[32m+[m[32m            parent = this._config.parent[0];[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          parent = document.querySelector(this._config.parent);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var selector = "[data-toggle=\"collapse\"][data-parent=\"" + this._config.parent + "\"]";[m
[32m+[m[32m        var children = [].slice.call(parent.querySelectorAll(selector));[m
[32m+[m[32m        $$$1(children).each(function (i, element) {[m
[32m+[m[32m          _this3._addAriaAndCollapsedClass(Collapse._getTargetFromElement(element), [element]);[m
[32m+[m[32m        });[m
[32m+[m[32m        return parent;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._addAriaAndCollapsedClass = function _addAriaAndCollapsedClass(element, triggerArray) {[m
[32m+[m[32m        if (element) {[m
[32m+[m[32m          var isOpen = $$$1(element).hasClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m          if (triggerArray.length) {[m
[32m+[m[32m            $$$1(triggerArray).toggleClass(ClassName.COLLAPSED, !isOpen).attr('aria-expanded', isOpen);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Collapse._getTargetFromElement = function _getTargetFromElement(element) {[m
[32m+[m[32m        var selector = Util.getSelectorFromElement(element);[m
[32m+[m[32m        return selector ? document.querySelector(selector) : null;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Collapse._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var $this = $$$1(this);[m
[32m+[m[32m          var data = $this.data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = _objectSpread({}, Default, $this.data(), typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m          if (!data && _config.toggle && /show|hide/.test(config)) {[m
[32m+[m[32m            _config.toggle = false;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Collapse(this, _config);[m
[32m+[m[32m            $this.data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config]();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Collapse, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return Collapse;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Data Api implementation[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m
[32m+[m[32m    $$$1(document).on(Event.CLICK_DATA_API, Selector.DATA_TOGGLE, function (event) {[m
[32m+[m[32m      // preventDefault only for <a> elements (which change the URL) not inside the collapsible element[m
[32m+[m[32m      if (event.currentTarget.tagName === 'A') {[m
[32m+[m[32m        event.preventDefault();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $trigger = $$$1(this);[m
[32m+[m[32m      var selector = Util.getSelectorFromElement(this);[m
[32m+[m[32m      var selectors = [].slice.call(document.querySelectorAll(selector));[m
[32m+[m[32m      $$$1(selectors).each(function () {[m
[32m+[m[32m        var $target = $$$1(this);[m
[32m+[m[32m        var data = $target.data(DATA_KEY);[m
[32m+[m[32m        var config = data ? 'toggle' : $trigger.data();[m
[32m+[m
[32m+[m[32m        Collapse._jQueryInterface.call($target, config);[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * jQuery[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME] = Collapse._jQueryInterface;[m
[32m+[m[32m    $$$1.fn[NAME].Constructor = Collapse;[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME].noConflict = function () {[m
[32m+[m[32m      $$$1.fn[NAME] = JQUERY_NO_CONFLICT;[m
[32m+[m[32m      return Collapse._jQueryInterface;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Collapse;[m
[32m+[m[32m  }($);[m
[32m+[m
[32m+[m[32m  /**![m
[32m+[m[32m   * @fileOverview Kickass library to create and place poppers near their reference elements.[m
[32m+[m[32m   * @version 1.14.3[m
[32m+[m[32m   * @license[m
[32m+[m[32m   * Copyright (c) 2016 Federico Zivolo and contributors[m
[32m+[m[32m   *[m
[32m+[m[32m   * Permission is hereby granted, free of charge, to any person obtaining a copy[m
[32m+[m[32m   * of this software and associated documentation files (the "Software"), to deal[m
[32m+[m[32m   * in the Software without restriction, including without limitation the rights[m
[32m+[m[32m   * to use, copy, modify, merge, publish, distribute, sublicense, and/or sell[m
[32m+[m[32m   * copies of the Software, and to permit persons to whom the Software is[m
[32m+[m[32m   * furnished to do so, subject to the following conditions:[m
[32m+[m[32m   *[m
[32m+[m[32m   * The above copyright notice and this permission notice shall be included in all[m
[32m+[m[32m   * copies or substantial portions of the Software.[m
[32m+[m[32m   *[m
[32m+[m[32m   * THE SOFTWARE IS PROVIDED "AS IS", WITHOUT WARRANTY OF ANY KIND, EXPRESS OR[m
[32m+[m[32m   * IMPLIED, INCLUDING BUT NOT LIMITED TO THE WARRANTIES OF MERCHANTABILITY,[m
[32m+[m[32m   * FITNESS FOR A PARTICULAR PURPOSE AND NONINFRINGEMENT. IN NO EVENT SHALL THE[m
[32m+[m[32m   * AUTHORS OR COPYRIGHT HOLDERS BE LIABLE FOR ANY CLAIM, DAMAGES OR OTHER[m
[32m+[m[32m   * LIABILITY, WHETHER IN AN ACTION OF CONTRACT, TORT OR OTHERWISE, ARISING FROM,[m
[32m+[m[32m   * OUT OF OR IN CONNECTION WITH THE SOFTWARE OR THE USE OR OTHER DEALINGS IN THE[m
[32m+[m[32m   * SOFTWARE.[m
[32m+[m[32m   */[m
[32m+[m[32m  var isBrowser = typeof window !== 'undefined' && typeof document !== 'undefined';[m
[32m+[m
[32m+[m[32m  var longerTimeoutBrowsers = ['Edge', 'Trident', 'Firefox'];[m
[32m+[m[32m  var timeoutDuration = 0;[m
[32m+[m[32m  for (var i = 0; i < longerTimeoutBrowsers.length; i += 1) {[m
[32m+[m[32m    if (isBrowser && navigator.userAgent.indexOf(longerTimeoutBrowsers[i]) >= 0) {[m
[32m+[m[32m      timeoutDuration = 1;[m
[32m+[m[32m      break;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function microtaskDebounce(fn) {[m
[32m+[m[32m    var called = false;[m
[32m+[m[32m    return function () {[m
[32m+[m[32m      if (called) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      called = true;[m
[32m+[m[32m      window.Promise.resolve().then(function () {[m
[32m+[m[32m        called = false;[m
[32m+[m[32m        fn();[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function taskDebounce(fn) {[m
[32m+[m[32m    var scheduled = false;[m
[32m+[m[32m    return function () {[m
[32m+[m[32m      if (!scheduled) {[m
[32m+[m[32m        scheduled = true;[m
[32m+[m[32m        setTimeout(function () {[m
[32m+[m[32m          scheduled = false;[m
[32m+[m[32m          fn();[m
[32m+[m[32m        }, timeoutDuration);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var supportsMicroTasks = isBrowser && window.Promise;[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m  * Create a debounced version of a method, that's asynchronously deferred[m
[32m+[m[32m  * but called in the minimum time possible.[m
[32m+[m[32m  *[m
[32m+[m[32m  * @method[m
[32m+[m[32m  * @memberof Popper.Utils[m
[32m+[m[32m  * @argument {Function} fn[m
[32m+[m[32m  * @returns {Function}[m
[32m+[m[32m  */[m
[32m+[m[32m  var debounce = supportsMicroTasks ? microtaskDebounce : taskDebounce;[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Check if the given variable is a function[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Any} functionToCheck - variable to check[m
[32m+[m[32m   * @returns {Boolean} answer to: is a function?[m
[32m+[m[32m   */[m
[32m+[m[32m  function isFunction(functionToCheck) {[m
[32m+[m[32m    var getType = {};[m
[32m+[m[32m    return functionToCheck && getType.toString.call(functionToCheck) === '[object Function]';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get CSS computed property of the given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Eement} element[m
[32m+[m[32m   * @argument {String} property[m
[32m+[m[32m   */[m
[32m+[m[32m  function getStyleComputedProperty(element, property) {[m
[32m+[m[32m    if (element.nodeType !== 1) {[m
[32m+[m[32m      return [];[m
[32m+[m[32m    }[m
[32m+[m[32m    // NOTE: 1 DOM access here[m
[32m+[m[32m    var css = getComputedStyle(element, null);[m
[32m+[m[32m    return property ? css[property] : css;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Returns the parentNode or the host of the element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Element} parent[m
[32m+[m[32m   */[m
[32m+[m[32m  function getParentNode(element) {[m
[32m+[m[32m    if (element.nodeName === 'HTML') {[m
[32m+[m[32m      return element;[m
[32m+[m[32m    }[m
[32m+[m[32m    return element.parentNode || element.host;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Returns the scrolling parent of the given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Element} scroll parent[m
[32m+[m[32m   */[m
[32m+[m[32m  function getScrollParent(element) {[m
[32m+[m[32m    // Return body, `getScroll` will take care to get the correct `scrollTop` from it[m
[32m+[m[32m    if (!element) {[m
[32m+[m[32m      return document.body;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    switch (element.nodeName) {[m
[32m+[m[32m      case 'HTML':[m
[32m+[m[32m      case 'BODY':[m
[32m+[m[32m        return element.ownerDocument.body;[m
[32m+[m[32m      case '#document':[m
[32m+[m[32m        return element.body;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Firefox want us to check `-x` and `-y` variations as well[m
[32m+[m
[32m+[m[32m    var _getStyleComputedProp = getStyleComputedProperty(element),[m
[32m+[m[32m        overflow = _getStyleComputedProp.overflow,[m
[32m+[m[32m        overflowX = _getStyleComputedProp.overflowX,[m
[32m+[m[32m        overflowY = _getStyleComputedProp.overflowY;[m
[32m+[m
[32m+[m[32m    if (/(auto|scroll|overlay)/.test(overflow + overflowY + overflowX)) {[m
[32m+[m[32m      return element;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return getScrollParent(getParentNode(element));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var isIE11 = isBrowser && !!(window.MSInputMethodContext && document.documentMode);[m
[32m+[m[32m  var isIE10 = isBrowser && /MSIE 10/.test(navigator.userAgent);[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Determines if the browser is Internet Explorer[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {Number} version to check[m
[32m+[m[32m   * @returns {Boolean} isIE[m
[32m+[m[32m   */[m
[32m+[m[32m  function isIE(version) {[m
[32m+[m[32m    if (version === 11) {[m
[32m+[m[32m      return isIE11;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (version === 10) {[m
[32m+[m[32m      return isIE10;[m
[32m+[m[32m    }[m
[32m+[m[32m    return isIE11 || isIE10;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Returns the offset parent of the given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Element} offset parent[m
[32m+[m[32m   */[m
[32m+[m[32m  function getOffsetParent(element) {[m
[32m+[m[32m    if (!element) {[m
[32m+[m[32m      return document.documentElement;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var noOffsetParent = isIE(10) ? document.body : null;[m
[32m+[m
[32m+[m[32m    // NOTE: 1 DOM access here[m
[32m+[m[32m    var offsetParent = element.offsetParent;[m
[32m+[m[32m    // Skip hidden elements which don't have an offsetParent[m
[32m+[m[32m    while (offsetParent === noOffsetParent && element.nextElementSibling) {[m
[32m+[m[32m      offsetParent = (element = element.nextElementSibling).offsetParent;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var nodeName = offsetParent && offsetParent.nodeName;[m
[32m+[m
[32m+[m[32m    if (!nodeName || nodeName === 'BODY' || nodeName === 'HTML') {[m
[32m+[m[32m      return element ? element.ownerDocument.documentElement : document.documentElement;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // .offsetParent will return the closest TD or TABLE in case[m
[32m+[m[32m    // no offsetParent is present, I hate this job...[m
[32m+[m[32m    if (['TD', 'TABLE'].indexOf(offsetParent.nodeName) !== -1 && getStyleComputedProperty(offsetParent, 'position') === 'static') {[m
[32m+[m[32m      return getOffsetParent(offsetParent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return offsetParent;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function isOffsetContainer(element) {[m
[32m+[m[32m    var nodeName = element.nodeName;[m
[32m+[m
[32m+[m[32m    if (nodeName === 'BODY') {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    return nodeName === 'HTML' || getOffsetParent(element.firstElementChild) === element;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Finds the root node (document, shadowDOM root) of the given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} node[m
[32m+[m[32m   * @returns {Element} root node[m
[32m+[m[32m   */[m
[32m+[m[32m  function getRoot(node) {[m
[32m+[m[32m    if (node.parentNode !== null) {[m
[32m+[m[32m      return getRoot(node.parentNode);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return node;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Finds the offset parent common to the two provided nodes[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element1[m
[32m+[m[32m   * @argument {Element} element2[m
[32m+[m[32m   * @returns {Element} common offset parent[m
[32m+[m[32m   */[m
[32m+[m[32m  function findCommonOffsetParent(element1, element2) {[m
[32m+[m[32m    // This check is needed to avoid errors in case one of the elements isn't defined for any reason[m
[32m+[m[32m    if (!element1 || !element1.nodeType || !element2 || !element2.nodeType) {[m
[32m+[m[32m      return document.documentElement;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Here we make sure to give as "start" the element that comes first in the DOM[m
[32m+[m[32m    var order = element1.compareDocumentPosition(element2) & Node.DOCUMENT_POSITION_FOLLOWING;[m
[32m+[m[32m    var start = order ? element1 : element2;[m
[32m+[m[32m    var end = order ? element2 : element1;[m
[32m+[m
[32m+[m[32m    // Get common ancestor container[m
[32m+[m[32m    var range = document.createRange();[m
[32m+[m[32m    range.setStart(start, 0);[m
[32m+[m[32m    range.setEnd(end, 0);[m
[32m+[m[32m    var commonAncestorContainer = range.commonAncestorContainer;[m
[32m+[m
[32m+[m[32m    // Both nodes are inside #document[m
[32m+[m
[32m+[m[32m    if (element1 !== commonAncestorContainer && element2 !== commonAncestorContainer || start.contains(end)) {[m
[32m+[m[32m      if (isOffsetContainer(commonAncestorContainer)) {[m
[32m+[m[32m        return commonAncestorContainer;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return getOffsetParent(commonAncestorContainer);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // one of the nodes is inside shadowDOM, find which one[m
[32m+[m[32m    var element1root = getRoot(element1);[m
[32m+[m[32m    if (element1root.host) {[m
[32m+[m[32m      return findCommonOffsetParent(element1root.host, element2);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return findCommonOffsetParent(element1, getRoot(element2).host);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Gets the scroll value of the given element in the given side (top and left)[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @argument {String} side `top` or `left`[m
[32m+[m[32m   * @returns {number} amount of scrolled pixels[m
[32m+[m[32m   */[m
[32m+[m[32m  function getScroll(element) {[m
[32m+[m[32m    var side = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'top';[m
[32m+[m
[32m+[m[32m    var upperSide = side === 'top' ? 'scrollTop' : 'scrollLeft';[m
[32m+[m[32m    var nodeName = element.nodeName;[m
[32m+[m
[32m+[m[32m    if (nodeName === 'BODY' || nodeName === 'HTML') {[m
[32m+[m[32m      var html = element.ownerDocument.documentElement;[m
[32m+[m[32m      var scrollingElement = element.ownerDocument.scrollingElement || html;[m
[32m+[m[32m      return scrollingElement[upperSide];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return element[upperSide];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m   * Sum or subtract the element scroll values (left and top) from a given rect object[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {Object} rect - Rect object you want to change[m
[32m+[m[32m   * @param {HTMLElement} element - The element from the function reads the scroll values[m
[32m+[m[32m   * @param {Boolean} subtract - set to true if you want to subtract the scroll values[m
[32m+[m[32m   * @return {Object} rect - The modifier rect object[m
[32m+[m[32m   */[m
[32m+[m[32m  function includeScroll(rect, element) {[m
[32m+[m[32m    var subtract = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;[m
[32m+[m
[32m+[m[32m    var scrollTop = getScroll(element, 'top');[m
[32m+[m[32m    var scrollLeft = getScroll(element, 'left');[m
[32m+[m[32m    var modifier = subtract ? -1 : 1;[m
[32m+[m[32m    rect.top += scrollTop * modifier;[m
[32m+[m[32m    rect.bottom += scrollTop * modifier;[m
[32m+[m[32m    rect.left += scrollLeft * modifier;[m
[32m+[m[32m    rect.right += scrollLeft * modifier;[m
[32m+[m[32m    return rect;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m   * Helper to detect borders of a given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {CSSStyleDeclaration} styles[m
[32m+[m[32m   * Result of `getStyleComputedProperty` on the given element[m
[32m+[m[32m   * @param {String} axis - `x` or `y`[m
[32m+[m[32m   * @return {number} borders - The borders size of the given axis[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  function getBordersSize(styles, axis) {[m
[32m+[m[32m    var sideA = axis === 'x' ? 'Left' : 'Top';[m
[32m+[m[32m    var sideB = sideA === 'Left' ? 'Right' : 'Bottom';[m
[32m+[m
[32m+[m[32m    return parseFloat(styles['border' + sideA + 'Width'], 10) + parseFloat(styles['border' + sideB + 'Width'], 10);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getSize(axis, body, html, computedStyle) {[m
[32m+[m[32m    return Math.max(body['offset' + axis], body['scroll' + axis], html['client' + axis], html['offset' + axis], html['scroll' + axis], isIE(10) ? html['offset' + axis] + computedStyle['margin' + (axis === 'Height' ? 'Top' : 'Left')] + computedStyle['margin' + (axis === 'Height' ? 'Bottom' : 'Right')] : 0);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getWindowSizes() {[m
[32m+[m[32m    var body = document.body;[m
[32m+[m[32m    var html = document.documentElement;[m
[32m+[m[32m    var computedStyle = isIE(10) && getComputedStyle(html);[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m      height: getSize('Height', body, html, computedStyle),[m
[32m+[m[32m      width: getSize('Width', body, html, computedStyle)[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var classCallCheck = function (instance, Constructor) {[m
[32m+[m[32m    if (!(instance instanceof Constructor)) {[m
[32m+[m[32m      throw new TypeError("Cannot call a class as a function");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var createClass = function () {[m
[32m+[m[32m    function defineProperties(target, props) {[m
[32m+[m[32m      for (var i = 0; i < props.length; i++) {[m
[32m+[m[32m        var descriptor = props[i];[m
[32m+[m[32m        descriptor.enumerable = descriptor.enumerable || false;[m
[32m+[m[32m        descriptor.configurable = true;[m
[32m+[m[32m        if ("value" in descriptor) descriptor.writable = true;[m
[32m+[m[32m        Object.defineProperty(target, descriptor.key, descriptor);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return function (Constructor, protoProps, staticProps) {[m
[32m+[m[32m      if (protoProps) defineProperties(Constructor.prototype, protoProps);[m
[32m+[m[32m      if (staticProps) defineProperties(Constructor, staticProps);[m
[32m+[m[32m      return Constructor;[m
[32m+[m[32m    };[m
[32m+[m[32m  }();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m  var defineProperty = function (obj, key, value) {[m
[32m+[m[32m    if (key in obj) {[m
[32m+[m[32m      Object.defineProperty(obj, key, {[m
[32m+[m[32m        value: value,[m
[32m+[m[32m        enumerable: true,[m
[32m+[m[32m        configurable: true,[m
[32m+[m[32m        writable: true[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      obj[key] = value;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var _extends = Object.assign || function (target) {[m
[32m+[m[32m    for (var i = 1; i < arguments.length; i++) {[m
[32m+[m[32m      var source = arguments[i];[m
[32m+[m
[32m+[m[32m      for (var key in source) {[m
[32m+[m[32m        if (Object.prototype.hasOwnProperty.call(source, key)) {[m
[32m+[m[32m          target[key] = source[key];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return target;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Given element offsets, generate an output similar to getBoundingClientRect[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Object} offsets[m
[32m+[m[32m   * @returns {Object} ClientRect like output[m
[32m+[m[32m   */[m
[32m+[m[32m  function getClientRect(offsets) {[m
[32m+[m[32m    return _extends({}, offsets, {[m
[32m+[m[32m      right: offsets.left + offsets.width,[m
[32m+[m[32m      bottom: offsets.top + offsets.height[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get bounding client rect of given element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {HTMLElement} element[m
[32m+[m[32m   * @return {Object} client rect[m
[32m+[m[32m   */[m
[32m+[m[32m  function getBoundingClientRect(element) {[m
[32m+[m[32m    var rect = {};[m
[32m+[m
[32m+[m[32m    // IE10 10 FIX: Please, don't ask, the element isn't[m
[32m+[m[32m    // considered in DOM in some circumstances...[m
[32m+[m[32m    // This isn't reproducible in IE10 compatibility mode of IE11[m
[32m+[m[32m    try {[m
[32m+[m[32m      if (isIE(10)) {[m
[32m+[m[32m        rect = element.getBoundingClientRect();[m
[32m+[m[32m        var scrollTop = getScroll(element, 'top');[m
[32m+[m[32m        var scrollLeft = getScroll(element, 'left');[m
[32m+[m[32m        rect.top += scrollTop;[m
[32m+[m[32m        rect.left += scrollLeft;[m
[32m+[m[32m        rect.bottom += scrollTop;[m
[32m+[m[32m        rect.right += scrollLeft;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        rect = element.getBoundingClientRect();[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m
[32m+[m[32m    var result = {[m
[32m+[m[32m      left: rect.left,[m
[32m+[m[32m      top: rect.top,[m
[32m+[m[32m      width: rect.right - rect.left,[m
[32m+[m[32m      height: rect.bottom - rect.top[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // subtract scrollbar size from sizes[m
[32m+[m[32m    var sizes = element.nodeName === 'HTML' ? getWindowSizes() : {};[m
[32m+[m[32m    var width = sizes.width || element.clientWidth || result.right - result.left;[m
[32m+[m[32m    var height = sizes.height || element.clientHeight || result.bottom - result.top;[m
[32m+[m
[32m+[m[32m    var horizScrollbar = element.offsetWidth - width;[m
[32m+[m[32m    var vertScrollbar = element.offsetHeight - height;[m
[32m+[m
[32m+[m[32m    // if an hypothetical scrollbar is detected, we must be sure it's not a `border`[m
[32m+[m[32m    // we make this check conditional for performance reasons[m
[32m+[m[32m    if (horizScrollbar || vertScrollbar) {[m
[32m+[m[32m      var styles = getStyleComputedProperty(element);[m
[32m+[m[32m      horizScrollbar -= getBordersSize(styles, 'x');[m
[32m+[m[32m      vertScrollbar -= getBordersSize(styles, 'y');[m
[32m+[m
[32m+[m[32m      result.width -= horizScrollbar;[m
[32m+[m[32m      result.height -= vertScrollbar;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return getClientRect(result);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getOffsetRectRelativeToArbitraryNode(children, parent) {[m
[32m+[m[32m    var fixedPosition = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;[m
[32m+[m
[32m+[m[32m    var isIE10 = isIE(10);[m
[32m+[m[32m    var isHTML = parent.nodeName === 'HTML';[m
[32m+[m[32m    var childrenRect = getBoundingClientRect(children);[m
[32m+[m[32m    var parentRect = getBoundingClientRect(parent);[m
[32m+[m[32m    var scrollParent = getScrollParent(children);[m
[32m+[m
[32m+[m[32m    var styles = getStyleComputedProperty(parent);[m
[32m+[m[32m    var borderTopWidth = parseFloat(styles.borderTopWidth, 10);[m
[32m+[m[32m    var borderLeftWidth = parseFloat(styles.borderLeftWidth, 10);[m
[32m+[m
[32m+[m[32m    // In cases where the parent is fixed, we must ignore negative scroll in offset calc[m
[32m+[m[32m    if (fixedPosition && parent.nodeName === 'HTML') {[m
[32m+[m[32m      parentRect.top = Math.max(parentRect.top, 0);[m
[32m+[m[32m      parentRect.left = Math.max(parentRect.left, 0);[m
[32m+[m[32m    }[m
[32m+[m[32m    var offsets = getClientRect({[m
[32m+[m[32m      top: childrenRect.top - parentRect.top - borderTopWidth,[m
[32m+[m[32m      left: childrenRect.left - parentRect.left - borderLeftWidth,[m
[32m+[m[32m      width: childrenRect.width,[m
[32m+[m[32m      height: childrenRect.height[m
[32m+[m[32m    });[m
[32m+[m[32m    offsets.marginTop = 0;[m
[32m+[m[32m    offsets.marginLeft = 0;[m
[32m+[m
[32m+[m[32m    // Subtract margins of documentElement in case it's being used as parent[m
[32m+[m[32m    // we do this only on HTML because it's the only element that behaves[m
[32m+[m[32m    // differently when margins are applied to it. The margins are included in[m
[32m+[m[32m    // the box of the documentElement, in the other cases not.[m
[32m+[m[32m    if (!isIE10 && isHTML) {[m
[32m+[m[32m      var marginTop = parseFloat(styles.marginTop, 10);[m
[32m+[m[32m      var marginLeft = parseFloat(styles.marginLeft, 10);[m
[32m+[m
[32m+[m[32m      offsets.top -= borderTopWidth - marginTop;[m
[32m+[m[32m      offsets.bottom -= borderTopWidth - marginTop;[m
[32m+[m[32m      offsets.left -= borderLeftWidth - marginLeft;[m
[32m+[m[32m      offsets.right -= borderLeftWidth - marginLeft;[m
[32m+[m
[32m+[m[32m      // Attach marginTop and marginLeft because in some circumstances we may need them[m
[32m+[m[32m      offsets.marginTop = marginTop;[m
[32m+[m[32m      offsets.marginLeft = marginLeft;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (isIE10 && !fixedPosition ? parent.contains(scrollParent) : parent === scrollParent && scrollParent.nodeName !== 'BODY') {[m
[32m+[m[32m      offsets = includeScroll(offsets, parent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return offsets;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getViewportOffsetRectRelativeToArtbitraryNode(element) {[m
[32m+[m[32m    var excludeScroll = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;[m
[32m+[m
[32m+[m[32m    var html = element.ownerDocument.documentElement;[m
[32m+[m[32m    var relativeOffset = getOffsetRectRelativeToArbitraryNode(element, html);[m
[32m+[m[32m    var width = Math.max(html.clientWidth, window.innerWidth || 0);[m
[32m+[m[32m    var height = Math.max(html.clientHeight, window.innerHeight || 0);[m
[32m+[m
[32m+[m[32m    var scrollTop = !excludeScroll ? getScroll(html) : 0;[m
[32m+[m[32m    var scrollLeft = !excludeScroll ? getScroll(html, 'left') : 0;[m
[32m+[m
[32m+[m[32m    var offset = {[m
[32m+[m[32m      top: scrollTop - relativeOffset.top + relativeOffset.marginTop,[m
[32m+[m[32m      left: scrollLeft - relativeOffset.left + relativeOffset.marginLeft,[m
[32m+[m[32m      width: width,[m
[32m+[m[32m      height: height[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return getClientRect(offset);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Check if the given element is fixed or is inside a fixed parent[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @argument {Element} customContainer[m
[32m+[m[32m   * @returns {Boolean} answer to "isFixed?"[m
[32m+[m[32m   */[m
[32m+[m[32m  function isFixed(element) {[m
[32m+[m[32m    var nodeName = element.nodeName;[m
[32m+[m[32m    if (nodeName === 'BODY' || nodeName === 'HTML') {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (getStyleComputedProperty(element, 'position') === 'fixed') {[m
[32m+[m[32m      return true;[m
[32m+[m[32m    }[m
[32m+[m[32m    return isFixed(getParentNode(element));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Finds the first parent of an element that has a transformed property defined[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Element} first transformed parent or documentElement[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  function getFixedPositionOffsetParent(element) {[m
[32m+[m[32m    // This check is needed to avoid errors in case one of the elements isn't defined for any reason[m
[32m+[m[32m    if (!element || !element.parentElement || isIE()) {[m
[32m+[m[32m      return document.documentElement;[m
[32m+[m[32m    }[m
[32m+[m[32m    var el = element.parentElement;[m
[32m+[m[32m    while (el && getStyleComputedProperty(el, 'transform') === 'none') {[m
[32m+[m[32m      el = el.parentElement;[m
[32m+[m[32m    }[m
[32m+[m[32m    return el || document.documentElement;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Computed the boundaries limits and return them[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {HTMLElement} popper[m
[32m+[m[32m   * @param {HTMLElement} reference[m
[32m+[m[32m   * @param {number} padding[m
[32m+[m[32m   * @param {HTMLElement} boundariesElement - Element used to define the boundaries[m
[32m+[m[32m   * @param {Boolean} fixedPosition - Is in fixed position mode[m
[32m+[m[32m   * @returns {Object} Coordinates of the boundaries[m
[32m+[m[32m   */[m
[32m+[m[32m  function getBoundaries(popper, reference, padding, boundariesElement) {[m
[32m+[m[32m    var fixedPosition = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : false;[m
[32m+[m
[32m+[m[32m    // NOTE: 1 DOM access here[m
[32m+[m
[32m+[m[32m    var boundaries = { top: 0, left: 0 };[m
[32m+[m[32m    var offsetParent = fixedPosition ? getFixedPositionOffsetParent(popper) : findCommonOffsetParent(popper, reference);[m
[32m+[m
[32m+[m[32m    // Handle viewport case[m
[32m+[m[32m    if (boundariesElement === 'viewport') {[m
[32m+[m[32m      boundaries = getViewportOffsetRectRelativeToArtbitraryNode(offsetParent, fixedPosition);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Handle other cases based on DOM element used as boundaries[m
[32m+[m[32m      var boundariesNode = void 0;[m
[32m+[m[32m      if (boundariesElement === 'scrollParent') {[m
[32m+[m[32m        boundariesNode = getScrollParent(getParentNode(reference));[m
[32m+[m[32m        if (boundariesNode.nodeName === 'BODY') {[m
[32m+[m[32m          boundariesNode = popper.ownerDocument.documentElement;[m
[32m+[m[32m        }[m
[32m+[m[32m      } else if (boundariesElement === 'window') {[m
[32m+[m[32m        boundariesNode = popper.ownerDocument.documentElement;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        boundariesNode = boundariesElement;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var offsets = getOffsetRectRelativeToArbitraryNode(boundariesNode, offsetParent, fixedPosition);[m
[32m+[m
[32m+[m[32m      // In case of HTML, we need a different computation[m
[32m+[m[32m      if (boundariesNode.nodeName === 'HTML' && !isFixed(offsetParent)) {[m
[32m+[m[32m        var _getWindowSizes = getWindowSizes(),[m
[32m+[m[32m            height = _getWindowSizes.height,[m
[32m+[m[32m            width = _getWindowSizes.width;[m
[32m+[m
[32m+[m[32m        boundaries.top += offsets.top - offsets.marginTop;[m
[32m+[m[32m        boundaries.bottom = height + offsets.top;[m
[32m+[m[32m        boundaries.left += offsets.left - offsets.marginLeft;[m
[32m+[m[32m        boundaries.right = width + offsets.left;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // for all the other DOM elements, this one is good[m
[32m+[m[32m        boundaries = offsets;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add paddings[m
[32m+[m[32m    boundaries.left += padding;[m
[32m+[m[32m    boundaries.top += padding;[m
[32m+[m[32m    boundaries.right -= padding;[m
[32m+[m[32m    boundaries.bottom -= padding;[m
[32m+[m
[32m+[m[32m    return boundaries;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getArea(_ref) {[m
[32m+[m[32m    var width = _ref.width,[m
[32m+[m[32m        height = _ref.height;[m
[32m+[m
[32m+[m[32m    return width * height;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Utility used to transform the `auto` placement to the placement with more[m
[32m+[m[32m   * available space.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function computeAutoPlacement(placement, refRect, popper, reference, boundariesElement) {[m
[32m+[m[32m    var padding = arguments.length > 5 && arguments[5] !== undefined ? arguments[5] : 0;[m
[32m+[m
[32m+[m[32m    if (placement.indexOf('auto') === -1) {[m
[32m+[m[32m      return placement;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var boundaries = getBoundaries(popper, reference, padding, boundariesElement);[m
[32m+[m
[32m+[m[32m    var rects = {[m
[32m+[m[32m      top: {[m
[32m+[m[32m        width: boundaries.width,[m
[32m+[m[32m        height: refRect.top - boundaries.top[m
[32m+[m[32m      },[m
[32m+[m[32m      right: {[m
[32m+[m[32m        width: boundaries.right - refRect.right,[m
[32m+[m[32m        height: boundaries.height[m
[32m+[m[32m      },[m
[32m+[m[32m      bottom: {[m
[32m+[m[32m        width: boundaries.width,[m
[32m+[m[32m        height: boundaries.bottom - refRect.bottom[m
[32m+[m[32m      },[m
[32m+[m[32m      left: {[m
[32m+[m[32m        width: refRect.left - boundaries.left,[m
[32m+[m[32m        height: boundaries.height[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var sortedAreas = Object.keys(rects).map(function (key) {[m
[32m+[m[32m      return _extends({[m
[32m+[m[32m        key: key[m
[32m+[m[32m      }, rects[key], {[m
[32m+[m[32m        area: getArea(rects[key])[m
[32m+[m[32m      });[m
[32m+[m[32m    }).sort(function (a, b) {[m
[32m+[m[32m      return b.area - a.area;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    var filteredAreas = sortedAreas.filter(function (_ref2) {[m
[32m+[m[32m      var width = _ref2.width,[m
[32m+[m[32m          height = _ref2.height;[m
[32m+[m[32m      return width >= popper.clientWidth && height >= popper.clientHeight;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    var computedPlacement = filteredAreas.length > 0 ? filteredAreas[0].key : sortedAreas[0].key;[m
[32m+[m
[32m+[m[32m    var variation = placement.split('-')[1];[m
[32m+[m
[32m+[m[32m    return computedPlacement + (variation ? '-' + variation : '');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get offsets to the reference element[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {Object} state[m
[32m+[m[32m   * @param {Element} popper - the popper element[m
[32m+[m[32m   * @param {Element} reference - the reference element (the popper will be relative to this)[m
[32m+[m[32m   * @param {Element} fixedPosition - is in fixed position mode[m
[32m+[m[32m   * @returns {Object} An object containing the offsets which will be applied to the popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function getReferenceOffsets(state, popper, reference) {[m
[32m+[m[32m    var fixedPosition = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : null;[m
[32m+[m
[32m+[m[32m    var commonOffsetParent = fixedPosition ? getFixedPositionOffsetParent(popper) : findCommonOffsetParent(popper, reference);[m
[32m+[m[32m    return getOffsetRectRelativeToArbitraryNode(reference, commonOffsetParent, fixedPosition);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get the outer sizes of the given element (offset size + margins)[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Object} object containing width and height properties[m
[32m+[m[32m   */[m
[32m+[m[32m  function getOuterSizes(element) {[m
[32m+[m[32m    var styles = getComputedStyle(element);[m
[32m+[m[32m    var x = parseFloat(styles.marginTop) + parseFloat(styles.marginBottom);[m
[32m+[m[32m    var y = parseFloat(styles.marginLeft) + parseFloat(styles.marginRight);[m
[32m+[m[32m    var result = {[m
[32m+[m[32m      width: element.offsetWidth + y,[m
[32m+[m[32m      height: element.offsetHeight + x[m
[32m+[m[32m    };[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get the opposite placement of the given one[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {String} placement[m
[32m+[m[32m   * @returns {String} flipped placement[m
[32m+[m[32m   */[m
[32m+[m[32m  function getOppositePlacement(placement) {[m
[32m+[m[32m    var hash = { left: 'right', right: 'left', bottom: 'top', top: 'bottom' };[m
[32m+[m[32m    return placement.replace(/left|right|bottom|top/g, function (matched) {[m
[32m+[m[32m      return hash[matched];[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get offsets to the popper[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {Object} position - CSS position the Popper will get applied[m
[32m+[m[32m   * @param {HTMLElement} popper - the popper element[m
[32m+[m[32m   * @param {Object} referenceOffsets - the reference offsets (the popper will be relative to this)[m
[32m+[m[32m   * @param {String} placement - one of the valid placement options[m
[32m+[m[32m   * @returns {Object} popperOffsets - An object containing the offsets which will be applied to the popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function getPopperOffsets(popper, referenceOffsets, placement) {[m
[32m+[m[32m    placement = placement.split('-')[0];[m
[32m+[m
[32m+[m[32m    // Get popper node sizes[m
[32m+[m[32m    var popperRect = getOuterSizes(popper);[m
[32m+[m
[32m+[m[32m    // Add position, width and height to our offsets object[m
[32m+[m[32m    var popperOffsets = {[m
[32m+[m[32m      width: popperRect.width,[m
[32m+[m[32m      height: popperRect.height[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // depending by the popper placement we have to compute its offsets slightly differently[m
[32m+[m[32m    var isHoriz = ['right', 'left'].indexOf(placement) !== -1;[m
[32m+[m[32m    var mainSide = isHoriz ? 'top' : 'left';[m
[32m+[m[32m    var secondarySide = isHoriz ? 'left' : 'top';[m
[32m+[m[32m    var measurement = isHoriz ? 'height' : 'width';[m
[32m+[m[32m    var secondaryMeasurement = !isHoriz ? 'height' : 'width';[m
[32m+[m
[32m+[m[32m    popperOffsets[mainSide] = referenceOffsets[mainSide] + referenceOffsets[measurement] / 2 - popperRect[measurement] / 2;[m
[32m+[m[32m    if (placement === secondarySide) {[m
[32m+[m[32m      popperOffsets[secondarySide] = referenceOffsets[secondarySide] - popperRect[secondaryMeasurement];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      popperOffsets[secondarySide] = referenceOffsets[getOppositePlacement(secondarySide)];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return popperOffsets;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Mimics the `find` method of Array[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Array} arr[m
[32m+[m[32m   * @argument prop[m
[32m+[m[32m   * @argument value[m
[32m+[m[32m   * @returns index or -1[m
[32m+[m[32m   */[m
[32m+[m[32m  function find(arr, check) {[m
[32m+[m[32m    // use native find if supported[m
[32m+[m[32m    if (Array.prototype.find) {[m
[32m+[m[32m      return arr.find(check);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // use `filter` to obtain the same behavior of `find`[m
[32m+[m[32m    return arr.filter(check)[0];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Return the index of the matching object[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Array} arr[m
[32m+[m[32m   * @argument prop[m
[32m+[m[32m   * @argument value[m
[32m+[m[32m   * @returns index or -1[m
[32m+[m[32m   */[m
[32m+[m[32m  function findIndex(arr, prop, value) {[m
[32m+[m[32m    // use native findIndex if supported[m
[32m+[m[32m    if (Array.prototype.findIndex) {[m
[32m+[m[32m      return arr.findIndex(function (cur) {[m
[32m+[m[32m        return cur[prop] === value;[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // use `find` + `indexOf` if `findIndex` isn't supported[m
[32m+[m[32m    var match = find(arr, function (obj) {[m
[32m+[m[32m      return obj[prop] === value;[m
[32m+[m[32m    });[m
[32m+[m[32m    return arr.indexOf(match);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Loop trough the list of modifiers and run them in order,[m
[32m+[m[32m   * each of them will then edit the data object.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {dataObject} data[m
[32m+[m[32m   * @param {Array} modifiers[m
[32m+[m[32m   * @param {String} ends - Optional modifier name used as stopper[m
[32m+[m[32m   * @returns {dataObject}[m
[32m+[m[32m   */[m
[32m+[m[32m  function runModifiers(modifiers, data, ends) {[m
[32m+[m[32m    var modifiersToRun = ends === undefined ? modifiers : modifiers.slice(0, findIndex(modifiers, 'name', ends));[m
[32m+[m
[32m+[m[32m    modifiersToRun.forEach(function (modifier) {[m
[32m+[m[32m      if (modifier['function']) {[m
[32m+[m[32m        // eslint-disable-line dot-notation[m
[32m+[m[32m        console.warn('`modifier.function` is deprecated, use `modifier.fn`!');[m
[32m+[m[32m      }[m
[32m+[m[32m      var fn = modifier['function'] || modifier.fn; // eslint-disable-line dot-notation[m
[32m+[m[32m      if (modifier.enabled && isFunction(fn)) {[m
[32m+[m[32m        // Add properties to offsets to make them a complete clientRect object[m
[32m+[m[32m        // we do this before each modifier to make sure the previous one doesn't[m
[32m+[m[32m        // mess with these values[m
[32m+[m[32m        data.offsets.popper = getClientRect(data.offsets.popper);[m
[32m+[m[32m        data.offsets.reference = getClientRect(data.offsets.reference);[m
[32m+[m
[32m+[m[32m        data = fn(data, modifier);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Updates the position of the popper, computing the new offsets and applying[m
[32m+[m[32m   * the new style.<br />[m
[32m+[m[32m   * Prefer `scheduleUpdate` over `update` because of performance reasons.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function update() {[m
[32m+[m[32m    // if popper is destroyed, don't perform any further update[m
[32m+[m[32m    if (this.state.isDestroyed) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var data = {[m
[32m+[m[32m      instance: this,[m
[32m+[m[32m      styles: {},[m
[32m+[m[32m      arrowStyles: {},[m
[32m+[m[32m      attributes: {},[m
[32m+[m[32m      flipped: false,[m
[32m+[m[32m      offsets: {}[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // compute reference element offsets[m
[32m+[m[32m    data.offsets.reference = getReferenceOffsets(this.state, this.popper, this.reference, this.options.positionFixed);[m
[32m+[m
[32m+[m[32m    // compute auto placement, store placement inside the data object,[m
[32m+[m[32m    // modifiers will be able to edit `placement` if needed[m
[32m+[m[32m    // and refer to originalPlacement to know the original value[m
[32m+[m[32m    data.placement = computeAutoPlacement(this.options.placement, data.offsets.reference, this.popper, this.reference, this.options.modifiers.flip.boundariesElement, this.options.modifiers.flip.padding);[m
[32m+[m
[32m+[m[32m    // store the computed placement inside `originalPlacement`[m
[32m+[m[32m    data.originalPlacement = data.placement;[m
[32m+[m
[32m+[m[32m    data.positionFixed = this.options.positionFixed;[m
[32m+[m
[32m+[m[32m    // compute the popper offsets[m
[32m+[m[32m    data.offsets.popper = getPopperOffsets(this.popper, data.offsets.reference, data.placement);[m
[32m+[m
[32m+[m[32m    data.offsets.popper.position = this.options.positionFixed ? 'fixed' : 'absolute';[m
[32m+[m
[32m+[m[32m    // run the modifiers[m
[32m+[m[32m    data = runModifiers(this.modifiers, data);[m
[32m+[m
[32m+[m[32m    // the first `update` will call `onCreate` callback[m
[32m+[m[32m    // the other ones will call `onUpdate` callback[m
[32m+[m[32m    if (!this.state.isCreated) {[m
[32m+[m[32m      this.state.isCreated = true;[m
[32m+[m[32m      this.options.onCreate(data);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.options.onUpdate(data);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper used to know if the given modifier is enabled.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @returns {Boolean}[m
[32m+[m[32m   */[m
[32m+[m[32m  function isModifierEnabled(modifiers, modifierName) {[m
[32m+[m[32m    return modifiers.some(function (_ref) {[m
[32m+[m[32m      var name = _ref.name,[m
[32m+[m[32m          enabled = _ref.enabled;[m
[32m+[m[32m      return enabled && name === modifierName;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get the prefixed supported property name[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {String} property (camelCase)[m
[32m+[m[32m   * @returns {String} prefixed property (camelCase or PascalCase, depending on the vendor prefix)[m
[32m+[m[32m   */[m
[32m+[m[32m  function getSupportedPropertyName(property) {[m
[32m+[m[32m    var prefixes = [false, 'ms', 'Webkit', 'Moz', 'O'];[m
[32m+[m[32m    var upperProp = property.charAt(0).toUpperCase() + property.slice(1);[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < prefixes.length; i++) {[m
[32m+[m[32m      var prefix = prefixes[i];[m
[32m+[m[32m      var toCheck = prefix ? '' + prefix + upperProp : property;[m
[32m+[m[32m      if (typeof document.body.style[toCheck] !== 'undefined') {[m
[32m+[m[32m        return toCheck;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Destroy the popper[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function destroy() {[m
[32m+[m[32m    this.state.isDestroyed = true;[m
[32m+[m
[32m+[m[32m    // touch DOM only if `applyStyle` modifier is enabled[m
[32m+[m[32m    if (isModifierEnabled(this.modifiers, 'applyStyle')) {[m
[32m+[m[32m      this.popper.removeAttribute('x-placement');[m
[32m+[m[32m      this.popper.style.position = '';[m
[32m+[m[32m      this.popper.style.top = '';[m
[32m+[m[32m      this.popper.style.left = '';[m
[32m+[m[32m      this.popper.style.right = '';[m
[32m+[m[32m      this.popper.style.bottom = '';[m
[32m+[m[32m      this.popper.style.willChange = '';[m
[32m+[m[32m      this.popper.style[getSupportedPropertyName('transform')] = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.disableEventListeners();[m
[32m+[m
[32m+[m[32m    // remove the popper if user explicity asked for the deletion on destroy[m
[32m+[m[32m    // do not use `remove` because IE11 doesn't support it[m
[32m+[m[32m    if (this.options.removeOnDestroy) {[m
[32m+[m[32m      this.popper.parentNode.removeChild(this.popper);[m
[32m+[m[32m    }[m
[32m+[m[32m    return this;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get the window associated with the element[m
[32m+[m[32m   * @argument {Element} element[m
[32m+[m[32m   * @returns {Window}[m
[32m+[m[32m   */[m
[32m+[m[32m  function getWindow(element) {[m
[32m+[m[32m    var ownerDocument = element.ownerDocument;[m
[32m+[m[32m    return ownerDocument ? ownerDocument.defaultView : window;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function attachToScrollParents(scrollParent, event, callback, scrollParents) {[m
[32m+[m[32m    var isBody = scrollParent.nodeName === 'BODY';[m
[32m+[m[32m    var target = isBody ? scrollParent.ownerDocument.defaultView : scrollParent;[m
[32m+[m[32m    target.addEventListener(event, callback, { passive: true });[m
[32m+[m
[32m+[m[32m    if (!isBody) {[m
[32m+[m[32m      attachToScrollParents(getScrollParent(target.parentNode), event, callback, scrollParents);[m
[32m+[m[32m    }[m
[32m+[m[32m    scrollParents.push(target);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Setup needed event listeners used to update the popper position[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @private[m
[32m+[m[32m   */[m
[32m+[m[32m  function setupEventListeners(reference, options, state, updateBound) {[m
[32m+[m[32m    // Resize event listener on window[m
[32m+[m[32m    state.updateBound = updateBound;[m
[32m+[m[32m    getWindow(reference).addEventListener('resize', state.updateBound, { passive: true });[m
[32m+[m
[32m+[m[32m    // Scroll event listener on scroll parents[m
[32m+[m[32m    var scrollElement = getScrollParent(reference);[m
[32m+[m[32m    attachToScrollParents(scrollElement, 'scroll', state.updateBound, state.scrollParents);[m
[32m+[m[32m    state.scrollElement = scrollElement;[m
[32m+[m[32m    state.eventsEnabled = true;[m
[32m+[m
[32m+[m[32m    return state;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * It will add resize/scroll events and start recalculating[m
[32m+[m[32m   * position of the popper element when they are triggered.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function enableEventListeners() {[m
[32m+[m[32m    if (!this.state.eventsEnabled) {[m
[32m+[m[32m      this.state = setupEventListeners(this.reference, this.options, this.state, this.scheduleUpdate);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Remove event listeners used to update the popper position[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @private[m
[32m+[m[32m   */[m
[32m+[m[32m  function removeEventListeners(reference, state) {[m
[32m+[m[32m    // Remove resize event listener on window[m
[32m+[m[32m    getWindow(reference).removeEventListener('resize', state.updateBound);[m
[32m+[m
[32m+[m[32m    // Remove scroll event listener on scroll parents[m
[32m+[m[32m    state.scrollParents.forEach(function (target) {[m
[32m+[m[32m      target.removeEventListener('scroll', state.updateBound);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Reset state[m
[32m+[m[32m    state.updateBound = null;[m
[32m+[m[32m    state.scrollParents = [];[m
[32m+[m[32m    state.scrollElement = null;[m
[32m+[m[32m    state.eventsEnabled = false;[m
[32m+[m[32m    return state;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * It will remove resize/scroll events and won't recalculate popper position[m
[32m+[m[32m   * when they are triggered. It also won't trigger onUpdate callback anymore,[m
[32m+[m[32m   * unless you call `update` method manually.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  function disableEventListeners() {[m
[32m+[m[32m    if (this.state.eventsEnabled) {[m
[32m+[m[32m      cancelAnimationFrame(this.scheduleUpdate);[m
[32m+[m[32m      this.state = removeEventListeners(this.reference, this.state);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Tells if a given input is a number[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {*} input to check[m
[32m+[m[32m   * @return {Boolean}[m
[32m+[m[32m   */[m
[32m+[m[32m  function isNumeric(n) {[m
[32m+[m[32m    return n !== '' && !isNaN(parseFloat(n)) && isFinite(n);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Set the style to the given popper[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element - Element to apply the style to[m
[32m+[m[32m   * @argument {Object} styles[m
[32m+[m[32m   * Object with a list of properties and values which will be applied to the element[m
[32m+[m[32m   */[m
[32m+[m[32m  function setStyles(element, styles) {[m
[32m+[m[32m    Object.keys(styles).forEach(function (prop) {[m
[32m+[m[32m      var unit = '';[m
[32m+[m[32m      // add unit if the value is numeric and is one of the following[m
[32m+[m[32m      if (['width', 'height', 'top', 'right', 'bottom', 'left'].indexOf(prop) !== -1 && isNumeric(styles[prop])) {[m
[32m+[m[32m        unit = 'px';[m
[32m+[m[32m      }[m
[32m+[m[32m      element.style[prop] = styles[prop] + unit;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Set the attributes to the given popper[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {Element} element - Element to apply the attributes to[m
[32m+[m[32m   * @argument {Object} styles[m
[32m+[m[32m   * Object with a list of properties and values which will be applied to the element[m
[32m+[m[32m   */[m
[32m+[m[32m  function setAttributes(element, attributes) {[m
[32m+[m[32m    Object.keys(attributes).forEach(function (prop) {[m
[32m+[m[32m      var value = attributes[prop];[m
[32m+[m[32m      if (value !== false) {[m
[32m+[m[32m        element.setAttribute(prop, attributes[prop]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        element.removeAttribute(prop);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} data.styles - List of style properties - values to apply to popper element[m
[32m+[m[32m   * @argument {Object} data.attributes - List of attribute properties - values to apply to popper element[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The same data object[m
[32m+[m[32m   */[m
[32m+[m[32m  function applyStyle(data) {[m
[32m+[m[32m    // any property present in `data.styles` will be applied to the popper,[m
[32m+[m[32m    // in this way we can make the 3rd party modifiers add custom styles to it[m
[32m+[m[32m    // Be aware, modifiers could override the properties defined in the previous[m
[32m+[m[32m    // lines of this modifier![m
[32m+[m[32m    setStyles(data.instance.popper, data.styles);[m
[32m+[m
[32m+[m[32m    // any property present in `data.attributes` will be applied to the popper,[m
[32m+[m[32m    // they will be set as HTML attributes of the element[m
[32m+[m[32m    setAttributes(data.instance.popper, data.attributes);[m
[32m+[m
[32m+[m[32m    // if arrowElement is defined and arrowStyles has some properties[m
[32m+[m[32m    if (data.arrowElement && Object.keys(data.arrowStyles).length) {[m
[32m+[m[32m      setStyles(data.arrowElement, data.arrowStyles);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Set the x-placement attribute before everything else because it could be used[m
[32m+[m[32m   * to add margins to the popper margins needs to be calculated to get the[m
[32m+[m[32m   * correct popper offsets.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.modifiers[m
[32m+[m[32m   * @param {HTMLElement} reference - The reference element used to position the popper[m
[32m+[m[32m   * @param {HTMLElement} popper - The HTML element used as popper[m
[32m+[m[32m   * @param {Object} options - Popper.js options[m
[32m+[m[32m   */[m
[32m+[m[32m  function applyStyleOnLoad(reference, popper, options, modifierOptions, state) {[m
[32m+[m[32m    // compute reference element offsets[m
[32m+[m[32m    var referenceOffsets = getReferenceOffsets(state, popper, reference, options.positionFixed);[m
[32m+[m
[32m+[m[32m    // compute auto placement, store placement inside the data object,[m
[32m+[m[32m    // modifiers will be able to edit `placement` if needed[m
[32m+[m[32m    // and refer to originalPlacement to know the original value[m
[32m+[m[32m    var placement = computeAutoPlacement(options.placement, referenceOffsets, popper, reference, options.modifiers.flip.boundariesElement, options.modifiers.flip.padding);[m
[32m+[m
[32m+[m[32m    popper.setAttribute('x-placement', placement);[m
[32m+[m
[32m+[m[32m    // Apply `position` to popper before anything else because[m
[32m+[m[32m    // without the position applied we can't guarantee correct computations[m
[32m+[m[32m    setStyles(popper, { position: options.positionFixed ? 'fixed' : 'absolute' });[m
[32m+[m
[32m+[m[32m    return options;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function computeStyle(data, options) {[m
[32m+[m[32m    var x = options.x,[m
[32m+[m[32m        y = options.y;[m
[32m+[m[32m    var popper = data.offsets.popper;[m
[32m+[m
[32m+[m[32m    // Remove this legacy support in Popper.js v2[m
[32m+[m
[32m+[m[32m    var legacyGpuAccelerationOption = find(data.instance.modifiers, function (modifier) {[m
[32m+[m[32m      return modifier.name === 'applyStyle';[m
[32m+[m[32m    }).gpuAcceleration;[m
[32m+[m[32m    if (legacyGpuAccelerationOption !== undefined) {[m
[32m+[m[32m      console.warn('WARNING: `gpuAcceleration` option moved to `computeStyle` modifier and will not be supported in future versions of Popper.js!');[m
[32m+[m[32m    }[m
[32m+[m[32m    var gpuAcceleration = legacyGpuAccelerationOption !== undefined ? legacyGpuAccelerationOption : options.gpuAcceleration;[m
[32m+[m
[32m+[m[32m    var offsetParent = getOffsetParent(data.instance.popper);[m
[32m+[m[32m    var offsetParentRect = getBoundingClientRect(offsetParent);[m
[32m+[m
[32m+[m[32m    // Styles[m
[32m+[m[32m    var styles = {[m
[32m+[m[32m      position: popper.position[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Avoid blurry text by using full pixel integers.[m
[32m+[m[32m    // For pixel-perfect positioning, top/bottom prefers rounded[m
[32m+[m[32m    // values, while left/right prefers floored values.[m
[32m+[m[32m    var offsets = {[m
[32m+[m[32m      left: Math.floor(popper.left),[m
[32m+[m[32m      top: Math.round(popper.top),[m
[32m+[m[32m      bottom: Math.round(popper.bottom),[m
[32m+[m[32m      right: Math.floor(popper.right)[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var sideA = x === 'bottom' ? 'top' : 'bottom';[m
[32m+[m[32m    var sideB = y === 'right' ? 'left' : 'right';[m
[32m+[m
[32m+[m[32m    // if gpuAcceleration is set to `true` and transform is supported,[m
[32m+[m[32m    //  we use `translate3d` to apply the position to the popper we[m
[32m+[m[32m    // automatically use the supported prefixed version if needed[m
[32m+[m[32m    var prefixedProperty = getSupportedPropertyName('transform');[m
[32m+[m
[32m+[m[32m    // now, let's make a step back and look at this code closely (wtf?)[m
[32m+[m[32m    // If the content of the popper grows once it's been positioned, it[m
[32m+[m[32m    // may happen that the popper gets misplaced because of the new content[m
[32m+[m[32m    // overflowing its reference element[m
[32m+[m[32m    // To avoid this problem, we provide two options (x and y), which allow[m
[32m+[m[32m    // the consumer to define the offset origin.[m
[32m+[m[32m    // If we position a popper on top of a reference element, we can set[m
[32m+[m[32m    // `x` to `top` to make the popper grow towards its top instead of[m
[32m+[m[32m    // its bottom.[m
[32m+[m[32m    var left = void 0,[m
[32m+[m[32m        top = void 0;[m
[32m+[m[32m    if (sideA === 'bottom') {[m
[32m+[m[32m      top = -offsetParentRect.height + offsets.bottom;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      top = offsets.top;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (sideB === 'right') {[m
[32m+[m[32m      left = -offsetParentRect.width + offsets.right;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      left = offsets.left;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (gpuAcceleration && prefixedProperty) {[m
[32m+[m[32m      styles[prefixedProperty] = 'translate3d(' + left + 'px, ' + top + 'px, 0)';[m
[32m+[m[32m      styles[sideA] = 0;[m
[32m+[m[32m      styles[sideB] = 0;[m
[32m+[m[32m      styles.willChange = 'transform';[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // othwerise, we use the standard `top`, `left`, `bottom` and `right` properties[m
[32m+[m[32m      var invertTop = sideA === 'bottom' ? -1 : 1;[m
[32m+[m[32m      var invertLeft = sideB === 'right' ? -1 : 1;[m
[32m+[m[32m      styles[sideA] = top * invertTop;[m
[32m+[m[32m      styles[sideB] = left * invertLeft;[m
[32m+[m[32m      styles.willChange = sideA + ', ' + sideB;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Attributes[m
[32m+[m[32m    var attributes = {[m
[32m+[m[32m      'x-placement': data.placement[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Update `data` attributes, styles and arrowStyles[m
[32m+[m[32m    data.attributes = _extends({}, attributes, data.attributes);[m
[32m+[m[32m    data.styles = _extends({}, styles, data.styles);[m
[32m+[m[32m    data.arrowStyles = _extends({}, data.offsets.arrow, data.arrowStyles);[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper used to know if the given modifier depends from another one.<br />[m
[32m+[m[32m   * It checks if the needed modifier is listed and enabled.[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @param {Array} modifiers - list of modifiers[m
[32m+[m[32m   * @param {String} requestingName - name of requesting modifier[m
[32m+[m[32m   * @param {String} requestedName - name of requested modifier[m
[32m+[m[32m   * @returns {Boolean}[m
[32m+[m[32m   */[m
[32m+[m[32m  function isModifierRequired(modifiers, requestingName, requestedName) {[m
[32m+[m[32m    var requesting = find(modifiers, function (_ref) {[m
[32m+[m[32m      var name = _ref.name;[m
[32m+[m[32m      return name === requestingName;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    var isRequired = !!requesting && modifiers.some(function (modifier) {[m
[32m+[m[32m      return modifier.name === requestedName && modifier.enabled && modifier.order < requesting.order;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if (!isRequired) {[m
[32m+[m[32m      var _requesting = '`' + requestingName + '`';[m
[32m+[m[32m      var requested = '`' + requestedName + '`';[m
[32m+[m[32m      console.warn(requested + ' modifier is required by ' + _requesting + ' modifier in order to work, be sure to include it before ' + _requesting + '!');[m
[32m+[m[32m    }[m
[32m+[m[32m    return isRequired;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function arrow(data, options) {[m
[32m+[m[32m    var _data$offsets$arrow;[m
[32m+[m
[32m+[m[32m    // arrow depends on keepTogether in order to work[m
[32m+[m[32m    if (!isModifierRequired(data.instance.modifiers, 'arrow', 'keepTogether')) {[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var arrowElement = options.element;[m
[32m+[m
[32m+[m[32m    // if arrowElement is a string, suppose it's a CSS selector[m
[32m+[m[32m    if (typeof arrowElement === 'string') {[m
[32m+[m[32m      arrowElement = data.instance.popper.querySelector(arrowElement);[m
[32m+[m
[32m+[m[32m      // if arrowElement is not found, don't run the modifier[m
[32m+[m[32m      if (!arrowElement) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // if the arrowElement isn't a query selector we must check that the[m
[32m+[m[32m      // provided DOM node is child of its popper node[m
[32m+[m[32m      if (!data.instance.popper.contains(arrowElement)) {[m
[32m+[m[32m        console.warn('WARNING: `arrow.element` must be child of its popper element!');[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var placement = data.placement.split('-')[0];[m
[32m+[m[32m    var _data$offsets = data.offsets,[m
[32m+[m[32m        popper = _data$offsets.popper,[m
[32m+[m[32m        reference = _data$offsets.reference;[m
[32m+[m
[32m+[m[32m    var isVertical = ['left', 'right'].indexOf(placement) !== -1;[m
[32m+[m
[32m+[m[32m    var len = isVertical ? 'height' : 'width';[m
[32m+[m[32m    var sideCapitalized = isVertical ? 'Top' : 'Left';[m
[32m+[m[32m    var side = sideCapitalized.toLowerCase();[m
[32m+[m[32m    var altSide = isVertical ? 'left' : 'top';[m
[32m+[m[32m    var opSide = isVertical ? 'bottom' : 'right';[m
[32m+[m[32m    var arrowElementSize = getOuterSizes(arrowElement)[len];[m
[32m+[m
[32m+[m[32m    //[m
[32m+[m[32m    // extends keepTogether behavior making sure the popper and its[m
[32m+[m[32m    // reference have enough pixels in conjuction[m
[32m+[m[32m    //[m
[32m+[m
[32m+[m[32m    // top/left side[m
[32m+[m[32m    if (reference[opSide] - arrowElementSize < popper[side]) {[m
[32m+[m[32m      data.offsets.popper[side] -= popper[side] - (reference[opSide] - arrowElementSize);[m
[32m+[m[32m    }[m
[32m+[m[32m    // bottom/right side[m
[32m+[m[32m    if (reference[side] + arrowElementSize > popper[opSide]) {[m
[32m+[m[32m      data.offsets.popper[side] += reference[side] + arrowElementSize - popper[opSide];[m
[32m+[m[32m    }[m
[32m+[m[32m    data.offsets.popper = getClientRect(data.offsets.popper);[m
[32m+[m
[32m+[m[32m    // compute center of the popper[m
[32m+[m[32m    var center = reference[side] + reference[len] / 2 - arrowElementSize / 2;[m
[32m+[m
[32m+[m[32m    // Compute the sideValue using the updated popper offsets[m
[32m+[m[32m    // take popper margin in account because we don't have this info available[m
[32m+[m[32m    var css = getStyleComputedProperty(data.instance.popper);[m
[32m+[m[32m    var popperMarginSide = parseFloat(css['margin' + sideCapitalized], 10);[m
[32m+[m[32m    var popperBorderSide = parseFloat(css['border' + sideCapitalized + 'Width'], 10);[m
[32m+[m[32m    var sideValue = center - data.offsets.popper[side] - popperMarginSide - popperBorderSide;[m
[32m+[m
[32m+[m[32m    // prevent arrowElement from being placed not contiguously to its popper[m
[32m+[m[32m    sideValue = Math.max(Math.min(popper[len] - arrowElementSize, sideValue), 0);[m
[32m+[m
[32m+[m[32m    data.arrowElement = arrowElement;[m
[32m+[m[32m    data.offsets.arrow = (_data$offsets$arrow = {}, defineProperty(_data$offsets$arrow, side, Math.round(sideValue)), defineProperty(_data$offsets$arrow, altSide, ''), _data$offsets$arrow);[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Get the opposite placement variation of the given one[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {String} placement variation[m
[32m+[m[32m   * @returns {String} flipped placement variation[m
[32m+[m[32m   */[m
[32m+[m[32m  function getOppositeVariation(variation) {[m
[32m+[m[32m    if (variation === 'end') {[m
[32m+[m[32m      return 'start';[m
[32m+[m[32m    } else if (variation === 'start') {[m
[32m+[m[32m      return 'end';[m
[32m+[m[32m    }[m
[32m+[m[32m    return variation;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * List of accepted placements to use as values of the `placement` option.<br />[m
[32m+[m[32m   * Valid placements are:[m
[32m+[m[32m   * - `auto`[m
[32m+[m[32m   * - `top`[m
[32m+[m[32m   * - `right`[m
[32m+[m[32m   * - `bottom`[m
[32m+[m[32m   * - `left`[m
[32m+[m[32m   *[m
[32m+[m[32m   * Each placement can have a variation from this list:[m
[32m+[m[32m   * - `-start`[m
[32m+[m[32m   * - `-end`[m
[32m+[m[32m   *[m
[32m+[m[32m   * Variations are interpreted easily if you think of them as the left to right[m
[32m+[m[32m   * written languages. Horizontally (`top` and `bottom`), `start` is left and `end`[m
[32m+[m[32m   * is right.<br />[m
[32m+[m[32m   * Vertically (`left` and `right`), `start` is top and `end` is bottom.[m
[32m+[m[32m   *[m
[32m+[m[32m   * Some valid examples are:[m
[32m+[m[32m   * - `top-end` (on top of reference, right aligned)[m
[32m+[m[32m   * - `right-start` (on right of reference, top aligned)[m
[32m+[m[32m   * - `bottom` (on bottom, centered)[m
[32m+[m[32m   * - `auto-right` (on the side with more space available, alignment depends by placement)[m
[32m+[m[32m   *[m
[32m+[m[32m   * @static[m
[32m+[m[32m   * @type {Array}[m
[32m+[m[32m   * @enum {String}[m
[32m+[m[32m   * @readonly[m
[32m+[m[32m   * @method placements[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  var placements = ['auto-start', 'auto', 'auto-end', 'top-start', 'top', 'top-end', 'right-start', 'right', 'right-end', 'bottom-end', 'bottom', 'bottom-start', 'left-end', 'left', 'left-start'];[m
[32m+[m
[32m+[m[32m  // Get rid of `auto` `auto-start` and `auto-end`[m
[32m+[m[32m  var validPlacements = placements.slice(3);[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Given an initial placement, returns all the subsequent placements[m
[32m+[m[32m   * clockwise (or counter-clockwise).[m
[32m+[m[32m   *[m
[32m+[m[32m   * @method[m
[32m+[m[32m   * @memberof Popper.Utils[m
[32m+[m[32m   * @argument {String} placement - A valid placement (it accepts variations)[m
[32m+[m[32m   * @argument {Boolean} counter - Set to true to walk the placements counterclockwise[m
[32m+[m[32m   * @returns {Array} placements including their variations[m
[32m+[m[32m   */[m
[32m+[m[32m  function clockwise(placement) {[m
[32m+[m[32m    var counter = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;[m
[32m+[m
[32m+[m[32m    var index = validPlacements.indexOf(placement);[m
[32m+[m[32m    var arr = validPlacements.slice(index + 1).concat(validPlacements.slice(0, index));[m
[32m+[m[32m    return counter ? arr.reverse() : arr;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var BEHAVIORS = {[m
[32m+[m[32m    FLIP: 'flip',[m
[32m+[m[32m    CLOCKWISE: 'clockwise',[m
[32m+[m[32m    COUNTERCLOCKWISE: 'counterclockwise'[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function flip(data, options) {[m
[32m+[m[32m    // if `inner` modifier is enabled, we can't use the `flip` modifier[m
[32m+[m[32m    if (isModifierEnabled(data.instance.modifiers, 'inner')) {[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.flipped && data.placement === data.originalPlacement) {[m
[32m+[m[32m      // seems like flip is trying to loop, probably there's not enough space on any of the flippable sides[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var boundaries = getBoundaries(data.instance.popper, data.instance.reference, options.padding, options.boundariesElement, data.positionFixed);[m
[32m+[m
[32m+[m[32m    var placement = data.placement.split('-')[0];[m
[32m+[m[32m    var placementOpposite = getOppositePlacement(placement);[m
[32m+[m[32m    var variation = data.placement.split('-')[1] || '';[m
[32m+[m
[32m+[m[32m    var flipOrder = [];[m
[32m+[m
[32m+[m[32m    switch (options.behavior) {[m
[32m+[m[32m      case BEHAVIORS.FLIP:[m
[32m+[m[32m        flipOrder = [placement, placementOpposite];[m
[32m+[m[32m        break;[m
[32m+[m[32m      case BEHAVIORS.CLOCKWISE:[m
[32m+[m[32m        flipOrder = clockwise(placement);[m
[32m+[m[32m        break;[m
[32m+[m[32m      case BEHAVIORS.COUNTERCLOCKWISE:[m
[32m+[m[32m        flipOrder = clockwise(placement, true);[m
[32m+[m[32m        break;[m
[32m+[m[32m      default:[m
[32m+[m[32m        flipOrder = options.behavior;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    flipOrder.forEach(function (step, index) {[m
[32m+[m[32m      if (placement !== step || flipOrder.length === index + 1) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      placement = data.placement.split('-')[0];[m
[32m+[m[32m      placementOpposite = getOppositePlacement(placement);[m
[32m+[m
[32m+[m[32m      var popperOffsets = data.offsets.popper;[m
[32m+[m[32m      var refOffsets = data.offsets.reference;[m
[32m+[m
[32m+[m[32m      // using floor because the reference offsets may contain decimals we are not going to consider here[m
[32m+[m[32m      var floor = Math.floor;[m
[32m+[m[32m      var overlapsRef = placement === 'left' && floor(popperOffsets.right) > floor(refOffsets.left) || placement === 'right' && floor(popperOffsets.left) < floor(refOffsets.right) || placement === 'top' && floor(popperOffsets.bottom) > floor(refOffsets.top) || placement === 'bottom' && floor(popperOffsets.top) < floor(refOffsets.bottom);[m
[32m+[m
[32m+[m[32m      var overflowsLeft = floor(popperOffsets.left) < floor(boundaries.left);[m
[32m+[m[32m      var overflowsRight = floor(popperOffsets.right) > floor(boundaries.right);[m
[32m+[m[32m      var overflowsTop = floor(popperOffsets.top) < floor(boundaries.top);[m
[32m+[m[32m      var overflowsBottom = floor(popperOffsets.bottom) > floor(boundaries.bottom);[m
[32m+[m
[32m+[m[32m      var overflowsBoundaries = placement === 'left' && overflowsLeft || placement === 'right' && overflowsRight || placement === 'top' && overflowsTop || placement === 'bottom' && overflowsBottom;[m
[32m+[m
[32m+[m[32m      // flip the variation if required[m
[32m+[m[32m      var isVertical = ['top', 'bottom'].indexOf(placement) !== -1;[m
[32m+[m[32m      var flippedVariation = !!options.flipVariations && (isVertical && variation === 'start' && overflowsLeft || isVertical && variation === 'end' && overflowsRight || !isVertical && variation === 'start' && overflowsTop || !isVertical && variation === 'end' && overflowsBottom);[m
[32m+[m
[32m+[m[32m      if (overlapsRef || overflowsBoundaries || flippedVariation) {[m
[32m+[m[32m        // this boolean to detect any flip loop[m
[32m+[m[32m        data.flipped = true;[m
[32m+[m
[32m+[m[32m        if (overlapsRef || overflowsBoundaries) {[m
[32m+[m[32m          placement = flipOrder[index + 1];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (flippedVariation) {[m
[32m+[m[32m          variation = getOppositeVariation(variation);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        data.placement = placement + (variation ? '-' + variation : '');[m
[32m+[m
[32m+[m[32m        // this object contains `position`, we want to preserve it along with[m
[32m+[m[32m        // any additional property we may add in the future[m
[32m+[m[32m        data.offsets.popper = _extends({}, data.offsets.popper, getPopperOffsets(data.instance.popper, data.offsets.reference, data.placement));[m
[32m+[m
[32m+[m[32m        data = runModifiers(data.instance.modifiers, data, 'flip');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function keepTogether(data) {[m
[32m+[m[32m    var _data$offsets = data.offsets,[m
[32m+[m[32m        popper = _data$offsets.popper,[m
[32m+[m[32m        reference = _data$offsets.reference;[m
[32m+[m
[32m+[m[32m    var placement = data.placement.split('-')[0];[m
[32m+[m[32m    var floor = Math.floor;[m
[32m+[m[32m    var isVertical = ['top', 'bottom'].indexOf(placement) !== -1;[m
[32m+[m[32m    var side = isVertical ? 'right' : 'bottom';[m
[32m+[m[32m    var opSide = isVertical ? 'left' : 'top';[m
[32m+[m[32m    var measurement = isVertical ? 'width' : 'height';[m
[32m+[m
[32m+[m[32m    if (popper[side] < floor(reference[opSide])) {[m
[32m+[m[32m      data.offsets.popper[opSide] = floor(reference[opSide]) - popper[measurement];[m
[32m+[m[32m    }[m
[32m+[m[32m    if (popper[opSide] > floor(reference[side])) {[m
[32m+[m[32m      data.offsets.popper[opSide] = floor(reference[side]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Converts a string containing value + unit into a px value number[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof {modifiers~offset}[m
[32m+[m[32m   * @private[m
[32m+[m[32m   * @argument {String} str - Value + unit string[m
[32m+[m[32m   * @argument {String} measurement - `height` or `width`[m
[32m+[m[32m   * @argument {Object} popperOffsets[m
[32m+[m[32m   * @argument {Object} referenceOffsets[m
[32m+[m[32m   * @returns {Number|String}[m
[32m+[m[32m   * Value in pixels, or original string if no values were extracted[m
[32m+[m[32m   */[m
[32m+[m[32m  function toValue(str, measurement, popperOffsets, referenceOffsets) {[m
[32m+[m[32m    // separate value from unit[m
[32m+[m[32m    var split = str.match(/((?:\-|\+)?\d*\.?\d*)(.*)/);[m
[32m+[m[32m    var value = +split[1];[m
[32m+[m[32m    var unit = split[2];[m
[32m+[m
[32m+[m[32m    // If it's not a number it's an operator, I guess[m
[32m+[m[32m    if (!value) {[m
[32m+[m[32m      return str;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (unit.indexOf('%') === 0) {[m
[32m+[m[32m      var element = void 0;[m
[32m+[m[32m      switch (unit) {[m
[32m+[m[32m        case '%p':[m
[32m+[m[32m          element = popperOffsets;[m
[32m+[m[32m          break;[m
[32m+[m[32m        case '%':[m
[32m+[m[32m        case '%r':[m
[32m+[m[32m        default:[m
[32m+[m[32m          element = referenceOffsets;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var rect = getClientRect(element);[m
[32m+[m[32m      return rect[measurement] / 100 * value;[m
[32m+[m[32m    } else if (unit === 'vh' || unit === 'vw') {[m
[32m+[m[32m      // if is a vh or vw, we calculate the size based on the viewport[m
[32m+[m[32m      var size = void 0;[m
[32m+[m[32m      if (unit === 'vh') {[m
[32m+[m[32m        size = Math.max(document.documentElement.clientHeight, window.innerHeight || 0);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        size = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);[m
[32m+[m[32m      }[m
[32m+[m[32m      return size / 100 * value;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // if is an explicit pixel unit, we get rid of the unit and keep the value[m
[32m+[m[32m      // if is an implicit unit, it's px, and we return just the value[m
[32m+[m[32m      return value;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Parse an `offset` string to extrapolate `x` and `y` numeric offsets.[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof {modifiers~offset}[m
[32m+[m[32m   * @private[m
[32m+[m[32m   * @argument {String} offset[m
[32m+[m[32m   * @argument {Object} popperOffsets[m
[32m+[m[32m   * @argument {Object} referenceOffsets[m
[32m+[m[32m   * @argument {String} basePlacement[m
[32m+[m[32m   * @returns {Array} a two cells array with x and y offsets in numbers[m
[32m+[m[32m   */[m
[32m+[m[32m  function parseOffset(offset, popperOffsets, referenceOffsets, basePlacement) {[m
[32m+[m[32m    var offsets = [0, 0];[m
[32m+[m
[32m+[m[32m    // Use height if placement is left or right and index is 0 otherwise use width[m
[32m+[m[32m    // in this way the first offset will use an axis and the second one[m
[32m+[m[32m    // will use the other one[m
[32m+[m[32m    var useHeight = ['right', 'left'].indexOf(basePlacement) !== -1;[m
[32m+[m
[32m+[m[32m    // Split the offset string to obtain a list of values and operands[m
[32m+[m[32m    // The regex addresses values with the plus or minus sign in front (+10, -20, etc)[m
[32m+[m[32m    var fragments = offset.split(/(\+|\-)/).map(function (frag) {[m
[32m+[m[32m      return frag.trim();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Detect if the offset string contains a pair of values or a single one[m
[32m+[m[32m    // they could be separated by comma or space[m
[32m+[m[32m    var divider = fragments.indexOf(find(fragments, function (frag) {[m
[32m+[m[32m      return frag.search(/,|\s/) !== -1;[m
[32m+[m[32m    }));[m
[32m+[m
[32m+[m[32m    if (fragments[divider] && fragments[divider].indexOf(',') === -1) {[m
[32m+[m[32m      console.warn('Offsets separated by white space(s) are deprecated, use a comma (,) instead.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // If divider is found, we divide the list of values and operands to divide[m
[32m+[m[32m    // them by ofset X and Y.[m
[32m+[m[32m    var splitRegex = /\s*,\s*|\s+/;[m
[32m+[m[32m    var ops = divider !== -1 ? [fragments.slice(0, divider).concat([fragments[divider].split(splitRegex)[0]]), [fragments[divider].split(splitRegex)[1]].concat(fragments.slice(divider + 1))] : [fragments];[m
[32m+[m
[32m+[m[32m    // Convert the values with units to absolute pixels to allow our computations[m
[32m+[m[32m    ops = ops.map(function (op, index) {[m
[32m+[m[32m      // Most of the units rely on the orientation of the popper[m
[32m+[m[32m      var measurement = (index === 1 ? !useHeight : useHeight) ? 'height' : 'width';[m
[32m+[m[32m      var mergeWithPrevious = false;[m
[32m+[m[32m      return op[m
[32m+[m[32m      // This aggregates any `+` or `-` sign that aren't considered operators[m
[32m+[m[32m      // e.g.: 10 + +5 => [10, +, +5][m
[32m+[m[32m      .reduce(function (a, b) {[m
[32m+[m[32m        if (a[a.length - 1] === '' && ['+', '-'].indexOf(b) !== -1) {[m
[32m+[m[32m          a[a.length - 1] = b;[m
[32m+[m[32m          mergeWithPrevious = true;[m
[32m+[m[32m          return a;[m
[32m+[m[32m        } else if (mergeWithPrevious) {[m
[32m+[m[32m          a[a.length - 1] += b;[m
[32m+[m[32m          mergeWithPrevious = false;[m
[32m+[m[32m          return a;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          return a.concat(b);[m
[32m+[m[32m        }[m
[32m+[m[32m      }, [])[m
[32m+[m[32m      // Here we convert the string values into number values (in px)[m
[32m+[m[32m      .map(function (str) {[m
[32m+[m[32m        return toValue(str, measurement, popperOffsets, referenceOffsets);[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Loop trough the offsets arrays and execute the operations[m
[32m+[m[32m    ops.forEach(function (op, index) {[m
[32m+[m[32m      op.forEach(function (frag, index2) {[m
[32m+[m[32m        if (isNumeric(frag)) {[m
[32m+[m[32m          offsets[index] += frag * (op[index2 - 1] === '-' ? -1 : 1);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m    return offsets;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @argument {Number|String} options.offset=0[m
[32m+[m[32m   * The offset value as described in the modifier description[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function offset(data, _ref) {[m
[32m+[m[32m    var offset = _ref.offset;[m
[32m+[m[32m    var placement = data.placement,[m
[32m+[m[32m        _data$offsets = data.offsets,[m
[32m+[m[32m        popper = _data$offsets.popper,[m
[32m+[m[32m        reference = _data$offsets.reference;[m
[32m+[m
[32m+[m[32m    var basePlacement = placement.split('-')[0];[m
[32m+[m
[32m+[m[32m    var offsets = void 0;[m
[32m+[m[32m    if (isNumeric(+offset)) {[m
[32m+[m[32m      offsets = [+offset, 0];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      offsets = parseOffset(offset, popper, reference, basePlacement);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (basePlacement === 'left') {[m
[32m+[m[32m      popper.top += offsets[0];[m
[32m+[m[32m      popper.left -= offsets[1];[m
[32m+[m[32m    } else if (basePlacement === 'right') {[m
[32m+[m[32m      popper.top += offsets[0];[m
[32m+[m[32m      popper.left += offsets[1];[m
[32m+[m[32m    } else if (basePlacement === 'top') {[m
[32m+[m[32m      popper.left += offsets[0];[m
[32m+[m[32m      popper.top -= offsets[1];[m
[32m+[m[32m    } else if (basePlacement === 'bottom') {[m
[32m+[m[32m      popper.left += offsets[0];[m
[32m+[m[32m      popper.top += offsets[1];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    data.popper = popper;[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function preventOverflow(data, options) {[m
[32m+[m[32m    var boundariesElement = options.boundariesElement || getOffsetParent(data.instance.popper);[m
[32m+[m
[32m+[m[32m    // If offsetParent is the reference element, we really want to[m
[32m+[m[32m    // go one step up and use the next offsetParent as reference to[m
[32m+[m[32m    // avoid to make this modifier completely useless and look like broken[m
[32m+[m[32m    if (data.instance.reference === boundariesElement) {[m
[32m+[m[32m      boundariesElement = getOffsetParent(boundariesElement);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // NOTE: DOM access here[m
[32m+[m[32m    // resets the popper's position so that the document size can be calculated excluding[m
[32m+[m[32m    // the size of the popper element itself[m
[32m+[m[32m    var transformProp = getSupportedPropertyName('transform');[m
[32m+[m[32m    var popperStyles = data.instance.popper.style; // assignment to help minification[m
[32m+[m[32m    var top = popperStyles.top,[m
[32m+[m[32m        left = popperStyles.left,[m
[32m+[m[32m        transform = popperStyles[transformProp];[m
[32m+[m
[32m+[m[32m    popperStyles.top = '';[m
[32m+[m[32m    popperStyles.left = '';[m
[32m+[m[32m    popperStyles[transformProp] = '';[m
[32m+[m
[32m+[m[32m    var boundaries = getBoundaries(data.instance.popper, data.instance.reference, options.padding, boundariesElement, data.positionFixed);[m
[32m+[m
[32m+[m[32m    // NOTE: DOM access here[m
[32m+[m[32m    // restores the original style properties after the offsets have been computed[m
[32m+[m[32m    popperStyles.top = top;[m
[32m+[m[32m    popperStyles.left = left;[m
[32m+[m[32m    popperStyles[transformProp] = transform;[m
[32m+[m
[32m+[m[32m    options.boundaries = boundaries;[m
[32m+[m
[32m+[m[32m    var order = options.priority;[m
[32m+[m[32m    var popper = data.offsets.popper;[m
[32m+[m
[32m+[m[32m    var check = {[m
[32m+[m[32m      primary: function primary(placement) {[m
[32m+[m[32m        var value = popper[placement];[m
[32m+[m[32m        if (popper[placement] < boundaries[placement] && !options.escapeWithReference) {[m
[32m+[m[32m          value = Math.max(popper[placement], boundaries[placement]);[m
[32m+[m[32m        }[m
[32m+[m[32m        return defineProperty({}, placement, value);[m
[32m+[m[32m      },[m
[32m+[m[32m      secondary: function secondary(placement) {[m
[32m+[m[32m        var mainSide = placement === 'right' ? 'left' : 'top';[m
[32m+[m[32m        var value = popper[mainSide];[m
[32m+[m[32m        if (popper[placement] > boundaries[placement] && !options.escapeWithReference) {[m
[32m+[m[32m          value = Math.min(popper[mainSide], boundaries[placement] - (placement === 'right' ? popper.width : popper.height));[m
[32m+[m[32m        }[m
[32m+[m[32m        return defineProperty({}, mainSide, value);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    order.forEach(function (placement) {[m
[32m+[m[32m      var side = ['left', 'top'].indexOf(placement) !== -1 ? 'primary' : 'secondary';[m
[32m+[m[32m      popper = _extends({}, popper, check[side](placement));[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    data.offsets.popper = popper;[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function shift(data) {[m
[32m+[m[32m    var placement = data.placement;[m
[32m+[m[32m    var basePlacement = placement.split('-')[0];[m
[32m+[m[32m    var shiftvariation = placement.split('-')[1];[m
[32m+[m
[32m+[m[32m    // if shift shiftvariation is specified, run the modifier[m
[32m+[m[32m    if (shiftvariation) {[m
[32m+[m[32m      var _data$offsets = data.offsets,[m
[32m+[m[32m          reference = _data$offsets.reference,[m
[32m+[m[32m          popper = _data$offsets.popper;[m
[32m+[m
[32m+[m[32m      var isVertical = ['bottom', 'top'].indexOf(basePlacement) !== -1;[m
[32m+[m[32m      var side = isVertical ? 'left' : 'top';[m
[32m+[m[32m      var measurement = isVertical ? 'width' : 'height';[m
[32m+[m
[32m+[m[32m      var shiftOffsets = {[m
[32m+[m[32m        start: defineProperty({}, side, reference[side]),[m
[32m+[m[32m        end: defineProperty({}, side, reference[side] + reference[measurement] - popper[measurement])[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      data.offsets.popper = _extends({}, popper, shiftOffsets[shiftvariation]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by update method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function hide(data) {[m
[32m+[m[32m    if (!isModifierRequired(data.instance.modifiers, 'hide', 'preventOverflow')) {[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var refRect = data.offsets.reference;[m
[32m+[m[32m    var bound = find(data.instance.modifiers, function (modifier) {[m
[32m+[m[32m      return modifier.name === 'preventOverflow';[m
[32m+[m[32m    }).boundaries;[m
[32m+[m
[32m+[m[32m    if (refRect.bottom < bound.top || refRect.left > bound.right || refRect.top > bound.bottom || refRect.right < bound.left) {[m
[32m+[m[32m      // Avoid unnecessary DOM access if visibility hasn't changed[m
[32m+[m[32m      if (data.hide === true) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      data.hide = true;[m
[32m+[m[32m      data.attributes['x-out-of-boundaries'] = '';[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Avoid unnecessary DOM access if visibility hasn't changed[m
[32m+[m[32m      if (data.hide === false) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      data.hide = false;[m
[32m+[m[32m      data.attributes['x-out-of-boundaries'] = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @function[m
[32m+[m[32m   * @memberof Modifiers[m
[32m+[m[32m   * @argument {Object} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {Object} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m[32m  function inner(data) {[m
[32m+[m[32m    var placement = data.placement;[m
[32m+[m[32m    var basePlacement = placement.split('-')[0];[m
[32m+[m[32m    var _data$offsets = data.offsets,[m
[32m+[m[32m        popper = _data$offsets.popper,[m
[32m+[m[32m        reference = _data$offsets.reference;[m
[32m+[m
[32m+[m[32m    var isHoriz = ['left', 'right'].indexOf(basePlacement) !== -1;[m
[32m+[m
[32m+[m[32m    var subtractLength = ['top', 'left'].indexOf(basePlacement) === -1;[m
[32m+[m
[32m+[m[32m    popper[isHoriz ? 'left' : 'top'] = reference[basePlacement] - (subtractLength ? popper[isHoriz ? 'width' : 'height'] : 0);[m
[32m+[m
[32m+[m[32m    data.placement = getOppositePlacement(placement);[m
[32m+[m[32m    data.offsets.popper = getClientRect(popper);[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Modifier function, each modifier can have a function of this type assigned[m
[32m+[m[32m   * to its `fn` property.<br />[m
[32m+[m[32m   * These functions will be called on each update, this means that you must[m
[32m+[m[32m   * make sure they are performant enough to avoid performance bottlenecks.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @function ModifierFn[m
[32m+[m[32m   * @argument {dataObject} data - The data object generated by `update` method[m
[32m+[m[32m   * @argument {Object} options - Modifiers configuration and options[m
[32m+[m[32m   * @returns {dataObject} The data object, properly modified[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Modifiers are plugins used to alter the behavior of your poppers.<br />[m
[32m+[m[32m   * Popper.js uses a set of 9 modifiers to provide all the basic functionalities[m
[32m+[m[32m   * needed by the library.[m
[32m+[m[32m   *[m
[32m+[m[32m   * Usually you don't want to override the `order`, `fn` and `onLoad` props.[m
[32m+[m[32m   * All the other properties are configurations that could be tweaked.[m
[32m+[m[32m   * @namespace modifiers[m
[32m+[m[32m   */[m
[32m+[m[32m  var modifiers = {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to shift the popper on the start or end of its reference[m
[32m+[m[32m     * element.<br />[m
[32m+[m[32m     * It will read the variation of the `placement` property.<br />[m
[32m+[m[32m     * It can be one either `-end` or `-start`.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    shift: {[m
[32m+[m[32m      /** @prop {number} order=100 - Index used to define the order of execution */[m
[32m+[m[32m      order: 100,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: shift[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The `offset` modifier can shift your popper on both its axis.[m
[32m+[m[32m     *[m
[32m+[m[32m     * It accepts the following units:[m
[32m+[m[32m     * - `px` or unitless, interpreted as pixels[m
[32m+[m[32m     * - `%` or `%r`, percentage relative to the length of the reference element[m
[32m+[m[32m     * - `%p`, percentage relative to the length of the popper element[m
[32m+[m[32m     * - `vw`, CSS viewport width unit[m
[32m+[m[32m     * - `vh`, CSS viewport height unit[m
[32m+[m[32m     *[m
[32m+[m[32m     * For length is intended the main axis relative to the placement of the popper.<br />[m
[32m+[m[32m     * This means that if the placement is `top` or `bottom`, the length will be the[m
[32m+[m[32m     * `width`. In case of `left` or `right`, it will be the height.[m
[32m+[m[32m     *[m
[32m+[m[32m     * You can provide a single value (as `Number` or `String`), or a pair of values[m
[32m+[m[32m     * as `String` divided by a comma or one (or more) white spaces.<br />[m
[32m+[m[32m     * The latter is a deprecated method because it leads to confusion and will be[m
[32m+[m[32m     * removed in v2.<br />[m
[32m+[m[32m     * Additionally, it accepts additions and subtractions between different units.[m
[32m+[m[32m     * Note that multiplications and divisions aren't supported.[m
[32m+[m[32m     *[m
[32m+[m[32m     * Valid examples are:[m
[32m+[m[32m     * ```[m
[32m+[m[32m     * 10[m
[32m+[m[32m     * '10%'[m
[32m+[m[32m     * '10, 10'[m
[32m+[m[32m     * '10%, 10'[m
[32m+[m[32m     * '10 + 10%'[m
[32m+[m[32m     * '10 - 5vh + 3%'[m
[32m+[m[32m     * '-10px + 5vh, 5px - 6%'[m
[32m+[m[32m     * ```[m
[32m+[m[32m     * > **NB**: If you desire to apply offsets to your poppers in a way that may make them overlap[m
[32m+[m[32m     * > with their reference element, unfortunately, you will have to disable the `flip` modifier.[m
[32m+[m[32m     * > More on this [reading this issue](https://github.com/FezVrasta/popper.js/issues/373)[m
[32m+[m[32m     *[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    offset: {[m
[32m+[m[32m      /** @prop {number} order=200 - Index used to define the order of execution */[m
[32m+[m[32m      order: 200,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: offset,[m
[32m+[m[32m      /** @prop {Number|String} offset=0[m
[32m+[m[32m       * The offset value as described in the modifier description[m
[32m+[m[32m       */[m
[32m+[m[32m      offset: 0[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to prevent the popper from being positioned outside the boundary.[m
[32m+[m[32m     *[m
[32m+[m[32m     * An scenario exists where the reference itself is not within the boundaries.<br />[m
[32m+[m[32m     * We can say it has "escaped the boundaries" ‚Äî or just "escaped".<br />[m
[32m+[m[32m     * In this case we need to decide whether the popper should either:[m
[32m+[m[32m     *[m
[32m+[m[32m     * - detach from the reference and remain "trapped" in the boundaries, or[m
[32m+[m[32m     * - if it should ignore the boundary and "escape with its reference"[m
[32m+[m[32m     *[m
[32m+[m[32m     * When `escapeWithReference` is set to`true` and reference is completely[m
[32m+[m[32m     * outside its boundaries, the popper will overflow (or completely leave)[m
[32m+[m[32m     * the boundaries in order to remain attached to the edge of the reference.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    preventOverflow: {[m
[32m+[m[32m      /** @prop {number} order=300 - Index used to define the order of execution */[m
[32m+[m[32m      order: 300,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: preventOverflow,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {Array} [priority=['left','right','top','bottom']][m
[32m+[m[32m       * Popper will try to prevent overflow following these priorities by default,[m
[32m+[m[32m       * then, it could overflow on the left and on top of the `boundariesElement`[m
[32m+[m[32m       */[m
[32m+[m[32m      priority: ['left', 'right', 'top', 'bottom'],[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {number} padding=5[m
[32m+[m[32m       * Amount of pixel used to define a minimum distance between the boundaries[m
[32m+[m[32m       * and the popper this makes sure the popper has always a little padding[m
[32m+[m[32m       * between the edges of its container[m
[32m+[m[32m       */[m
[32m+[m[32m      padding: 5,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {String|HTMLElement} boundariesElement='scrollParent'[m
[32m+[m[32m       * Boundaries used by the modifier, can be `scrollParent`, `window`,[m
[32m+[m[32m       * `viewport` or any DOM element.[m
[32m+[m[32m       */[m
[32m+[m[32m      boundariesElement: 'scrollParent'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to make sure the reference and its popper stay near eachothers[m
[32m+[m[32m     * without leaving any gap between the two. Expecially useful when the arrow is[m
[32m+[m[32m     * enabled and you want to assure it to point to its reference element.[m
[32m+[m[32m     * It cares only about the first axis, you can still have poppers with margin[m
[32m+[m[32m     * between the popper and its reference element.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    keepTogether: {[m
[32m+[m[32m      /** @prop {number} order=400 - Index used to define the order of execution */[m
[32m+[m[32m      order: 400,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: keepTogether[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * This modifier is used to move the `arrowElement` of the popper to make[m
[32m+[m[32m     * sure it is positioned between the reference element and its popper element.[m
[32m+[m[32m     * It will read the outer size of the `arrowElement` node to detect how many[m
[32m+[m[32m     * pixels of conjuction are needed.[m
[32m+[m[32m     *[m
[32m+[m[32m     * It has no effect if no `arrowElement` is provided.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    arrow: {[m
[32m+[m[32m      /** @prop {number} order=500 - Index used to define the order of execution */[m
[32m+[m[32m      order: 500,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: arrow,[m
[32m+[m[32m      /** @prop {String|HTMLElement} element='[x-arrow]' - Selector or node used as arrow */[m
[32m+[m[32m      element: '[x-arrow]'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to flip the popper's placement when it starts to overlap its[m
[32m+[m[32m     * reference element.[m
[32m+[m[32m     *[m
[32m+[m[32m     * Requires the `preventOverflow` modifier before it in order to work.[m
[32m+[m[32m     *[m
[32m+[m[32m     * **NOTE:** this modifier will interrupt the current update cycle and will[m
[32m+[m[32m     * restart it if it detects the need to flip the placement.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    flip: {[m
[32m+[m[32m      /** @prop {number} order=600 - Index used to define the order of execution */[m
[32m+[m[32m      order: 600,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: flip,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {String|Array} behavior='flip'[m
[32m+[m[32m       * The behavior used to change the popper's placement. It can be one of[m
[32m+[m[32m       * `flip`, `clockwise`, `counterclockwise` or an array with a list of valid[m
[32m+[m[32m       * placements (with optional variations).[m
[32m+[m[32m       */[m
[32m+[m[32m      behavior: 'flip',[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {number} padding=5[m
[32m+[m[32m       * The popper will flip if it hits the edges of the `boundariesElement`[m
[32m+[m[32m       */[m
[32m+[m[32m      padding: 5,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {String|HTMLElement} boundariesElement='viewport'[m
[32m+[m[32m       * The element which will define the boundaries of the popper position,[m
[32m+[m[32m       * the popper will never be placed outside of the defined boundaries[m
[32m+[m[32m       * (except if keepTogether is enabled)[m
[32m+[m[32m       */[m
[32m+[m[32m      boundariesElement: 'viewport'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to make the popper flow toward the inner of the reference element.[m
[32m+[m[32m     * By default, when this modifier is disabled, the popper will be placed outside[m
[32m+[m[32m     * the reference element.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    inner: {[m
[32m+[m[32m      /** @prop {number} order=700 - Index used to define the order of execution */[m
[32m+[m[32m      order: 700,[m
[32m+[m[32m      /** @prop {Boolean} enabled=false - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: false,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: inner[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Modifier used to hide the popper when its reference element is outside of the[m
[32m+[m[32m     * popper boundaries. It will set a `x-out-of-boundaries` attribute which can[m
[32m+[m[32m     * be used to hide with a CSS selector the popper when its reference is[m
[32m+[m[32m     * out of boundaries.[m
[32m+[m[32m     *[m
[32m+[m[32m     * Requires the `preventOverflow` modifier before it in order to work.[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    hide: {[m
[32m+[m[32m      /** @prop {number} order=800 - Index used to define the order of execution */[m
[32m+[m[32m      order: 800,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: hide[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Computes the style that will be applied to the popper element to gets[m
[32m+[m[32m     * properly positioned.[m
[32m+[m[32m     *[m
[32m+[m[32m     * Note that this modifier will not touch the DOM, it just prepares the styles[m
[32m+[m[32m     * so that `applyStyle` modifier can apply it. This separation is useful[m
[32m+[m[32m     * in case you need to replace `applyStyle` with a custom implementation.[m
[32m+[m[32m     *[m
[32m+[m[32m     * This modifier has `850` as `order` value to maintain backward compatibility[m
[32m+[m[32m     * with previous versions of Popper.js. Expect the modifiers ordering method[m
[32m+[m[32m     * to change in future major versions of the library.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    computeStyle: {[m
[32m+[m[32m      /** @prop {number} order=850 - Index used to define the order of execution */[m
[32m+[m[32m      order: 850,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: computeStyle,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {Boolean} gpuAcceleration=true[m
[32m+[m[32m       * If true, it uses the CSS 3d transformation to position the popper.[m
[32m+[m[32m       * Otherwise, it will use the `top` and `left` properties.[m
[32m+[m[32m       */[m
[32m+[m[32m      gpuAcceleration: true,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {string} [x='bottom'][m
[32m+[m[32m       * Where to anchor the X axis (`bottom` or `top`). AKA X offset origin.[m
[32m+[m[32m       * Change this if your popper should grow in a direction different from `bottom`[m
[32m+[m[32m       */[m
[32m+[m[32m      x: 'bottom',[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @prop {string} [x='left'][m
[32m+[m[32m       * Where to anchor the Y axis (`left` or `right`). AKA Y offset origin.[m
[32m+[m[32m       * Change this if your popper should grow in a direction different from `right`[m
[32m+[m[32m       */[m
[32m+[m[32m      y: 'right'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Applies the computed styles to the popper element.[m
[32m+[m[32m     *[m
[32m+[m[32m     * All the DOM manipulations are limited to this modifier. This is useful in case[m
[32m+[m[32m     * you want to integrate Popper.js inside a framework or view library and you[m
[32m+[m[32m     * want to delegate all the DOM manipulations to it.[m
[32m+[m[32m     *[m
[32m+[m[32m     * Note that if you disable this modifier, you must make sure the popper element[m
[32m+[m[32m     * has its position set to `absolute` before Popper.js can do its work![m
[32m+[m[32m     *[m
[32m+[m[32m     * Just disable this modifier and define you own to achieve the desired effect.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @memberof modifiers[m
[32m+[m[32m     * @inner[m
[32m+[m[32m     */[m
[32m+[m[32m    applyStyle: {[m
[32m+[m[32m      /** @prop {number} order=900 - Index used to define the order of execution */[m
[32m+[m[32m      order: 900,[m
[32m+[m[32m      /** @prop {Boolean} enabled=true - Whether the modifier is enabled or not */[m
[32m+[m[32m      enabled: true,[m
[32m+[m[32m      /** @prop {ModifierFn} */[m
[32m+[m[32m      fn: applyStyle,[m
[32m+[m[32m      /** @prop {Function} */[m
[32m+[m[32m      onLoad: applyStyleOnLoad,[m
[32m+[m[32m      /**[m
[32m+[m[32m       * @deprecated since version 1.10.0, the property moved to `computeStyle` modifier[m
[32m+[m[32m       * @prop {Boolean} gpuAcceleration=true[m
[32m+[m[32m       * If true, it uses the CSS 3d transformation to position the popper.[m
[32m+[m[32m       * Otherwise, it will use the `top` and `left` properties.[m
[32m+[m[32m       */[m
[32m+[m[32m      gpuAcceleration: undefined[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * The `dataObject` is an object containing all the informations used by Popper.js[m
[32m+[m[32m   * this object get passed to modifiers and to the `onCreate` and `onUpdate` callbacks.[m
[32m+[m[32m   * @name dataObject[m
[32m+[m[32m   * @property {Object} data.instance The Popper.js instance[m
[32m+[m[32m   * @property {String} data.placement Placement applied to popper[m
[32m+[m[32m   * @property {String} data.originalPlacement Placement originally defined on init[m
[32m+[m[32m   * @property {Boolean} data.flipped True if popper has been flipped by flip modifier[m
[32m+[m[32m   * @property {Boolean} data.hide True if the reference element is out of boundaries, useful to know when to hide the popper.[m
[32m+[m[32m   * @property {HTMLElement} data.arrowElement Node used as arrow by arrow modifier[m
[32m+[m[32m   * @property {Object} data.styles Any CSS property defined here will be applied to the popper, it expects the JavaScript nomenclature (eg. `marginBottom`)[m
[32m+[m[32m   * @property {Object} data.arrowStyles Any CSS property defined here will be applied to the popper arrow, it expects the JavaScript nomenclature (eg. `marginBottom`)[m
[32m+[m[32m   * @property {Object} data.boundaries Offsets of the popper boundaries[m
[32m+[m[32m   * @property {Object} data.offsets The measurements of popper, reference and arrow elements.[m
[32m+[m[32m   * @property {Object} data.offsets.popper `top`, `left`, `width`, `height` values[m
[32m+[m[32m   * @property {Object} data.offsets.reference `top`, `left`, `width`, `height` values[m
[32m+[m[32m   * @property {Object} data.offsets.arrow] `top` and `left` offsets, only one of them will be different from 0[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Default options provided to Popper.js constructor.<br />[m
[32m+[m[32m   * These can be overriden using the `options` argument of Popper.js.<br />[m
[32m+[m[32m   * To override an option, simply pass as 3rd argument an object with the same[m
[32m+[m[32m   * structure of this object, example:[m
[32m+[m[32m   * ```[m
[32m+[m[32m   * new Popper(ref, pop, {[m
[32m+[m[32m   *   modifiers: {[m
[32m+[m[32m   *     preventOverflow: { enabled: false }[m
[32m+[m[32m   *   }[m
[32m+[m[32m   * })[m
[32m+[m[32m   * ```[m
[32m+[m[32m   * @type {Object}[m
[32m+[m[32m   * @static[m
[32m+[m[32m   * @memberof Popper[m
[32m+[m[32m   */[m
[32m+[m[32m  var Defaults = {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Popper's placement[m
[32m+[m[32m     * @prop {Popper.placements} placement='bottom'[m
[32m+[m[32m     */[m
[32m+[m[32m    placement: 'bottom',[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Set this to true if you want popper to position it self in 'fixed' mode[m
[32m+[m[32m     * @prop {Boolean} positionFixed=false[m
[32m+[m[32m     */[m
[32m+[m[32m    positionFixed: false,[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Whether events (resize, scroll) are initially enabled[m
[32m+[m[32m     * @prop {Boolean} eventsEnabled=true[m
[32m+[m[32m     */[m
[32m+[m[32m    eventsEnabled: true,[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Set to true if you want to automatically remove the popper when[m
[32m+[m[32m     * you call the `destroy` method.[m
[32m+[m[32m     * @prop {Boolean} removeOnDestroy=false[m
[32m+[m[32m     */[m
[32m+[m[32m    removeOnDestroy: false,[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Callback called when the popper is created.<br />[m
[32m+[m[32m     * By default, is set to no-op.<br />[m
[32m+[m[32m     * Access Popper.js instance with `data.instance`.[m
[32m+[m[32m     * @prop {onCreate}[m
[32m+[m[32m     */[m
[32m+[m[32m    onCreate: function onCreate() {},[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Callback called when the popper is updated, this callback is not called[m
[32m+[m[32m     * on the initialization/creation of the popper, but only on subsequent[m
[32m+[m[32m     * updates.<br />[m
[32m+[m[32m     * By default, is set to no-op.<br />[m
[32m+[m[32m     * Access Popper.js instance with `data.instance`.[m
[32m+[m[32m     * @prop {onUpdate}[m
[32m+[m[32m     */[m
[32m+[m[32m    onUpdate: function onUpdate() {},[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * List of modifiers used to modify the offsets before they are applied to the popper.[m
[32m+[m[32m     * They provide most of the functionalities of Popper.js[m
[32m+[m[32m     * @prop {modifiers}[m
[32m+[m[32m     */[m
[32m+[m[32m    modifiers: modifiers[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @callback onCreate[m
[32m+[m[32m   * @param {dataObject} data[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @callback onUpdate[m
[32m+[m[32m   * @param {dataObject} data[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  // Utils[m
[32m+[m[32m  // Methods[m
[32m+[m[32m  var Popper = function () {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Create a new Popper.js instance[m
[32m+[m[32m     * @class Popper[m
[32m+[m[32m     * @param {HTMLElement|referenceObject} reference - The reference element used to position the popper[m
[32m+[m[32m     * @param {HTMLElement} popper - The HTML element used as popper.[m
[32m+[m[32m     * @param {Object} options - Your custom options to override the ones defined in [Defaults](#defaults)[m
[32m+[m[32m     * @return {Object} instance - The generated Popper.js instance[m
[32m+[m[32m     */[m
[32m+[m[32m    function Popper(reference, popper) {[m
[32m+[m[32m      var _this = this;[m
[32m+[m
[32m+[m[32m      var options = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : {};[m
[32m+[m[32m      classCallCheck(this, Popper);[m
[32m+[m
[32m+[m[32m      this.scheduleUpdate = function () {[m
[32m+[m[32m        return requestAnimationFrame(_this.update);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      // make update() debounced, so that it only runs at most once-per-tick[m
[32m+[m[32m      this.update = debounce(this.update.bind(this));[m
[32m+[m
[32m+[m[32m      // with {} we create a new object with the options inside it[m
[32m+[m[32m      this.options = _extends({}, Popper.Defaults, options);[m
[32m+[m
[32m+[m[32m      // init state[m
[32m+[m[32m      this.state = {[m
[32m+[m[32m        isDestroyed: false,[m
[32m+[m[32m        isCreated: false,[m
[32m+[m[32m        scrollParents: [][m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      // get reference and popper elements (allow jQuery wrappers)[m
[32m+[m[32m      this.reference = reference && reference.jquery ? reference[0] : reference;[m
[32m+[m[32m      this.popper = popper && popper.jquery ? popper[0] : popper;[m
[32m+[m
[32m+[m[32m      // Deep merge modifiers options[m
[32m+[m[32m      this.options.modifiers = {};[m
[32m+[m[32m      Object.keys(_extends({}, Popper.Defaults.modifiers, options.modifiers)).forEach(function (name) {[m
[32m+[m[32m        _this.options.modifiers[name] = _extends({}, Popper.Defaults.modifiers[name] || {}, options.modifiers ? options.modifiers[name] : {});[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // Refactoring modifiers' list (Object => Array)[m
[32m+[m[32m      this.modifiers = Object.keys(this.options.modifiers).map(function (name) {[m
[32m+[m[32m        return _extends({[m
[32m+[m[32m          name: name[m
[32m+[m[32m        }, _this.options.modifiers[name]);[m
[32m+[m[32m      })[m
[32m+[m[32m      // sort the modifiers by order[m
[32m+[m[32m      .sort(function (a, b) {[m
[32m+[m[32m        return a.order - b.order;[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // modifiers have the ability to execute arbitrary code when Popper.js get inited[m
[32m+[m[32m      // such code is executed in the same order of its modifier[m
[32m+[m[32m      // they could add new properties to their options configuration[m
[32m+[m[32m      // BE AWARE: don't add options to `options.modifiers.name` but to `modifierOptions`![m
[32m+[m[32m      this.modifiers.forEach(function (modifierOptions) {[m
[32m+[m[32m        if (modifierOptions.enabled && isFunction(modifierOptions.onLoad)) {[m
[32m+[m[32m          modifierOptions.onLoad(_this.reference, _this.popper, _this.options, modifierOptions, _this.state);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // fire the first update to position the popper in the right place[m
[32m+[m[32m      this.update();[m
[32m+[m
[32m+[m[32m      var eventsEnabled = this.options.eventsEnabled;[m
[32m+[m[32m      if (eventsEnabled) {[m
[32m+[m[32m        // setup event listeners, they will take care of update the position in specific situations[m
[32m+[m[32m        this.enableEventListeners();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.state.eventsEnabled = eventsEnabled;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // We can't use class properties because they don't get listed in the[m
[32m+[m[32m    // class prototype and break stuff like Sinon stubs[m
[32m+[m
[32m+[m
[32m+[m[32m    createClass(Popper, [{[m
[32m+[m[32m      key: 'update',[m
[32m+[m[32m      value: function update$$1() {[m
[32m+[m[32m        return update.call(this);[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: 'destroy',[m
[32m+[m[32m      value: function destroy$$1() {[m
[32m+[m[32m        return destroy.call(this);[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: 'enableEventListeners',[m
[32m+[m[32m      value: function enableEventListeners$$1() {[m
[32m+[m[32m        return enableEventListeners.call(this);[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: 'disableEventListeners',[m
[32m+[m[32m      value: function disableEventListeners$$1() {[m
[32m+[m[32m        return disableEventListeners.call(this);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      /**[m
[32m+[m[32m       * Schedule an update, it will run on the next UI update available[m
[32m+[m[32m       * @method scheduleUpdate[m
[32m+[m[32m       * @memberof Popper[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m
[32m+[m[32m      /**[m
[32m+[m[32m       * Collection of utilities useful when writing custom modifiers.[m
[32m+[m[32m       * Starting from version 1.7, this method is available only if you[m
[32m+[m[32m       * include `popper-utils.js` before `popper.js`.[m
[32m+[m[32m       *[m
[32m+[m[32m       * **DEPRECATION**: This way to access PopperUtils is deprecated[m
[32m+[m[32m       * and will be removed in v2! Use the PopperUtils module directly instead.[m
[32m+[m[32m       * Due to the high instability of the methods contained in Utils, we can't[m
[32m+[m[32m       * guarantee them to follow semver. Use them at your own risk![m
[32m+[m[32m       * @static[m
[32m+[m[32m       * @private[m
[32m+[m[32m       * @type {Object}[m
[32m+[m[32m       * @deprecated since version 1.8[m
[32m+[m[32m       * @member Utils[m
[32m+[m[32m       * @memberof Popper[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    }]);[m
[32m+[m[32m    return Popper;[m
[32m+[m[32m  }();[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * The `referenceObject` is an object that provides an interface compatible with Popper.js[m
[32m+[m[32m   * and lets you use it as replacement of a real DOM node.<br />[m
[32m+[m[32m   * You can use this method to position a popper relatively to a set of coordinates[m
[32m+[m[32m   * in case you don't have a DOM node to use as reference.[m
[32m+[m[32m   *[m
[32m+[m[32m   * ```[m
[32m+[m[32m   * new Popper(referenceObject, popperNode);[m
[32m+[m[32m   * ```[m
[32m+[m[32m   *[m
[32m+[m[32m   * NB: This feature isn't supported in Internet Explorer 10[m
[32m+[m[32m   * @name referenceObject[m
[32m+[m[32m   * @property {Function} data.getBoundingClientRect[m
[32m+[m[32m   * A function that returns a set of coordinates compatible with the native `getBoundingClientRect` method.[m
[32m+[m[32m   * @property {number} data.clientWidth[m
[32m+[m[32m   * An ES6 getter that will return the width of the virtual reference element.[m
[32m+[m[32m   * @property {number} data.clientHeight[m
[32m+[m[32m   * An ES6 getter that will return the height of the virtual reference element.[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m
[32m+[m[32m  Popper.Utils = (typeof window !== 'undefined' ? window : global).PopperUtils;[m
[32m+[m[32m  Popper.placements = placements;[m
[32m+[m[32m  Popper.Defaults = Defaults;[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   * Bootstrap (v4.1.3): dropdown.js[m
[32m+[m[32m   * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var Dropdown = function ($$$1) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Constants[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m[32m    var NAME = 'dropdown';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.dropdown';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var DATA_API_KEY = '.data-api';[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var ESCAPE_KEYCODE = 27; // KeyboardEvent.which value for Escape (Esc) key[m
[32m+[m
[32m+[m[32m    var SPACE_KEYCODE = 32; // KeyboardEvent.which value for space key[m
[32m+[m
[32m+[m[32m    var TAB_KEYCODE = 9; // KeyboardEvent.which value for tab key[m
[32m+[m
[32m+[m[32m    var ARROW_UP_KEYCODE = 38; // KeyboardEvent.which value for up arrow key[m
[32m+[m
[32m+[m[32m    var ARROW_DOWN_KEYCODE = 40; // KeyboardEvent.which value for down arrow key[m
[32m+[m
[32m+[m[32m    var RIGHT_MOUSE_BUTTON_WHICH = 3; // MouseEvent.which value for the right button (assuming a right-handed mouse)[m
[32m+[m
[32m+[m[32m    var REGEXP_KEYDOWN = new RegExp(ARROW_UP_KEYCODE + "|" + ARROW_DOWN_KEYCODE + "|" + ESCAPE_KEYCODE);[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      HIDE: "hide" + EVENT_KEY,[m
[32m+[m[32m      HIDDEN: "hidden" + EVENT_KEY,[m
[32m+[m[32m      SHOW: "show" + EVENT_KEY,[m
[32m+[m[32m      SHOWN: "shown" + EVENT_KEY,[m
[32m+[m[32m      CLICK: "click" + EVENT_KEY,[m
[32m+[m[32m      CLICK_DATA_API: "click" + EVENT_KEY + DATA_API_KEY,[m
[32m+[m[32m      KEYDOWN_DATA_API: "keydown" + EVENT_KEY + DATA_API_KEY,[m
[32m+[m[32m      KEYUP_DATA_API: "keyup" + EVENT_KEY + DATA_API_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      DISABLED: 'disabled',[m
[32m+[m[32m      SHOW: 'show',[m
[32m+[m[32m      DROPUP: 'dropup',[m
[32m+[m[32m      DROPRIGHT: 'dropright',[m
[32m+[m[32m      DROPLEFT: 'dropleft',[m
[32m+[m[32m      MENURIGHT: 'dropdown-menu-right',[m
[32m+[m[32m      MENULEFT: 'dropdown-menu-left',[m
[32m+[m[32m      POSITION_STATIC: 'position-static'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      DATA_TOGGLE: '[data-toggle="dropdown"]',[m
[32m+[m[32m      FORM_CHILD: '.dropdown form',[m
[32m+[m[32m      MENU: '.dropdown-menu',[m
[32m+[m[32m      NAVBAR_NAV: '.navbar-nav',[m
[32m+[m[32m      VISIBLE_ITEMS: '.dropdown-menu .dropdown-item:not(.disabled):not(:disabled)'[m
[32m+[m[32m    };[m
[32m+[m[32m    var AttachmentMap = {[m
[32m+[m[32m      TOP: 'top-start',[m
[32m+[m[32m      TOPEND: 'top-end',[m
[32m+[m[32m      BOTTOM: 'bottom-start',[m
[32m+[m[32m      BOTTOMEND: 'bottom-end',[m
[32m+[m[32m      RIGHT: 'right-start',[m
[32m+[m[32m      RIGHTEND: 'right-end',[m
[32m+[m[32m      LEFT: 'left-start',[m
[32m+[m[32m      LEFTEND: 'left-end'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      offset: 0,[m
[32m+[m[32m      flip: true,[m
[32m+[m[32m      boundary: 'scrollParent',[m
[32m+[m[32m      reference: 'toggle',[m
[32m+[m[32m      display: 'dynamic'[m
[32m+[m[32m    };[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      offset: '(number|string|function)',[m
[32m+[m[32m      flip: 'boolean',[m
[32m+[m[32m      boundary: '(string|element)',[m
[32m+[m[32m      reference: '(string|element)',[m
[32m+[m[32m      display: 'string'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var Dropdown =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function Dropdown(element, config) {[m
[32m+[m[32m        this._element = element;[m
[32m+[m[32m        this._popper = null;[m
[32m+[m[32m        this._config = this._getConfig(config);[m
[32m+[m[32m        this._menu = this._getMenuElement();[m
[32m+[m[32m        this._inNavbar = this._detectNavbar();[m
[32m+[m
[32m+[m[32m        this._addEventListeners();[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = Dropdown.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.toggle = function toggle() {[m
[32m+[m[32m        if (this._element.disabled || $$$1(this._element).hasClass(ClassName.DISABLED)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var parent = Dropdown._getParentFromElement(this._element);[m
[32m+[m
[32m+[m[32m        var isActive = $$$1(this._menu).hasClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m        Dropdown._clearMenus();[m
[32m+[m
[32m+[m[32m        if (isActive) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var relatedTarget = {[m
[32m+[m[32m          relatedTarget: this._element[m
[32m+[m[32m        };[m
[32m+[m[32m        var showEvent = $$$1.Event(Event.SHOW, relatedTarget);[m
[32m+[m[32m        $$$1(parent).trigger(showEvent);[m
[32m+[m
[32m+[m[32m        if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        } // Disable totally Popper.js for Dropdown in Navbar[m
[32m+[m
[32m+[m
[32m+[m[32m        if (!this._inNavbar) {[m
[32m+[m[32m          /**[m
[32m+[m[32m           * Check for Popper dependency[m
[32m+[m[32m           * Popper - https://popper.js.org[m
[32m+[m[32m           */[m
[32m+[m[32m          if (typeof Popper === 'undefined') {[m
[32m+[m[32m            throw new TypeError('Bootstrap dropdown require Popper.js (https://popper.js.org)');[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var referenceElement = this._element;[m
[32m+[m
[32m+[m[32m          if (this._config.reference === 'parent') {[m
[32m+[m[32m            referenceElement = parent;[m
[32m+[m[32m          } else if (Util.isElement(this._config.reference)) {[m
[32m+[m[32m            referenceElement = this._config.reference; // Check if it's jQuery element[m
[32m+[m
[32m+[m[32m            if (typeof this._config.reference.jquery !== 'undefined') {[m
[32m+[m[32m              referenceElement = this._config.reference[0];[m
[32m+[m[32m            }[m
[32m+[m[32m          } // If boundary is not `scrollParent`, then set position to `static`[m
[32m+[m[32m          // to allow the menu to "escape" the scroll parent's boundaries[m
[32m+[m[32m          // https://github.com/twbs/bootstrap/issues/24251[m
[32m+[m
[32m+[m
[32m+[m[32m          if (this._config.boundary !== 'scrollParent') {[m
[32m+[m[32m            $$$1(parent).addClass(ClassName.POSITION_STATIC);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          this._popper = new Popper(referenceElement, this._menu, this._getPopperConfig());[m
[32m+[m[32m        } // If this is a touch-enabled device we add extra[m
[32m+[m[32m        // empty mouseover listeners to the body's immediate children;[m
[32m+[m[32m        // only needed because of broken event delegation on iOS[m
[32m+[m[32m        // https://www.quirksmode.org/blog/archives/2014/02/mouse_event_bub.html[m
[32m+[m
[32m+[m
[32m+[m[32m        if ('ontouchstart' in document.documentElement && $$$1(parent).closest(Selector.NAVBAR_NAV).length === 0) {[m
[32m+[m[32m          $$$1(document.body).children().on('mouseover', null, $$$1.noop);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._element.focus();[m
[32m+[m
[32m+[m[32m        this._element.setAttribute('aria-expanded', true);[m
[32m+[m
[32m+[m[32m        $$$1(this._menu).toggleClass(ClassName.SHOW);[m
[32m+[m[32m        $$$1(parent).toggleClass(ClassName.SHOW).trigger($$$1.Event(Event.SHOWN, relatedTarget));[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        $$$1.removeData(this._element, DATA_KEY);[m
[32m+[m[32m        $$$1(this._element).off(EVENT_KEY);[m
[32m+[m[32m        this._element = null;[m
[32m+[m[32m        this._menu = null;[m
[32m+[m
[32m+[m[32m        if (this._popper !== null) {[m
[32m+[m[32m          this._popper.destroy();[m
[32m+[m
[32m+[m[32m          this._popper = null;[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.update = function update() {[m
[32m+[m[32m        this._inNavbar = this._detectNavbar();[m
[32m+[m
[32m+[m[32m        if (this._popper !== null) {[m
[32m+[m[32m          this._popper.scheduleUpdate();[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._addEventListeners = function _addEventListeners() {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        $$$1(this._element).on(Event.CLICK, function (event) {[m
[32m+[m[32m          event.preventDefault();[m
[32m+[m[32m          event.stopPropagation();[m
[32m+[m
[32m+[m[32m          _this.toggle();[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, this.constructor.Default, $$$1(this._element).data(), config);[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, this.constructor.DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getMenuElement = function _getMenuElement() {[m
[32m+[m[32m        if (!this._menu) {[m
[32m+[m[32m          var parent = Dropdown._getParentFromElement(this._element);[m
[32m+[m
[32m+[m[32m          if (parent) {[m
[32m+[m[32m            this._menu = parent.querySelector(Selector.MENU);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return this._menu;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getPlacement = function _getPlacement() {[m
[32m+[m[32m        var $parentDropdown = $$$1(this._element.parentNode);[m
[32m+[m[32m        var placement = AttachmentMap.BOTTOM; // Handle dropup[m
[32m+[m
[32m+[m[32m        if ($parentDropdown.hasClass(ClassName.DROPUP)) {[m
[32m+[m[32m          placement = AttachmentMap.TOP;[m
[32m+[m
[32m+[m[32m          if ($$$1(this._menu).hasClass(ClassName.MENURIGHT)) {[m
[32m+[m[32m            placement = AttachmentMap.TOPEND;[m
[32m+[m[32m          }[m
[32m+[m[32m        } else if ($parentDropdown.hasClass(ClassName.DROPRIGHT)) {[m
[32m+[m[32m          placement = AttachmentMap.RIGHT;[m
[32m+[m[32m        } else if ($parentDropdown.hasClass(ClassName.DROPLEFT)) {[m
[32m+[m[32m          placement = AttachmentMap.LEFT;[m
[32m+[m[32m        } else if ($$$1(this._menu).hasClass(ClassName.MENURIGHT)) {[m
[32m+[m[32m          placement = AttachmentMap.BOTTOMEND;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return placement;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._detectNavbar = function _detectNavbar() {[m
[32m+[m[32m        return $$$1(this._element).closest('.navbar').length > 0;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getPopperConfig = function _getPopperConfig() {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        var offsetConf = {};[m
[32m+[m
[32m+[m[32m        if (typeof this._config.offset === 'function') {[m
[32m+[m[32m          offsetConf.fn = function (data) {[m
[32m+[m[32m            data.offsets = _objectSpread({}, data.offsets, _this2._config.offset(data.offsets) || {});[m
[32m+[m[32m            return data;[m
[32m+[m[32m          };[m
[32m+[m[32m        } else {[m
[32m+[m[32m          offsetConf.offset = this._config.offset;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var popperConfig = {[m
[32m+[m[32m          placement: this._getPlacement(),[m
[32m+[m[32m          modifiers: {[m
[32m+[m[32m            offset: offsetConf,[m
[32m+[m[32m            flip: {[m
[32m+[m[32m              enabled: this._config.flip[m
[32m+[m[32m            },[m
[32m+[m[32m            preventOverflow: {[m
[32m+[m[32m              boundariesElement: this._config.boundary[m
[32m+[m[32m            }[m
[32m+[m[32m          } // Disable Popper.js if we have a static display[m
[32m+[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (this._config.display === 'static') {[m
[32m+[m[32m          popperConfig.modifiers.applyStyle = {[m
[32m+[m[32m            enabled: false[m
[32m+[m[32m          };[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return popperConfig;[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Dropdown._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = typeof config === 'object' ? config : null;[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Dropdown(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config]();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Dropdown._clearMenus = function _clearMenus(event) {[m
[32m+[m[32m        if (event && (event.which === RIGHT_MOUSE_BUTTON_WHICH || event.type === 'keyup' && event.which !== TAB_KEYCODE)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var toggles = [].slice.call(document.querySelectorAll(Selector.DATA_TOGGLE));[m
[32m+[m
[32m+[m[32m        for (var i = 0, len = toggles.length; i < len; i++) {[m
[32m+[m[32m          var parent = Dropdown._getParentFromElement(toggles[i]);[m
[32m+[m
[32m+[m[32m          var context = $$$1(toggles[i]).data(DATA_KEY);[m
[32m+[m[32m          var relatedTarget = {[m
[32m+[m[32m            relatedTarget: toggles[i][m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if (event && event.type === 'click') {[m
[32m+[m[32m            relatedTarget.clickEvent = event;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (!context) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var dropdownMenu = context._menu;[m
[32m+[m
[32m+[m[32m          if (!$$$1(parent).hasClass(ClassName.SHOW)) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (event && (event.type === 'click' && /input|textarea/i.test(event.target.tagName) || event.type === 'keyup' && event.which === TAB_KEYCODE) && $$$1.contains(parent, event.target)) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var hideEvent = $$$1.Event(Event.HIDE, relatedTarget);[m
[32m+[m[32m          $$$1(parent).trigger(hideEvent);[m
[32m+[m
[32m+[m[32m          if (hideEvent.isDefaultPrevented()) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          } // If this is a touch-enabled device we remove the extra[m
[32m+[m[32m          // empty mouseover listeners we added for iOS support[m
[32m+[m
[32m+[m
[32m+[m[32m          if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m            $$$1(document.body).children().off('mouseover', null, $$$1.noop);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          toggles[i].setAttribute('aria-expanded', 'false');[m
[32m+[m[32m          $$$1(dropdownMenu).removeClass(ClassName.SHOW);[m
[32m+[m[32m          $$$1(parent).removeClass(ClassName.SHOW).trigger($$$1.Event(Event.HIDDEN, relatedTarget));[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Dropdown._getParentFromElement = function _getParentFromElement(element) {[m
[32m+[m[32m        var parent;[m
[32m+[m[32m        var selector = Util.getSelectorFromElement(element);[m
[32m+[m
[32m+[m[32m        if (selector) {[m
[32m+[m[32m          parent = document.querySelector(selector);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return parent || element.parentNode;[m
[32m+[m[32m      }; // eslint-disable-next-line complexity[m
[32m+[m
[32m+[m
[32m+[m[32m      Dropdown._dataApiKeydownHandler = function _dataApiKeydownHandler(event) {[m
[32m+[m[32m        // If not input/textarea:[m
[32m+[m[32m        //  - And not a key in REGEXP_KEYDOWN => not a dropdown command[m
[32m+[m[32m        // If input/textarea:[m
[32m+[m[32m        //  - If space key => not a dropdown command[m
[32m+[m[32m        //  - If key is other than escape[m
[32m+[m[32m        //    - If key is not up or down => not a dropdown command[m
[32m+[m[32m        //    - If trigger inside the menu => not a dropdown command[m
[32m+[m[32m        if (/input|textarea/i.test(event.target.tagName) ? event.which === SPACE_KEYCODE || event.which !== ESCAPE_KEYCODE && (event.which !== ARROW_DOWN_KEYCODE && event.which !== ARROW_UP_KEYCODE || $$$1(event.target).closest(Selector.MENU).length) : !REGEXP_KEYDOWN.test(event.which)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        event.preventDefault();[m
[32m+[m[32m        event.stopPropagation();[m
[32m+[m
[32m+[m[32m        if (this.disabled || $$$1(this).hasClass(ClassName.DISABLED)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var parent = Dropdown._getParentFromElement(this);[m
[32m+[m
[32m+[m[32m        var isActive = $$$1(parent).hasClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m        if (!isActive && (event.which !== ESCAPE_KEYCODE || event.which !== SPACE_KEYCODE) || isActive && (event.which === ESCAPE_KEYCODE || event.which === SPACE_KEYCODE)) {[m
[32m+[m[32m          if (event.which === ESCAPE_KEYCODE) {[m
[32m+[m[32m            var toggle = parent.querySelector(Selector.DATA_TOGGLE);[m
[32m+[m[32m            $$$1(toggle).trigger('focus');[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this).trigger('click');[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var items = [].slice.call(parent.querySelectorAll(Selector.VISIBLE_ITEMS));[m
[32m+[m
[32m+[m[32m        if (items.length === 0) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var index = items.indexOf(event.target);[m
[32m+[m
[32m+[m[32m        if (event.which === ARROW_UP_KEYCODE && index > 0) {[m
[32m+[m[32m          // Up[m
[32m+[m[32m          index--;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event.which === ARROW_DOWN_KEYCODE && index < items.length - 1) {[m
[32m+[m[32m          // Down[m
[32m+[m[32m          index++;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (index < 0) {[m
[32m+[m[32m          index = 0;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        items[index].focus();[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Dropdown, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "DefaultType",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return DefaultType;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return Dropdown;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
