[32m+[m[32m    var BSCLS_PREFIX_REGEX = new RegExp("(^|\\s)" + CLASS_PREFIX + "\\S+", 'g');[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      animation: 'boolean',[m
[32m+[m[32m      template: 'string',[m
[32m+[m[32m      title: '(string|element|function)',[m
[32m+[m[32m      trigger: 'string',[m
[32m+[m[32m      delay: '(number|object)',[m
[32m+[m[32m      html: 'boolean',[m
[32m+[m[32m      selector: '(string|boolean)',[m
[32m+[m[32m      placement: '(string|function)',[m
[32m+[m[32m      offset: '(number|string)',[m
[32m+[m[32m      container: '(string|element|boolean)',[m
[32m+[m[32m      fallbackPlacement: '(string|array)',[m
[32m+[m[32m      boundary: '(string|element)'[m
[32m+[m[32m    };[m
[32m+[m[32m    var AttachmentMap = {[m
[32m+[m[32m      AUTO: 'auto',[m
[32m+[m[32m      TOP: 'top',[m
[32m+[m[32m      RIGHT: 'right',[m
[32m+[m[32m      BOTTOM: 'bottom',[m
[32m+[m[32m      LEFT: 'left'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      animation: true,[m
[32m+[m[32m      template: '<div class="tooltip" role="tooltip">' + '<div class="arrow"></div>' + '<div class="tooltip-inner"></div></div>',[m
[32m+[m[32m      trigger: 'hover focus',[m
[32m+[m[32m      title: '',[m
[32m+[m[32m      delay: 0,[m
[32m+[m[32m      html: false,[m
[32m+[m[32m      selector: false,[m
[32m+[m[32m      placement: 'top',[m
[32m+[m[32m      offset: 0,[m
[32m+[m[32m      container: false,[m
[32m+[m[32m      fallbackPlacement: 'flip',[m
[32m+[m[32m      boundary: 'scrollParent'[m
[32m+[m[32m    };[m
[32m+[m[32m    var HoverState = {[m
[32m+[m[32m      SHOW: 'show',[m
[32m+[m[32m      OUT: 'out'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      HIDE: "hide" + EVENT_KEY,[m
[32m+[m[32m      HIDDEN: "hidden" + EVENT_KEY,[m
[32m+[m[32m      SHOW: "show" + EVENT_KEY,[m
[32m+[m[32m      SHOWN: "shown" + EVENT_KEY,[m
[32m+[m[32m      INSERTED: "inserted" + EVENT_KEY,[m
[32m+[m[32m      CLICK: "click" + EVENT_KEY,[m
[32m+[m[32m      FOCUSIN: "focusin" + EVENT_KEY,[m
[32m+[m[32m      FOCUSOUT: "focusout" + EVENT_KEY,[m
[32m+[m[32m      MOUSEENTER: "mouseenter" + EVENT_KEY,[m
[32m+[m[32m      MOUSELEAVE: "mouseleave" + EVENT_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      FADE: 'fade',[m
[32m+[m[32m      SHOW: 'show'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      TOOLTIP: '.tooltip',[m
[32m+[m[32m      TOOLTIP_INNER: '.tooltip-inner',[m
[32m+[m[32m      ARROW: '.arrow'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Trigger = {[m
[32m+[m[32m      HOVER: 'hover',[m
[32m+[m[32m      FOCUS: 'focus',[m
[32m+[m[32m      CLICK: 'click',[m
[32m+[m[32m      MANUAL: 'manual'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var Tooltip =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function Tooltip(element, config) {[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Check for Popper dependency[m
[32m+[m[32m         * Popper - https://popper.js.org[m
[32m+[m[32m         */[m
[32m+[m[32m        if (typeof Popper === 'undefined') {[m
[32m+[m[32m          throw new TypeError('Bootstrap tooltips require Popper.js (https://popper.js.org)');[m
[32m+[m[32m        } // private[m
[32m+[m
[32m+[m
[32m+[m[32m        this._isEnabled = true;[m
[32m+[m[32m        this._timeout = 0;[m
[32m+[m[32m        this._hoverState = '';[m
[32m+[m[32m        this._activeTrigger = {};[m
[32m+[m[32m        this._popper = null; // Protected[m
[32m+[m
[32m+[m[32m        this.element = element;[m
[32m+[m[32m        this.config = this._getConfig(config);[m
[32m+[m[32m        this.tip = null;[m
[32m+[m
[32m+[m[32m        this._setListeners();[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = Tooltip.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.enable = function enable() {[m
[32m+[m[32m        this._isEnabled = true;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.disable = function disable() {[m
[32m+[m[32m        this._isEnabled = false;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.toggleEnabled = function toggleEnabled() {[m
[32m+[m[32m        this._isEnabled = !this._isEnabled;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.toggle = function toggle(event) {[m
[32m+[m[32m        if (!this._isEnabled) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event) {[m
[32m+[m[32m          var dataKey = this.constructor.DATA_KEY;[m
[32m+[m[32m          var context = $$$1(event.currentTarget).data(dataKey);[m
[32m+[m
[32m+[m[32m          if (!context) {[m
[32m+[m[32m            context = new this.constructor(event.currentTarget, this._getDelegateConfig());[m
[32m+[m[32m            $$$1(event.currentTarget).data(dataKey, context);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          context._activeTrigger.click = !context._activeTrigger.click;[m
[32m+[m
[32m+[m[32m          if (context._isWithActiveTrigger()) {[m
[32m+[m[32m            context._enter(null, context);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            context._leave(null, context);[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          if ($$$1(this.getTipElement()).hasClass(ClassName.SHOW)) {[m
[32m+[m[32m            this._leave(null, this);[m
[32m+[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          this._enter(null, this);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        clearTimeout(this._timeout);[m
[32m+[m[32m        $$$1.removeData(this.element, this.constructor.DATA_KEY);[m
[32m+[m[32m        $$$1(this.element).off(this.constructor.EVENT_KEY);[m
[32m+[m[32m        $$$1(this.element).closest('.modal').off('hide.bs.modal');[m
[32m+[m
[32m+[m[32m        if (this.tip) {[m
[32m+[m[32m          $$$1(this.tip).remove();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._isEnabled = null;[m
[32m+[m[32m        this._timeout = null;[m
[32m+[m[32m        this._hoverState = null;[m
[32m+[m[32m        this._activeTrigger = null;[m
[32m+[m
[32m+[m[32m        if (this._popper !== null) {[m
[32m+[m[32m          this._popper.destroy();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._popper = null;[m
[32m+[m[32m        this.element = null;[m
[32m+[m[32m        this.config = null;[m
[32m+[m[32m        this.tip = null;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.show = function show() {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        if ($$$1(this.element).css('display') === 'none') {[m
[32m+[m[32m          throw new Error('Please use show on visible elements');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var showEvent = $$$1.Event(this.constructor.Event.SHOW);[m
[32m+[m
[32m+[m[32m        if (this.isWithContent() && this._isEnabled) {[m
[32m+[m[32m          $$$1(this.element).trigger(showEvent);[m
[32m+[m[32m          var isInTheDom = $$$1.contains(this.element.ownerDocument.documentElement, this.element);[m
[32m+[m
[32m+[m[32m          if (showEvent.isDefaultPrevented() || !isInTheDom) {[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var tip = this.getTipElement();[m
[32m+[m[32m          var tipId = Util.getUID(this.constructor.NAME);[m
[32m+[m[32m          tip.setAttribute('id', tipId);[m
