[32m+[m
[32m+[m[32m  var Carousel = function ($$$1) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Constants[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m[32m    var NAME = 'carousel';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.carousel';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var DATA_API_KEY = '.data-api';[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var ARROW_LEFT_KEYCODE = 37; // KeyboardEvent.which value for left arrow key[m
[32m+[m
[32m+[m[32m    var ARROW_RIGHT_KEYCODE = 39; // KeyboardEvent.which value for right arrow key[m
[32m+[m
[32m+[m[32m    var TOUCHEVENT_COMPAT_WAIT = 500; // Time for mouse compat events to fire after touch[m
[32m+[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      interval: 5000,[m
[32m+[m[32m      keyboard: true,[m
[32m+[m[32m      slide: false,[m
[32m+[m[32m      pause: 'hover',[m
[32m+[m[32m      wrap: true[m
[32m+[m[32m    };[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      interval: '(number|boolean)',[m
[32m+[m[32m      keyboard: 'boolean',[m
[32m+[m[32m      slide: '(boolean|string)',[m
[32m+[m[32m      pause: '(string|boolean)',[m
[32m+[m[32m      wrap: 'boolean'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Direction = {[m
[32m+[m[32m      NEXT: 'next',[m
[32m+[m[32m      PREV: 'prev',[m
[32m+[m[32m      LEFT: 'left',[m
[32m+[m[32m      RIGHT: 'right'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      SLIDE: "slide" + EVENT_KEY,[m
[32m+[m[32m      SLID: "slid" + EVENT_KEY,[m
[32m+[m[32m      KEYDOWN: "keydown" + EVENT_KEY,[m
[32m+[m[32m      MOUSEENTER: "mouseenter" + EVENT_KEY,[m
[32m+[m[32m      MOUSELEAVE: "mouseleave" + EVENT_KEY,[m
[32m+[m[32m      TOUCHEND: "touchend" + EVENT_KEY,[m
[32m+[m[32m      LOAD_DATA_API: "load" + EVENT_KEY + DATA_API_KEY,[m
[32m+[m[32m      CLICK_DATA_API: "click" + EVENT_KEY + DATA_API_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      CAROUSEL: 'carousel',[m
[32m+[m[32m      ACTIVE: 'active',[m
[32m+[m[32m      SLIDE: 'slide',[m
[32m+[m[32m      RIGHT: 'carousel-item-right',[m
[32m+[m[32m      LEFT: 'carousel-item-left',[m
[32m+[m[32m      NEXT: 'carousel-item-next',[m
[32m+[m[32m      PREV: 'carousel-item-prev',[m
[32m+[m[32m      ITEM: 'carousel-item'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      ACTIVE: '.active',[m
[32m+[m[32m      ACTIVE_ITEM: '.active.carousel-item',[m
[32m+[m[32m      ITEM: '.carousel-item',[m
[32m+[m[32m      NEXT_PREV: '.carousel-item-next, .carousel-item-prev',[m
[32m+[m[32m      INDICATORS: '.carousel-indicators',[m
[32m+[m[32m      DATA_SLIDE: '[data-slide], [data-slide-to]',[m
[32m+[m[32m      DATA_RIDE: '[data-ride="carousel"]'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var Carousel =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function Carousel(element, config) {[m
[32m+[m[32m        this._items = null;[m
[32m+[m[32m        this._interval = null;[m
[32m+[m[32m        this._activeElement = null;[m
[32m+[m[32m        this._isPaused = false;[m
[32m+[m[32m        this._isSliding = false;[m
[32m+[m[32m        this.touchTimeout = null;[m
[32m+[m[32m        this._config = this._getConfig(config);[m
[32m+[m[32m        this._element = $$$1(element)[0];[m
[32m+[m[32m        this._indicatorsElement = this._element.querySelector(Selector.INDICATORS);[m
[32m+[m
[32m+[m[32m        this._addEventListeners();[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = Carousel.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.next = function next() {[m
[32m+[m[32m        if (!this._isSliding) {[m
[32m+[m[32m          this._slide(Direction.NEXT);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.nextWhenVisible = function nextWhenVisible() {[m
[32m+[m[32m        // Don't call next when the page isn't visible[m
[32m+[m[32m        // or the carousel or its parent isn't visible[m
[32m+[m[32m        if (!document.hidden && $$$1(this._element).is(':visible') && $$$1(this._element).css('visibility') !== 'hidden') {[m
[32m+[m[32m          this.next();[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.prev = function prev() {[m
[32m+[m[32m        if (!this._isSliding) {[m
[32m+[m[32m          this._slide(Direction.PREV);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.pause = function pause(event) {[m
[32m+[m[32m        if (!event) {[m
[32m+[m[32m          this._isPaused = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._element.querySelector(Selector.NEXT_PREV)) {[m
[32m+[m[32m          Util.triggerTransitionEnd(this._element);[m
[32m+[m[32m          this.cycle(true);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        clearInterval(this._interval);[m
[32m+[m[32m        this._interval = null;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.cycle = function cycle(event) {[m
[32m+[m[32m        if (!event) {[m
[32m+[m[32m          this._isPaused = false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._interval) {[m
[32m+[m[32m          clearInterval(this._interval);[m
[32m+[m[32m          this._interval = null;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._config.interval && !this._isPaused) {[m
[32m+[m[32m          this._interval = setInterval((document.visibilityState ? this.nextWhenVisible : this.next).bind(this), this._config.interval);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.to = function to(index) {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        this._activeElement = this._element.querySelector(Selector.ACTIVE_ITEM);[m
[32m+[m
[32m+[m[32m        var activeIndex = this._getItemIndex(this._activeElement);[m
[32m+[m
[32m+[m[32m        if (index > this._items.length - 1 || index < 0) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._isSliding) {[m
[32m+[m[32m          $$$1(this._element).one(Event.SLID, function () {[m
[32m+[m[32m            return _this.to(index);[m
[32m+[m[32m          });[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (activeIndex === index) {[m
[32m+[m[32m          this.pause();[m
[32m+[m[32m          this.cycle();[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var direction = index > activeIndex ? Direction.NEXT : Direction.PREV;[m
[32m+[m
[32m+[m[32m        this._slide(direction, this._items[index]);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        $$$1(this._element).off(EVENT_KEY);[m
[32m+[m[32m        $$$1.removeData(this._element, DATA_KEY);[m
[32m+[m[32m        this._items = null;[m
[32m+[m[32m        this._config = null;[m
[32m+[m[32m        this._element = null;[m
[32m+[m[32m        this._interval = null;[m
[32m+[m[32m        this._isPaused = null;[m
[32m+[m[32m        this._isSliding = null;[m
[32m+[m[32m        this._activeElement = null;[m
[32m+[m[32m        this._indicatorsElement = null;[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, Default, config);[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._addEventListeners = function _addEventListeners() {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        if (this._config.keyboard) {[m
[32m+[m[32m          $$$1(this._element).on(Event.KEYDOWN, function (event) {[m
[32m+[m[32m            return _this2._keydown(event);[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (this._config.pause === 'hover') {[m
[32m+[m[32m          $$$1(this._element).on(Event.MOUSEENTER, function (event) {[m
[32m+[m[32m            return _this2.pause(event);[m
[32m+[m[32m          }).on(Event.MOUSELEAVE, function (event) {[m
[32m+[m[32m            return _this2.cycle(event);[m
[32m+[m[32m          });[m
[32m+[m
[32m+[m[32m          if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m            // If it's a touch-enabled device, mouseenter/leave are fired as[m
[32m+[m[32m            // part of the mouse compatibility events on first tap - the carousel[m
[32m+[m[32m            // would stop cycling until user tapped out of it;[m
[32m+[m[32m            // here, we listen for touchend, explicitly pause the carousel[m
[32m+[m[32m            // (as if it's the second time we tap on it, mouseenter compat event[m
[32m+[m[32m            // is NOT fired) and after a timeout (to allow for mouse compatibility[m
[32m+[m[32m            // events to fire) we explicitly restart cycling[m
[32m+[m[32m            $$$1(this._element).on(Event.TOUCHEND, function () {[m
[32m+[m[32m              _this2.pause();[m
[32m+[m
[32m+[m[32m              if (_this2.touchTimeout) {[m
[32m+[m[32m                clearTimeout(_this2.touchTimeout);[m
[32m+[m[32m              }[m
[32m+[m
[32m+[m[32m              _this2.touchTimeout = setTimeout(function (event) {[m
[32m+[m[32m                return _this2.cycle(event);[m
[32m+[m[32m              }, TOUCHEVENT_COMPAT_WAIT + _this2._config.interval);[m
[32m+[m[32m            });[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._keydown = function _keydown(event) {[m
[32m+[m[32m        if (/input|textarea/i.test(event.target.tagName)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        switch (event.which) {[m
[32m+[m[32m          case ARROW_LEFT_KEYCODE:[m
[32m+[m[32m            event.preventDefault();[m
[32m+[m[32m            this.prev();[m
[32m+[m[32m            break;[m
[32m+[m
[32m+[m[32m          case ARROW_RIGHT_KEYCODE:[m
[32m+[m[32m            event.preventDefault();[m
[32m+[m[32m            this.next();[m
[32m+[m[32m            break;[m
[32m+[m
[32m+[m[32m          default:[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getItemIndex = function _getItemIndex(element) {[m
[32m+[m[32m        this._items = element && element.parentNode ? [].slice.call(element.parentNode.querySelectorAll(Selector.ITEM)) : [];[m
[32m+[m[32m        return this._items.indexOf(element);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getItemByDirection = function _getItemByDirection(direction, activeElement) {[m
[32m+[m[32m        var isNextDirection = direction === Direction.NEXT;[m
[32m+[m[32m        var isPrevDirection = direction === Direction.PREV;[m
[32m+[m
[32m+[m[32m        var activeIndex = this._getItemIndex(activeElement);[m
[32m+[m
[32m+[m[32m        var lastItemIndex = this._items.length - 1;[m
[32m+[m[32m        var isGoingToWrap = isPrevDirection && activeIndex === 0 || isNextDirection && activeIndex === lastItemIndex;[m
[32m+[m
[32m+[m[32m        if (isGoingToWrap && !this._config.wrap) {[m
[32m+[m[32m          return activeElement;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var delta = direction === Direction.PREV ? -1 : 1;[m
[32m+[m[32m        var itemIndex = (activeIndex + delta) % this._items.length;[m
[32m+[m[32m        return itemIndex === -1 ? this._items[this._items.length - 1] : this._items[itemIndex];[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._triggerSlideEvent = function _triggerSlideEvent(relatedTarget, eventDirectionName) {[m
[32m+[m[32m        var targetIndex = this._getItemIndex(relatedTarget);[m
[32m+[m
[32m+[m[32m        var fromIndex = this._getItemIndex(this._element.querySelector(Selector.ACTIVE_ITEM));[m
[32m+[m
[32m+[m[32m        var slideEvent = $$$1.Event(Event.SLIDE, {[m
[32m+[m[32m          relatedTarget: relatedTarget,[m
[32m+[m[32m          direction: eventDirectionName,[m
[32m+[m[32m          from: fromIndex,[m
[32m+[m[32m          to: targetIndex[m
[32m+[m[32m        });[m
[32m+[m[32m        $$$1(this._element).trigger(slideEvent);[m
[32m+[m[32m        return slideEvent;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._setActiveIndicatorElement = function _setActiveIndicatorElement(element) {[m
[32m+[m[32m        if (this._indicatorsElement) {[m
[32m+[m[32m          var indicators = [].slice.call(this._indicatorsElement.querySelectorAll(Selector.ACTIVE));[m
[32m+[m[32m          $$$1(indicators).removeClass(ClassName.ACTIVE);[m
[32m+[m
[32m+[m[32m          var nextIndicator = this._indicatorsElement.children[this._getItemIndex(element)];[m
[32m+[m
[32m+[m[32m          if (nextIndicator) {[m
[32m+[m[32m            $$$1(nextIndicator).addClass(ClassName.ACTIVE);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._slide = function _slide(direction, element) {[m
[32m+[m[32m        var _this3 = this;[m
[32m+[m
[32m+[m[32m        var activeElement = this._element.querySelector(Selector.ACTIVE_ITEM);[m
[32m+[m
[32m+[m[32m        var activeElementIndex = this._getItemIndex(activeElement);[m
[32m+[m
[32m+[m[32m        var nextElement = element || activeElement && this._getItemByDirection(direction, activeElement);[m
[32m+[m
[32m+[m[32m        var nextElementIndex = this._getItemIndex(nextElement);[m
[32m+[m
[32m+[m[32m        var isCycling = Boolean(this._interval);[m
[32m+[m[32m        var directionalClassName;[m
[32m+[m[32m        var orderClassName;[m
[32m+[m[32m        var eventDirectionName;[m
[32m+[m
[32m+[m[32m        if (direction === Direction.NEXT) {[m
[32m+[m[32m          directionalClassName = ClassName.LEFT;[m
[32m+[m[32m          orderClassName = ClassName.NEXT;[m
[32m+[m[32m          eventDirectionName = Direction.LEFT;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          directionalClassName = ClassName.RIGHT;[m
[32m+[m[32m          orderClassName = ClassName.PREV;[m
[32m+[m[32m          eventDirectionName = Direction.RIGHT;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (nextElement && $$$1(nextElement).hasClass(ClassName.ACTIVE)) {[m
[32m+[m[32m          this._isSliding = false;[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var slideEvent = this._triggerSlideEvent(nextElement, eventDirectionName);[m
[32m+[m
[32m+[m[32m        if (slideEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!activeElement || !nextElement) {[m
[32m+[m[32m          // Some weirdness is happening, so we bail[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._isSliding = true;[m
[32m+[m
[32m+[m[32m        if (isCycling) {[m
[32m+[m[32m          this.pause();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._setActiveIndicatorElement(nextElement);[m
[32m+[m
[32m+[m[32m        var slidEvent = $$$1.Event(Event.SLID, {[m
[32m+[m[32m          relatedTarget: nextElement,[m
[32m+[m[32m          direction: eventDirectionName,[m
[32m+[m[32m          from: activeElementIndex,[m
[32m+[m[32m          to: nextElementIndex[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        if ($$$1(this._element).hasClass(ClassName.SLIDE)) {[m
[32m+[m[32m          $$$1(nextElement).addClass(orderClassName);[m
[32m+[m[32m          Util.reflow(nextElement);[m
[32m+[m[32m          $$$1(activeElement).addClass(directionalClassName);[m
[32m+[m[32m          $$$1(nextElement).addClass(directionalClassName);[m
[32m+[m[32m          var transitionDuration = Util.getTransitionDurationFromElement(activeElement);[m
[32m+[m[32m          $$$1(activeElement).one(Util.TRANSITION_END, function () {[m
[32m+[m[32m            $$$1(nextElement).removeClass(directionalClassName + " " + orderClassName).addClass(ClassName.ACTIVE);[m
[32m+[m[32m            $$$1(activeElement).removeClass(ClassName.ACTIVE + " " + orderClassName + " " + directionalClassName);[m
[32m+[m[32m            _this3._isSliding = false;[m
[32m+[m[32m            setTimeout(function () {[m
[32m+[m[32m              return $$$1(_this3._element).trigger(slidEvent);[m
[32m+[m[32m            }, 0);[m
[32m+[m[32m          }).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $$$1(activeElement).removeClass(ClassName.ACTIVE);[m
[32m+[m[32m          $$$1(nextElement).addClass(ClassName.ACTIVE);[m
[32m+[m[32m          this._isSliding = false;[m
[32m+[m[32m          $$$1(this._element).trigger(slidEvent);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (isCycling) {[m
[32m+[m[32m          this.cycle();[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Carousel._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = _objectSpread({}, Default, $$$1(this).data());[m
[32m+[m
[32m+[m[32m          if (typeof config === 'object') {[m
[32m+[m[32m            _config = _objectSpread({}, _config, config);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var action = typeof config === 'string' ? config : _config.slide;[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Carousel(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'number') {[m
[32m+[m[32m            data.to(config);[m
[32m+[m[32m          } else if (typeof action === 'string') {[m
[32m+[m[32m            if (typeof data[action] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + action + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[action]();[m
[32m+[m[32m          } else if (_config.interval) {[m
[32m+[m[32m            data.pause();[m
[32m+[m[32m            data.cycle();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Carousel._dataApiClickHandler = function _dataApiClickHandler(event) {[m
[32m+[m[32m        var selector = Util.getSelectorFromElement(this);[m
[32m+[m
[32m+[m[32m        if (!selector) {[m
