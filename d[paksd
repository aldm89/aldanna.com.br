[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.setContent = function setContent() {[m
[32m+[m[32m        var $tip = $$$1(this.getTipElement()); // We use append for html objects to maintain js events[m
[32m+[m
[32m+[m[32m        this.setElementContent($tip.find(Selector.TITLE), this.getTitle());[m
[32m+[m
[32m+[m[32m        var content = this._getContent();[m
[32m+[m
[32m+[m[32m        if (typeof content === 'function') {[m
[32m+[m[32m          content = content.call(this.element);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.setElementContent($tip.find(Selector.CONTENT), content);[m
[32m+[m[32m        $tip.removeClass(ClassName.FADE + " " + ClassName.SHOW);[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._getContent = function _getContent() {[m
