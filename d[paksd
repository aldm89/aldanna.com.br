[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this.element).trigger(this.constructor.Event.INSERTED);[m
[32m+[m[32m          this._popper = new Popper(this.element, tip, {[m
[32m+[m[32m            placement: attachment,[m
[32m+[m[32m            modifiers: {[m
[32m+[m[32m              offset: {[m
[32m+[m[32m                offset: this.config.offset[m
[32m+[m[32m              },[m
[32m+[m[32m              flip: {[m
[32m+[m[32m                behavior: this.config.fallbackPlacement[m
[32m+[m[32m              },[m
