[32m+[m[32m   * --------------------------------------------------------------------------[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var Dropdown = function ($$$1) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Constants[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m[32m    var NAME = 'dropdown';[m
[32m+[m[32m    var VERSION = '4.1.3';[m
[32m+[m[32m    var DATA_KEY = 'bs.dropdown';[m
[32m+[m[32m    var EVENT_KEY = "." + DATA_KEY;[m
[32m+[m[32m    var DATA_API_KEY = '.data-api';[m
[32m+[m[32m    var JQUERY_NO_CONFLICT = $$$1.fn[NAME];[m
[32m+[m[32m    var ESCAPE_KEYCODE = 27; // KeyboardEvent.which value for Escape (Esc) key[m
[32m+[m
[32m+[m[32m    var SPACE_KEYCODE = 32; // KeyboardEvent.which value for space key[m
[32m+[m
[32m+[m[32m    var TAB_KEYCODE = 9; // KeyboardEvent.which value for tab key[m
[32m+[m
[32m+[m[32m    var ARROW_UP_KEYCODE = 38; // KeyboardEvent.which value for up arrow key[m
[32m+[m
[32m+[m[32m    var ARROW_DOWN_KEYCODE = 40; // KeyboardEvent.which value for down arrow key[m
[32m+[m
[32m+[m[32m    var RIGHT_MOUSE_BUTTON_WHICH = 3; // MouseEvent.which value for the right button (assuming a right-handed mouse)[m
[32m+[m
[32m+[m[32m    var REGEXP_KEYDOWN = new RegExp(ARROW_UP_KEYCODE + "|" + ARROW_DOWN_KEYCODE + "|" + ESCAPE_KEYCODE);[m
[32m+[m[32m    var Event = {[m
[32m+[m[32m      HIDE: "hide" + EVENT_KEY,[m
[32m+[m[32m      HIDDEN: "hidden" + EVENT_KEY,[m
[32m+[m[32m      SHOW: "show" + EVENT_KEY,[m
[32m+[m[32m      SHOWN: "shown" + EVENT_KEY,[m
[32m+[m[32m      CLICK: "click" + EVENT_KEY,[m
[32m+[m[32m      CLICK_DATA_API: "click" + EVENT_KEY + DATA_API_KEY,[m
[32m+[m[32m      KEYDOWN_DATA_API: "keydown" + EVENT_KEY + DATA_API_KEY,[m
[32m+[m[32m      KEYUP_DATA_API: "keyup" + EVENT_KEY + DATA_API_KEY[m
[32m+[m[32m    };[m
[32m+[m[32m    var ClassName = {[m
[32m+[m[32m      DISABLED: 'disabled',[m
[32m+[m[32m      SHOW: 'show',[m
[32m+[m[32m      DROPUP: 'dropup',[m
[32m+[m[32m      DROPRIGHT: 'dropright',[m
[32m+[m[32m      DROPLEFT: 'dropleft',[m
[32m+[m[32m      MENURIGHT: 'dropdown-menu-right',[m
[32m+[m[32m      MENULEFT: 'dropdown-menu-left',[m
[32m+[m[32m      POSITION_STATIC: 'position-static'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Selector = {[m
[32m+[m[32m      DATA_TOGGLE: '[data-toggle="dropdown"]',[m
[32m+[m[32m      FORM_CHILD: '.dropdown form',[m
[32m+[m[32m      MENU: '.dropdown-menu',[m
[32m+[m[32m      NAVBAR_NAV: '.navbar-nav',[m
[32m+[m[32m      VISIBLE_ITEMS: '.dropdown-menu .dropdown-item:not(.disabled):not(:disabled)'[m
[32m+[m[32m    };[m
[32m+[m[32m    var AttachmentMap = {[m
[32m+[m[32m      TOP: 'top-start',[m
[32m+[m[32m      TOPEND: 'top-end',[m
[32m+[m[32m      BOTTOM: 'bottom-start',[m
[32m+[m[32m      BOTTOMEND: 'bottom-end',[m
[32m+[m[32m      RIGHT: 'right-start',[m
[32m+[m[32m      RIGHTEND: 'right-end',[m
[32m+[m[32m      LEFT: 'left-start',[m
[32m+[m[32m      LEFTEND: 'left-end'[m
[32m+[m[32m    };[m
[32m+[m[32m    var Default = {[m
[32m+[m[32m      offset: 0,[m
[32m+[m[32m      flip: true,[m
[32m+[m[32m      boundary: 'scrollParent',[m
[32m+[m[32m      reference: 'toggle',[m
[32m+[m[32m      display: 'dynamic'[m
[32m+[m[32m    };[m
[32m+[m[32m    var DefaultType = {[m
[32m+[m[32m      offset: '(number|string|function)',[m
[32m+[m[32m      flip: 'boolean',[m
[32m+[m[32m      boundary: '(string|element)',[m
[32m+[m[32m      reference: '(string|element)',[m
[32m+[m[32m      display: 'string'[m
[32m+[m[32m      /**[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       * Class Definition[m
[32m+[m[32m       * ------------------------------------------------------------------------[m
[32m+[m[32m       */[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var Dropdown =[m
[32m+[m[32m    /*#__PURE__*/[m
[32m+[m[32m    function () {[m
[32m+[m[32m      function Dropdown(element, config) {[m
[32m+[m[32m        this._element = element;[m
[32m+[m[32m        this._popper = null;[m
[32m+[m[32m        this._config = this._getConfig(config);[m
[32m+[m[32m        this._menu = this._getMenuElement();[m
[32m+[m[32m        this._inNavbar = this._detectNavbar();[m
[32m+[m
[32m+[m[32m        this._addEventListeners();[m
[32m+[m[32m      } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m      var _proto = Dropdown.prototype;[m
[32m+[m
[32m+[m[32m      // Public[m
[32m+[m[32m      _proto.toggle = function toggle() {[m
[32m+[m[32m        if (this._element.disabled || $$$1(this._element).hasClass(ClassName.DISABLED)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var parent = Dropdown._getParentFromElement(this._element);[m
[32m+[m
[32m+[m[32m        var isActive = $$$1(this._menu).hasClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m        Dropdown._clearMenus();[m
[32m+[m
[32m+[m[32m        if (isActive) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var relatedTarget = {[m
[32m+[m[32m          relatedTarget: this._element[m
[32m+[m[32m        };[m
[32m+[m[32m        var showEvent = $$$1.Event(Event.SHOW, relatedTarget);[m
[32m+[m[32m        $$$1(parent).trigger(showEvent);[m
[32m+[m
[32m+[m[32m        if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        } // Disable totally Popper.js for Dropdown in Navbar[m
[32m+[m
[32m+[m
[32m+[m[32m        if (!this._inNavbar) {[m
[32m+[m[32m          /**[m
[32m+[m[32m           * Check for Popper dependency[m
[32m+[m[32m           * Popper - https://popper.js.org[m
[32m+[m[32m           */[m
[32m+[m[32m          if (typeof Popper === 'undefined') {[m
[32m+[m[32m            throw new TypeError('Bootstrap dropdown require Popper.js (https://popper.js.org)');[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var referenceElement = this._element;[m
[32m+[m
[32m+[m[32m          if (this._config.reference === 'parent') {[m
[32m+[m[32m            referenceElement = parent;[m
[32m+[m[32m          } else if (Util.isElement(this._config.reference)) {[m
[32m+[m[32m            referenceElement = this._config.reference; // Check if it's jQuery element[m
[32m+[m
[32m+[m[32m            if (typeof this._config.reference.jquery !== 'undefined') {[m
[32m+[m[32m              referenceElement = this._config.reference[0];[m
[32m+[m[32m            }[m
[32m+[m[32m          } // If boundary is not `scrollParent`, then set position to `static`[m
[32m+[m[32m          // to allow the menu to "escape" the scroll parent's boundaries[m
[32m+[m[32m          // https://github.com/twbs/bootstrap/issues/24251[m
[32m+[m
[32m+[m
[32m+[m[32m          if (this._config.boundary !== 'scrollParent') {[m
[32m+[m[32m            $$$1(parent).addClass(ClassName.POSITION_STATIC);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          this._popper = new Popper(referenceElement, this._menu, this._getPopperConfig());[m
[32m+[m[32m        } // If this is a touch-enabled device we add extra[m
[32m+[m[32m        // empty mouseover listeners to the body's immediate children;[m
[32m+[m[32m        // only needed because of broken event delegation on iOS[m
[32m+[m[32m        // https://www.quirksmode.org/blog/archives/2014/02/mouse_event_bub.html[m
[32m+[m
[32m+[m
[32m+[m[32m        if ('ontouchstart' in document.documentElement && $$$1(parent).closest(Selector.NAVBAR_NAV).length === 0) {[m
[32m+[m[32m          $$$1(document.body).children().on('mouseover', null, $$$1.noop);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._element.focus();[m
[32m+[m
[32m+[m[32m        this._element.setAttribute('aria-expanded', true);[m
[32m+[m
[32m+[m[32m        $$$1(this._menu).toggleClass(ClassName.SHOW);[m
[32m+[m[32m        $$$1(parent).toggleClass(ClassName.SHOW).trigger($$$1.Event(Event.SHOWN, relatedTarget));[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.dispose = function dispose() {[m
[32m+[m[32m        $$$1.removeData(this._element, DATA_KEY);[m
[32m+[m[32m        $$$1(this._element).off(EVENT_KEY);[m
[32m+[m[32m        this._element = null;[m
[32m+[m[32m        this._menu = null;[m
[32m+[m
[32m+[m[32m        if (this._popper !== null) {[m
[32m+[m[32m          this._popper.destroy();[m
[32m+[m
[32m+[m[32m          this._popper = null;[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto.update = function update() {[m
[32m+[m[32m        this._inNavbar = this._detectNavbar();[m
[32m+[m
[32m+[m[32m        if (this._popper !== null) {[m
[32m+[m[32m          this._popper.scheduleUpdate();[m
[32m+[m[32m        }[m
[32m+[m[32m      }; // Private[m
[32m+[m
[32m+[m
[32m+[m[32m      _proto._addEventListeners = function _addEventListeners() {[m
[32m+[m[32m        var _this = this;[m
[32m+[m
[32m+[m[32m        $$$1(this._element).on(Event.CLICK, function (event) {[m
[32m+[m[32m          event.preventDefault();[m
[32m+[m[32m          event.stopPropagation();[m
[32m+[m
[32m+[m[32m          _this.toggle();[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m        config = _objectSpread({}, this.constructor.Default, $$$1(this._element).data(), config);[m
[32m+[m[32m        Util.typeCheckConfig(NAME, config, this.constructor.DefaultType);[m
[32m+[m[32m        return config;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getMenuElement = function _getMenuElement() {[m
[32m+[m[32m        if (!this._menu) {[m
[32m+[m[32m          var parent = Dropdown._getParentFromElement(this._element);[m
[32m+[m
[32m+[m[32m          if (parent) {[m
[32m+[m[32m            this._menu = parent.querySelector(Selector.MENU);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return this._menu;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getPlacement = function _getPlacement() {[m
[32m+[m[32m        var $parentDropdown = $$$1(this._element.parentNode);[m
[32m+[m[32m        var placement = AttachmentMap.BOTTOM; // Handle dropup[m
[32m+[m
[32m+[m[32m        if ($parentDropdown.hasClass(ClassName.DROPUP)) {[m
[32m+[m[32m          placement = AttachmentMap.TOP;[m
[32m+[m
[32m+[m[32m          if ($$$1(this._menu).hasClass(ClassName.MENURIGHT)) {[m
[32m+[m[32m            placement = AttachmentMap.TOPEND;[m
[32m+[m[32m          }[m
[32m+[m[32m        } else if ($parentDropdown.hasClass(ClassName.DROPRIGHT)) {[m
[32m+[m[32m          placement = AttachmentMap.RIGHT;[m
[32m+[m[32m        } else if ($parentDropdown.hasClass(ClassName.DROPLEFT)) {[m
[32m+[m[32m          placement = AttachmentMap.LEFT;[m
[32m+[m[32m        } else if ($$$1(this._menu).hasClass(ClassName.MENURIGHT)) {[m
[32m+[m[32m          placement = AttachmentMap.BOTTOMEND;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return placement;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._detectNavbar = function _detectNavbar() {[m
[32m+[m[32m        return $$$1(this._element).closest('.navbar').length > 0;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _proto._getPopperConfig = function _getPopperConfig() {[m
[32m+[m[32m        var _this2 = this;[m
[32m+[m
[32m+[m[32m        var offsetConf = {};[m
[32m+[m
[32m+[m[32m        if (typeof this._config.offset === 'function') {[m
[32m+[m[32m          offsetConf.fn = function (data) {[m
[32m+[m[32m            data.offsets = _objectSpread({}, data.offsets, _this2._config.offset(data.offsets) || {});[m
[32m+[m[32m            return data;[m
[32m+[m[32m          };[m
[32m+[m[32m        } else {[m
[32m+[m[32m          offsetConf.offset = this._config.offset;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var popperConfig = {[m
[32m+[m[32m          placement: this._getPlacement(),[m
[32m+[m[32m          modifiers: {[m
[32m+[m[32m            offset: offsetConf,[m
[32m+[m[32m            flip: {[m
[32m+[m[32m              enabled: this._config.flip[m
[32m+[m[32m            },[m
[32m+[m[32m            preventOverflow: {[m
[32m+[m[32m              boundariesElement: this._config.boundary[m
[32m+[m[32m            }[m
[32m+[m[32m          } // Disable Popper.js if we have a static display[m
[32m+[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (this._config.display === 'static') {[m
[32m+[m[32m          popperConfig.modifiers.applyStyle = {[m
[32m+[m[32m            enabled: false[m
[32m+[m[32m          };[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return popperConfig;[m
[32m+[m[32m      }; // Static[m
[32m+[m
[32m+[m
[32m+[m[32m      Dropdown._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m        return this.each(function () {[m
[32m+[m[32m          var data = $$$1(this).data(DATA_KEY);[m
[32m+[m
[32m+[m[32m          var _config = typeof config === 'object' ? config : null;[m
[32m+[m
[32m+[m[32m          if (!data) {[m
[32m+[m[32m            data = new Dropdown(this, _config);[m
[32m+[m[32m            $$$1(this).data(DATA_KEY, data);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (typeof config === 'string') {[m
[32m+[m[32m            if (typeof data[config] === 'undefined') {[m
[32m+[m[32m              throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            data[config]();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Dropdown._clearMenus = function _clearMenus(event) {[m
[32m+[m[32m        if (event && (event.which === RIGHT_MOUSE_BUTTON_WHICH || event.type === 'keyup' && event.which !== TAB_KEYCODE)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var toggles = [].slice.call(document.querySelectorAll(Selector.DATA_TOGGLE));[m
[32m+[m
[32m+[m[32m        for (var i = 0, len = toggles.length; i < len; i++) {[m
[32m+[m[32m          var parent = Dropdown._getParentFromElement(toggles[i]);[m
[32m+[m
[32m+[m[32m          var context = $$$1(toggles[i]).data(DATA_KEY);[m
[32m+[m[32m          var relatedTarget = {[m
[32m+[m[32m            relatedTarget: toggles[i][m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if (event && event.type === 'click') {[m
[32m+[m[32m            relatedTarget.clickEvent = event;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (!context) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var dropdownMenu = context._menu;[m
[32m+[m
[32m+[m[32m          if (!$$$1(parent).hasClass(ClassName.SHOW)) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (event && (event.type === 'click' && /input|textarea/i.test(event.target.tagName) || event.type === 'keyup' && event.which === TAB_KEYCODE) && $$$1.contains(parent, event.target)) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          var hideEvent = $$$1.Event(Event.HIDE, relatedTarget);[m
[32m+[m[32m          $$$1(parent).trigger(hideEvent);[m
[32m+[m
[32m+[m[32m          if (hideEvent.isDefaultPrevented()) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m          } // If this is a touch-enabled device we remove the extra[m
[32m+[m[32m          // empty mouseover listeners we added for iOS support[m
[32m+[m
[32m+[m
[32m+[m[32m          if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m            $$$1(document.body).children().off('mouseover', null, $$$1.noop);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          toggles[i].setAttribute('aria-expanded', 'false');[m
[32m+[m[32m          $$$1(dropdownMenu).removeClass(ClassName.SHOW);[m
[32m+[m[32m          $$$1(parent).removeClass(ClassName.SHOW).trigger($$$1.Event(Event.HIDDEN, relatedTarget));[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      Dropdown._getParentFromElement = function _getParentFromElement(element) {[m
[32m+[m[32m        var parent;[m
[32m+[m[32m        var selector = Util.getSelectorFromElement(element);[m
[32m+[m
[32m+[m[32m        if (selector) {[m
[32m+[m[32m          parent = document.querySelector(selector);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return parent || element.parentNode;[m
[32m+[m[32m      }; // eslint-disable-next-line complexity[m
[32m+[m
[32m+[m
[32m+[m[32m      Dropdown._dataApiKeydownHandler = function _dataApiKeydownHandler(event) {[m
[32m+[m[32m        // If not input/textarea:[m
[32m+[m[32m        //  - And not a key in REGEXP_KEYDOWN => not a dropdown command[m
[32m+[m[32m        // If input/textarea:[m
[32m+[m[32m        //  - If space key => not a dropdown command[m
[32m+[m[32m        //  - If key is other than escape[m
[32m+[m[32m        //    - If key is not up or down => not a dropdown command[m
[32m+[m[32m        //    - If trigger inside the menu => not a dropdown command[m
[32m+[m[32m        if (/input|textarea/i.test(event.target.tagName) ? event.which === SPACE_KEYCODE || event.which !== ESCAPE_KEYCODE && (event.which !== ARROW_DOWN_KEYCODE && event.which !== ARROW_UP_KEYCODE || $$$1(event.target).closest(Selector.MENU).length) : !REGEXP_KEYDOWN.test(event.which)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        event.preventDefault();[m
[32m+[m[32m        event.stopPropagation();[m
[32m+[m
[32m+[m[32m        if (this.disabled || $$$1(this).hasClass(ClassName.DISABLED)) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var parent = Dropdown._getParentFromElement(this);[m
[32m+[m
[32m+[m[32m        var isActive = $$$1(parent).hasClass(ClassName.SHOW);[m
[32m+[m
[32m+[m[32m        if (!isActive && (event.which !== ESCAPE_KEYCODE || event.which !== SPACE_KEYCODE) || isActive && (event.which === ESCAPE_KEYCODE || event.which === SPACE_KEYCODE)) {[m
[32m+[m[32m          if (event.which === ESCAPE_KEYCODE) {[m
[32m+[m[32m            var toggle = parent.querySelector(Selector.DATA_TOGGLE);[m
[32m+[m[32m            $$$1(toggle).trigger('focus');[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $$$1(this).trigger('click');[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var items = [].slice.call(parent.querySelectorAll(Selector.VISIBLE_ITEMS));[m
[32m+[m
[32m+[m[32m        if (items.length === 0) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var index = items.indexOf(event.target);[m
[32m+[m
[32m+[m[32m        if (event.which === ARROW_UP_KEYCODE && index > 0) {[m
[32m+[m[32m          // Up[m
[32m+[m[32m          index--;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event.which === ARROW_DOWN_KEYCODE && index < items.length - 1) {[m
[32m+[m[32m          // Down[m
[32m+[m[32m          index++;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (index < 0) {[m
[32m+[m[32m          index = 0;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        items[index].focus();[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      _createClass(Dropdown, null, [{[m
[32m+[m[32m        key: "VERSION",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return VERSION;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "Default",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return Default;[m
[32m+[m[32m        }[m
[32m+[m[32m      }, {[m
[32m+[m[32m        key: "DefaultType",[m
[32m+[m[32m        get: function get() {[m
[32m+[m[32m          return DefaultType;[m
[32m+[m[32m        }[m
[32m+[m[32m      }]);[m
[32m+[m
[32m+[m[32m      return Dropdown;[m
[32m+[m[32m    }();[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * Data Api implementation[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m
[32m+[m[32m    $$$1(document).on(Event.KEYDOWN_DATA_API, Selector.DATA_TOGGLE, Dropdown._dataApiKeydownHandler).on(Event.KEYDOWN_DATA_API, Selector.MENU, Dropdown._dataApiKeydownHandler).on(Event.CLICK_DATA_API + " " + Event.KEYUP_DATA_API, Dropdown._clearMenus).on(Event.CLICK_DATA_API, Selector.DATA_TOGGLE, function (event) {[m
[32m+[m[32m      event.preventDefault();[m
[32m+[m[32m      event.stopPropagation();[m
[32m+[m
[32m+[m[32m      Dropdown._jQueryInterface.call($$$1(this), 'toggle');[m
[32m+[m[32m    }).on(Event.CLICK_DATA_API, Selector.FORM_CHILD, function (e) {[m
[32m+[m[32m      e.stopPropagation();[m
[32m+[m[32m    });[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     * jQuery[m
[32m+[m[32m     * ------------------------------------------------------------------------[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME] = Dropdown._jQueryInterface;[m
[32m+[m[32m    $$$1.fn[NAME].Constructor = Dropdown;[m
[32m+[m
[32m+[m[32m    $$$1.fn[NAME].noConflict = function () {[m
[32m+[m[32m      $$$1.fn[NAME] = JQUERY_NO_CONFLICT;[m
[32m+[m[32m      return Dropdown._jQueryInterface;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Dropdown;[m
[32m+[m[32m  }($, Popper);[m
[32m+[m
[32m+[m[32m  /**[m
